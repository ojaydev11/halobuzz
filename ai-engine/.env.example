# AI Engine Configuration
NODE_ENV=development
PORT=5020
LOG_LEVEL=info

# Security
AI_ENGINE_SECRET=generate-strong-ai-secret

# Service URLs
BACKEND_URL=http://localhost:5010
REDIS_URL=redis://localhost:6379
MONGODB_URI=mongodb://localhost:27017/halobuzz_local

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key

# TensorFlow Models
TENSORFLOW_MODEL_PATH=./models/
NSFW_MODEL_PATH=./models/nsfw-detection

# AI Processing Limits
MAX_REQUESTS_PER_MINUTE=60
MAX_CONCURRENT_JOBS=5

# Timezone
TZ=Australia/Sydney