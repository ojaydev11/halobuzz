# HaloBuzz Mobile App Production Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
API_BASE_URL=https://<backend>.railway.app

# Agora Video Streaming
AGORA_APP_ID=<your-agora-app-id>

# Payment Providers
STRIPE_PUBLISHABLE_KEY=<your-stripe-publishable-key>
ESEWA_ENABLED=true
KHALTI_ENABLED=true

# Feature Flags
GAMES_ENABLED=true
MODERATION_ENABLED=true
ANALYTICS_ENABLED=true

# App Configuration
APP_NAME=HaloBuzz
APP_VERSION=0.1.0
SUPPORT_EMAIL=support@halobuzz.com

# Security
CERT_PINNING_ENABLED=true
BIOMETRIC_AUTH_ENABLED=true

# Performance
IMAGE_CACHE_SIZE=100
VIDEO_CACHE_SIZE=50
NETWORK_TIMEOUT=30000