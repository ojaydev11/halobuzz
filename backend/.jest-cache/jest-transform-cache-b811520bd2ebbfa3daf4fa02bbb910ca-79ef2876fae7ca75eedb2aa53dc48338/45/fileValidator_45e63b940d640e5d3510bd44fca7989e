  /**
   * Scans file for malicious content (basic check)
   */
  static async scanForMaliciousContent(fileBuffer) {
    /* istanbul ignore next */
    cov_2pgq2dzh1y().f[15]++;
    cov_2pgq2dzh1y().s[95]++;
    try {
      // Convert buffer to string for pattern matching
      const fileContent =
      /* istanbul ignore next */
      (cov_2pgq2dzh1y().s[96]++, fileBuffer.toString('binary'));
      // Check for common malicious patterns
      const maliciousPatterns =
      /* istanbul ignore next */
      (cov_2pgq2dzh1y().s[97]++, [/<script[^>]*>.*?<\/script>/gi, /javascript:/gi, /vbscript:/gi, /onload\s*=/gi, /onerror\s*=/gi, /eval\s*\(/gi, /document\.cookie/gi, /window\.location/gi, /\.exe/gi, /\.bat/gi, /\.cmd/gi]);
      /* istanbul ignore next */
      cov_2pgq2dzh1y().s[98]++;
      for (const pattern of maliciousPatterns) {
        /* istanbul ignore next */
        cov_2pgq2dzh1y().s[99]++;
        if (pattern.test(fileContent)) {
          /* istanbul ignore next */
          cov_2pgq2dzh1y().b[35][0]++;
          cov_2pgq2dzh1y().s[100]++;
          logger_1.logger.warn('Malicious content detected in file');
          /* istanbul ignore next */
          cov_2pgq2dzh1y().s[101]++;
          return false;
        } else
        /* istanbul ignore next */
        {
          cov_2pgq2dzh1y().b[35][1]++;
