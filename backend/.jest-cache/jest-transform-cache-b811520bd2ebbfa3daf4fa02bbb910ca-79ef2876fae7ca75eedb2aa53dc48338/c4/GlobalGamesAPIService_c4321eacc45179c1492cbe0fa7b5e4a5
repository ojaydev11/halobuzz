  /**
   * Launch a cross-platform game
   */
  async launchGame(request) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[8]++;
    const {
      userId,
      gameId,
      stake,
      launchMode,
      spectatorMode,
      metadata
    } =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[32]++, request);
    // Validate user balance
    const wallet =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[33]++, await CoinWallet_1.CoinWallet.findOne({
      userId
    }));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[34]++;
    if (
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[10][0]++, !wallet) ||
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[10][1]++, !wallet.canSpend(stake))) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[9][0]++;
      cov_2jk8kvshps().s[35]++;
      throw new Error('Insufficient balance');
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[9][1]++;
    }
    // Find game configuration
    const game =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[36]++, await this.findGame(gameId));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[37]++;
    if (
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[12][0]++, !game) ||
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[12][1]++, !game.isActive)) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[11][0]++;
      cov_2jk8kvshps().s[38]++;
      throw new Error('Game not available');
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[11][1]++;
    }
    // Validate stake limits
    cov_2jk8kvshps().s[39]++;
    if (
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[14][0]++, stake < game.minStake) ||
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[14][1]++, stake > game.maxStake)) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[13][0]++;
      cov_2jk8kvshps().s[40]++;
      throw new Error(`Stake must be between ${game.minStake} and ${game.maxStake} coins`);
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[13][1]++;
    }
    const provider =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[41]++, this.providers.get(game.providerId));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[42]++;
    if (
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[16][0]++, !provider) ||
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[16][1]++, !provider.isActive)) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[15][0]++;
      cov_2jk8kvshps().s[43]++;
      throw new Error('Game provider not available');
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[15][1]++;
    }
    // Generate unique session ID
    const sessionId =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[44]++, `session_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`);
    // Lock user balance
    /* istanbul ignore next */
    cov_2jk8kvshps().s[45]++;
    await this.lockUserBalance(userId, stake, sessionId);
    /* istanbul ignore next */
    cov_2jk8kvshps().s[46]++;
    try {
      let launchResponse;
      /* istanbul ignore next */
      cov_2jk8kvshps().s[47]++;
      if (provider.id === 'halobuzz-native') {
        /* istanbul ignore next */
        cov_2jk8kvshps().b[17][0]++;
        cov_2jk8kvshps().s[48]++;
        // Handle native HaloBuzz games
        launchResponse = await this.launchNativeGame(request, sessionId);
      } else {
        /* istanbul ignore next */
        cov_2jk8kvshps().b[17][1]++;
        cov_2jk8kvshps().s[49]++;
        // Handle external provider games
        launchResponse = await this.launchExternalGame(request, sessionId, provider, game);
      }
      // Store active session
      /* istanbul ignore next */
      cov_2jk8kvshps().s[50]++;
      this.activeSessions.set(sessionId, {
        userId,
        gameId,
        providerId: game.providerId,
        stake,
        startTime: new Date(),
        status: 'active',
        spectatorMode,
        metadata
      });
      /* istanbul ignore next */
      cov_2jk8kvshps().s[51]++;
      this.emit('gameSessionStarted', {
        sessionId,
        userId,
        gameId,
        stake,
        provider: provider.name
      });
      /* istanbul ignore next */
      cov_2jk8kvshps().s[52]++;
      return {
        sessionId,
        launchUrl:
        /* istanbul ignore next */
        (cov_2jk8kvshps().b[18][0]++, launchResponse.launchUrl) ||
        /* istanbul ignore next */
        (cov_2jk8kvshps().b[18][1]++, null),
        iframe:
        /* istanbul ignore next */
        (cov_2jk8kvshps().b[19][0]++, launchResponse.iframe) ||
        /* istanbul ignore next */
        (cov_2jk8kvshps().b[19][1]++, null),
        gameData:
        /* istanbul ignore next */
        (cov_2jk8kvshps().b[20][0]++, launchResponse.gameData) ||
        /* istanbul ignore next */
        (cov_2jk8kvshps().b[20][1]++, null),
        spectatorUrl: spectatorMode ?
        /* istanbul ignore next */
        (cov_2jk8kvshps().b[21][0]++, launchResponse.spectatorUrl) :
        /* istanbul ignore next */
        (cov_2jk8kvshps().b[21][1]++, null)
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_2jk8kvshps().s[53]++;
      // Unlock balance on failure
      await this.unlockUserBalance(userId, stake, sessionId);
      /* istanbul ignore next */
      cov_2jk8kvshps().s[54]++;
      throw error;
    }
  }
  /**
   * Handle game completion and process results
   */
  async processGameResult(result) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[9]++;
    const {
      sessionId,
      userId,
      gameId,
      providerId,
      stake,
      payout,
      gameData
    } =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[55]++, result);
    const session =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[56]++, this.activeSessions.get(sessionId));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[57]++;
    if (!session) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[22][0]++;
      cov_2jk8kvshps().s[58]++;
      throw new Error('Invalid session');
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[22][1]++;
    }
    cov_2jk8kvshps().s[59]++;
    try {
      // Update user wallet
      const wallet =
      /* istanbul ignore next */
      (cov_2jk8kvshps().s[60]++, await CoinWallet_1.CoinWallet.findOne({
        userId
      }));
      /* istanbul ignore next */
      cov_2jk8kvshps().s[61]++;
      if (!wallet) {
        /* istanbul ignore next */
        cov_2jk8kvshps().b[23][0]++;
        cov_2jk8kvshps().s[62]++;
        throw new Error('Wallet not found');
      } else
      /* istanbul ignore next */
      {
        cov_2jk8kvshps().b[23][1]++;
      }
      // Calculate platform commission
      const provider =
      /* istanbul ignore next */
      (cov_2jk8kvshps().s[63]++, this.providers.get(providerId));
      const commission =
      /* istanbul ignore next */
      (cov_2jk8kvshps().s[64]++, provider ?
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[24][0]++, payout * provider.commissionRate) :
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[24][1]++, 0));
      const netPayout =
      /* istanbul ignore next */
      (cov_2jk8kvshps().s[65]++, Math.max(0, payout - commission));
      // Unlock original stake
      /* istanbul ignore next */
      cov_2jk8kvshps().s[66]++;
      await this.unlockUserBalance(userId, stake, sessionId);
      // Process win/loss
      /* istanbul ignore next */
      cov_2jk8kvshps().s[67]++;
      if (
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[26][0]++, result.result === 'win') &&
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[26][1]++, netPayout > 0)) {
        /* istanbul ignore next */
        cov_2jk8kvshps().b[25][0]++;
        cov_2jk8kvshps().s[68]++;
        await this.creditWallet(userId, netPayout, {
          type: 'game_win',
          gameId,
          sessionId,
          providerId
        });
      } else
      /* istanbul ignore next */
      {
        cov_2jk8kvshps().b[25][1]++;
      }
      // Record transaction
      cov_2jk8kvshps().s[69]++;
      await this.recordGameTransaction(result, netPayout, commission);
      // Update session status
      /* istanbul ignore next */
      cov_2jk8kvshps().s[70]++;
      session.status = 'completed';
      /* istanbul ignore next */
      cov_2jk8kvshps().s[71]++;
      session.endTime = new Date();
      /* istanbul ignore next */
      cov_2jk8kvshps().s[72]++;
      session.result = result.result;
      /* istanbul ignore next */
      cov_2jk8kvshps().s[73]++;
      session.payout = netPayout;
      /* istanbul ignore next */
      cov_2jk8kvshps().s[74]++;
      this.emit('gameSessionCompleted', {
        sessionId,
        userId,
        gameId,
        result: result.result,
        stake,
        payout: netPayout,
        commission,
        duration: result.duration
      });
      // Clean up session after delay
      /* istanbul ignore next */
      cov_2jk8kvshps().s[75]++;
      setTimeout(() => {
        /* istanbul ignore next */
        cov_2jk8kvshps().f[10]++;
        cov_2jk8kvshps().s[76]++;
        this.activeSessions.delete(sessionId);
      }, 300000); // 5 minutes
    } catch (error) {
      /* istanbul ignore next */
      cov_2jk8kvshps().s[77]++;
      logger_1.logger.error('Error processing game result:', error);
      /* istanbul ignore next */
      cov_2jk8kvshps().s[78]++;
      session.status = 'error';
      /* istanbul ignore next */
      cov_2jk8kvshps().s[79]++;
      throw error;
    }
  }
  /**
   * Get game statistics and analytics
   */
  async getGameAnalytics(providerId, gameId, days =
  /* istanbul ignore next */
  (cov_2jk8kvshps().b[27][0]++, 30)) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[11]++;
    const since =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[80]++, new Date(Date.now() - days * 24 * 60 * 60 * 1000));
    const matchStage =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[81]++, {
      createdAt: {
        $gte: since
      },
      type: {
        $in: ['game_stake', 'game_win', 'game_loss']
      },
      status: 'completed'
    });
    /* istanbul ignore next */
    cov_2jk8kvshps().s[82]++;
    if (providerId) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[28][0]++;
      cov_2jk8kvshps().s[83]++;
      matchStage['context.providerId'] = providerId;
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[28][1]++;
    }
    cov_2jk8kvshps().s[84]++;
    if (gameId) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[29][0]++;
      cov_2jk8kvshps().s[85]++;
      matchStage['context.gameId'] = gameId;
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[29][1]++;
    }
    const analytics =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[86]++, await CoinTransaction_1.CoinTransaction.aggregate([{
      $match: matchStage
    }, {
      $group: {
        _id: {
          providerId: '$context.providerId',
          gameId: '$context.gameId'
        },
        totalSessions: {
          $sum: 1
        },
        totalStake: {
          $sum: '$amount'
        },
        totalPayout: {
          $sum: {
            $cond: [{
              $eq: ['$type', 'game_win']
            }, '$amount', 0]
          }
        },
        avgStake: {
          $avg: '$amount'
        },
        uniqueUsers: {
          $addToSet: '$userId'
        },
        winSessions: {
          $sum: {
            $cond: [{
              $eq: ['$type', 'game_win']
            }, 1, 0]
          }
        }
      }
    }, {
      $addFields: {
        uniqueUserCount: {
          $size: '$uniqueUsers'
        },
        winRate: {
          $cond: [{
            $gt: ['$totalSessions', 0]
          }, {
            $divide: ['$winSessions', '$totalSessions']
          }, 0]
        },
        rtp: {
          $cond: [{
            $gt: ['$totalStake', 0]
          }, {
            $divide: ['$totalPayout', '$totalStake']
          }, 0]
        }
      }
    }, {
      $sort: {
        totalStake: -1
      }
    }]));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[87]++;
    return analytics;
  }
  /**
   * Launch native HaloBuzz game
   */
  async launchNativeGame(request, sessionId) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[12]++;
    const {
      userId,
      gameId,
      stake
    } =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[88]++, request);
    // Create player object
    const player =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[89]++, {
      id: userId,
      username: `user_${userId}`,
      level: 1,
      avatar: '',
      stake
    });
    // Join the advanced game session
    const gameSession =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[90]++, await AdvancedGamesService_1.advancedGamesService.joinGame(gameId, player));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[91]++;
    return {
      gameData: {
        sessionId: gameSession.sessionId,
        gameId,
        status: gameSession.status,
        players: gameSession.players.size,
