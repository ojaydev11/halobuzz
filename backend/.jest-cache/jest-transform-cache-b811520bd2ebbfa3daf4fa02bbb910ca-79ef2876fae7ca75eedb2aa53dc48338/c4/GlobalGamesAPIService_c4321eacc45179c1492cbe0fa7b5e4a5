      }
    };
  }
  /**
   * Launch external provider game
   */
  async launchExternalGame(request, sessionId, provider, game) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[13]++;
    const {
      userId,
      stake,
      launchMode
    } =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[92]++, request);
    // Mock external API call (replace with actual provider integration)
    const apiEndpoint =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[93]++, `${provider.endpoint}/launch`);
    /* istanbul ignore next */
    cov_2jk8kvshps().s[94]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_2jk8kvshps().s[95]++, await axios_1.default.post(apiEndpoint, {
        game_id: game.gameId,
        user_id: userId,
        session_id: sessionId,
        currency: 'HBC',
        amount: stake,
        launch_mode: launchMode,
        return_url: `${process.env.API_BASE_URL}/games/external/return/${sessionId}`,
        webhook_url: `${process.env.API_BASE_URL}/games/external/webhook`
      }, {
        headers: {
          'Authorization': `Bearer ${provider.apiKey}`,
          'Content-Type': 'application/json'
        }
      }));
      /* istanbul ignore next */
      cov_2jk8kvshps().s[96]++;
