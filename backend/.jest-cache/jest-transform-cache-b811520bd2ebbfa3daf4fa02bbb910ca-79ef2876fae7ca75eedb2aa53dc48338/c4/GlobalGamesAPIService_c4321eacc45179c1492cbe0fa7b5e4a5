      cov_2jk8kvshps().s[98]++;
      return {
        launchUrl: `https://demo.provider.com/game/${game.gameId}?session=${sessionId}`,
        iframe: launchMode === 'iframe'
      };
    }
  }
  /**
   * Lock user balance for game session
   */
  async lockUserBalance(userId, amount, sessionId) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[14]++;
    const wallet =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[99]++, await CoinWallet_1.CoinWallet.findOne({
      userId
    }));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[100]++;
    if (!wallet) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[30][0]++;
      cov_2jk8kvshps().s[101]++;
      throw new Error('Wallet not found');
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[30][1]++;
    }
    cov_2jk8kvshps().s[102]++;
    if (wallet.availableBalance < amount) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[31][0]++;
      cov_2jk8kvshps().s[103]++;
      throw new Error('Insufficient balance');
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[31][1]++;
    }
    cov_2jk8kvshps().s[104]++;
    wallet.availableBalance -= amount;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[105]++;
    wallet.lockedBalance += amount;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[106]++;
    wallet.isLocked = true;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[107]++;
    wallet.lockedBy = sessionId;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[108]++;
    await wallet.save();
    // Record stake transaction
    /* istanbul ignore next */
    cov_2jk8kvshps().s[109]++;
    await CoinTransaction_1.CoinTransaction.create({
      userId,
      type: 'game_stake',
      amount,
      balanceBefore: wallet.availableBalance + amount,
      balanceAfter: wallet.availableBalance,
      source: 'game',
      destination: 'game_pot',
      context: {
        gameSessionId: sessionId
      },
      status: 'completed'
    });
  }
  /**
   * Unlock user balance
   */
  async unlockUserBalance(userId, amount, sessionId) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[15]++;
    const wallet =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[110]++, await CoinWallet_1.CoinWallet.findOne({
      userId
    }));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[111]++;
    if (!wallet) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[32][0]++;
      cov_2jk8kvshps().s[112]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[32][1]++;
    }
    cov_2jk8kvshps().s[113]++;
    wallet.availableBalance += amount;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[114]++;
    wallet.lockedBalance = Math.max(0, wallet.lockedBalance - amount);
    /* istanbul ignore next */
    cov_2jk8kvshps().s[115]++;
    if (wallet.lockedBy === sessionId) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[33][0]++;
      cov_2jk8kvshps().s[116]++;
      wallet.isLocked = false;
      /* istanbul ignore next */
      cov_2jk8kvshps().s[117]++;
      wallet.lockedBy = undefined;
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[33][1]++;
    }
    cov_2jk8kvshps().s[118]++;
    await wallet.save();
  }
  /**
   * Credit wallet with winnings
   */
  async creditWallet(userId, amount, context) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[16]++;
    const wallet =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[119]++, await CoinWallet_1.CoinWallet.findOne({
      userId
    }));
    /* istanbul ignore next */
    cov_2jk8kvshps().s[120]++;
    if (!wallet) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[34][0]++;
      cov_2jk8kvshps().s[121]++;
      throw new Error('Wallet not found');
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[34][1]++;
    }
    cov_2jk8kvshps().s[122]++;
    wallet.availableBalance += amount;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[123]++;
    wallet.totalBalance += amount;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[124]++;
    wallet.balanceBySource.earned += amount;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[125]++;
    wallet.stats.totalEarned += amount;
    /* istanbul ignore next */
    cov_2jk8kvshps().s[126]++;
    if (amount > wallet.stats.biggestWin) {
      /* istanbul ignore next */
      cov_2jk8kvshps().b[35][0]++;
      cov_2jk8kvshps().s[127]++;
      wallet.stats.biggestWin = amount;
    } else
    /* istanbul ignore next */
    {
      cov_2jk8kvshps().b[35][1]++;
    }
    cov_2jk8kvshps().s[128]++;
    await wallet.save();
    // Record win transaction
    /* istanbul ignore next */
    cov_2jk8kvshps().s[129]++;
    await CoinTransaction_1.CoinTransaction.create({
      userId,
      type: 'game_win',
      amount,
      balanceBefore: wallet.availableBalance - amount,
      balanceAfter: wallet.availableBalance,
      source: 'game',
      destination: 'wallet',
      context,
      status: 'completed'
    });
  }
  /**
   * Record game transaction for analytics
   */
  async recordGameTransaction(result, netPayout, commission) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[17]++;
    cov_2jk8kvshps().s[130]++;
    await CoinTransaction_1.CoinTransaction.create({
      userId: result.userId,
      type: result.result === 'win' ?
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[36][0]++, 'game_win') :
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[36][1]++, 'game_loss'),
      amount: result.result === 'win' ?
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[37][0]++, netPayout) :
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[37][1]++, result.stake),
      balanceBefore: 0,
      // Will be updated by wallet operations
      balanceAfter: 0,
      // Will be updated by wallet operations
      source: 'game',
      destination: result.result === 'win' ?
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[38][0]++, 'wallet') :
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[38][1]++, 'system_fee'),
      context: {
        gameId: result.gameId,
        gameSessionId: result.sessionId,
        providerId: result.providerId,
        originalPayout: result.payout,
        commission,
        gameData: JSON.stringify(result.gameData)
      },
      status: 'completed'
    });
  }
  /**
   * Find game by ID across all providers
   */
  async findGame(gameId) {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[18]++;
    const games =
    /* istanbul ignore next */
    (cov_2jk8kvshps().s[131]++, await this.getAvailableGames());
    /* istanbul ignore next */
    cov_2jk8kvshps().s[132]++;
    return /* istanbul ignore next */(cov_2jk8kvshps().b[39][0]++, games.find(game => {
      /* istanbul ignore next */
      cov_2jk8kvshps().f[19]++;
      cov_2jk8kvshps().s[133]++;
      return /* istanbul ignore next */(cov_2jk8kvshps().b[40][0]++, game.gameId === gameId) ||
      /* istanbul ignore next */
      (cov_2jk8kvshps().b[40][1]++, game.haloBuzzGameId === gameId);
    })) ||
    /* istanbul ignore next */
    (cov_2jk8kvshps().b[39][1]++, null);
  }
  /**
   * Start monitoring and health checks
   */
  startMonitoring() {
    /* istanbul ignore next */
    cov_2jk8kvshps().f[20]++;
    cov_2jk8kvshps().s[134]++;
    // Monitor provider health every 5 minutes
    setInterval(async () => {
      /* istanbul ignore next */
      cov_2jk8kvshps().f[21]++;
      cov_2jk8kvshps().s[135]++;
      for (const provider of this.providers.values()) {
        /* istanbul ignore next */
        cov_2jk8kvshps().s[136]++;
        if (provider.id === 'halobuzz-native') {
          /* istanbul ignore next */
          cov_2jk8kvshps().b[41][0]++;
          cov_2jk8kvshps().s[137]++;
          continue;
        } else
        /* istanbul ignore next */
        {
          cov_2jk8kvshps().b[41][1]++;
        }
        cov_2jk8kvshps().s[138]++;
        try {
          /* istanbul ignore next */
          cov_2jk8kvshps().s[139]++;
          // Ping provider health endpoint
          await axios_1.default.get(`${provider.endpoint}/health`, {
            headers: {
              'Authorization': `Bearer ${provider.apiKey}`
            },
            timeout: 5000
          });
        } catch (error) {
          /* istanbul ignore next */
          cov_2jk8kvshps().s[140]++;
          logger_1.logger.warn(`Provider ${provider.name} health check failed:`, error);
          // Could implement auto-disable logic here
        }
      }
    }, 300000); // 5 minutes
    // Clean up expired sessions every hour
    /* istanbul ignore next */
    cov_2jk8kvshps().s[141]++;
    setInterval(() => {
      /* istanbul ignore next */
      cov_2jk8kvshps().f[22]++;
      const now =
      /* istanbul ignore next */
      (cov_2jk8kvshps().s[142]++, Date.now());
      const expireTime =
      /* istanbul ignore next */
      (cov_2jk8kvshps().s[143]++, 24 * 60 * 60 * 1000); // 24 hours
      /* istanbul ignore next */
      cov_2jk8kvshps().s[144]++;
      for (const [sessionId, session] of this.activeSessions.entries()) {
        /* istanbul ignore next */
        cov_2jk8kvshps().s[145]++;
        if (now - session.startTime.getTime() > expireTime) {
          /* istanbul ignore next */
          cov_2jk8kvshps().b[42][0]++;
          cov_2jk8kvshps().s[146]++;
          this.activeSessions.delete(sessionId);
          /* istanbul ignore next */
          cov_2jk8kvshps().s[147]++;
          logger_1.logger.info(`Cleaned up expired session: ${sessionId}`);
        } else
        /* istanbul ignore next */
        {
          cov_2jk8kvshps().b[42][1]++;
        }
      }
    }, 3600000); // 1 hour
  }
}
