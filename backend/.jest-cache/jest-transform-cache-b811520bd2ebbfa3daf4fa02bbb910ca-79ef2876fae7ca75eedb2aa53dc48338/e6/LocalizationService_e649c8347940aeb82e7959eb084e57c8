  }
  // Get content filter for country
  async getContentFilter(countryCode) {
    /* istanbul ignore next */
    cov_eov4ievwz().f[11]++;
    cov_eov4ievwz().s[49]++;
    try {
      /* istanbul ignore next */
      cov_eov4ievwz().s[50]++;
      return /* istanbul ignore next */(cov_eov4ievwz().b[7][0]++, this.contentFilters.get(countryCode)) ||
      /* istanbul ignore next */
      (cov_eov4ievwz().b[7][1]++, null);
    } catch (error) {
      /* istanbul ignore next */
      cov_eov4ievwz().s[51]++;
      logger_1.logger.error('Error getting content filter:', error);
      /* istanbul ignore next */
      cov_eov4ievwz().s[52]++;
      return null;
    }
  }
  // Check if content is allowed
  async isContentAllowed(countryCode, contentType, category, userAge) {
    /* istanbul ignore next */
    cov_eov4ievwz().f[12]++;
    cov_eov4ievwz().s[53]++;
    try {
      const filter =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[54]++, await this.getContentFilter(countryCode));
      /* istanbul ignore next */
      cov_eov4ievwz().s[55]++;
      if (!filter) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[8][0]++;
        cov_eov4ievwz().s[56]++;
        return {
          allowed: true
        }; // No restrictions if no filter
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[8][1]++;
      }
      const restrictions =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[57]++, []);
      // Check content type
      /* istanbul ignore next */
      cov_eov4ievwz().s[58]++;
      if (!filter.restrictions.contentTypes.includes(contentType)) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[9][0]++;
        cov_eov4ievwz().s[59]++;
        return {
          allowed: false,
          reason: 'Content type not allowed in this region',
          restrictions: ['content_type']
        };
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[9][1]++;
      }
      // Check blocked categories
      cov_eov4ievwz().s[60]++;
      if (filter.restrictions.blockedCategories.includes(category)) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[10][0]++;
        cov_eov4ievwz().s[61]++;
        return {
          allowed: false,
          reason: 'Content category blocked in this region',
          restrictions: ['category']
        };
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[10][1]++;
      }
      // Check age gate
      cov_eov4ievwz().s[62]++;
      if (
      /* istanbul ignore next */
      (cov_eov4ievwz().b[12][0]++, filter.restrictions.ageGate) && (
      /* istanbul ignore next */
      (cov_eov4ievwz().b[12][1]++, !userAge) ||
      /* istanbul ignore next */
      (cov_eov4ievwz().b[12][2]++, userAge < 18))) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[11][0]++;
        cov_eov4ievwz().s[63]++;
        restrictions.push('age_verification_required');
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[11][1]++;
      }
      // Check time restrictions
      const currentHour =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[64]++, new Date().getHours());
      const currentTime =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[65]++, `${currentHour.toString().padStart(2, '0')}:00`);
      const hasTimeRestriction =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[66]++, filter.restrictions.timeRestrictions.some(restriction => {
        /* istanbul ignore next */
        cov_eov4ievwz().f[13]++;
        cov_eov4ievwz().s[67]++;
        return /* istanbul ignore next */(cov_eov4ievwz().b[13][0]++, currentTime >= restriction.start) ||
        /* istanbul ignore next */
        (cov_eov4ievwz().b[13][1]++, currentTime <= restriction.end);
      }));
      /* istanbul ignore next */
      cov_eov4ievwz().s[68]++;
      if (hasTimeRestriction) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[14][0]++;
        cov_eov4ievwz().s[69]++;
        restrictions.push('time_restriction');
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[14][1]++;
      }
      cov_eov4ievwz().s[70]++;
      return {
        allowed: restrictions.length === 0,
        restrictions: restrictions.length > 0 ?
        /* istanbul ignore next */
        (cov_eov4ievwz().b[15][0]++, restrictions) :
        /* istanbul ignore next */
        (cov_eov4ievwz().b[15][1]++, undefined)
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_eov4ievwz().s[71]++;
      logger_1.logger.error('Error checking content allowance:', error);
      /* istanbul ignore next */
      cov_eov4ievwz().s[72]++;
      return {
        allowed: true
      }; // Default to allowed on error
    }
  }
  // Format currency
  formatCurrency(amount, localeCode) {
    /* istanbul ignore next */
    cov_eov4ievwz().f[14]++;
    cov_eov4ievwz().s[73]++;
    try {
      const locale =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[74]++, this.supportedLocales.get(localeCode));
      /* istanbul ignore next */
      cov_eov4ievwz().s[75]++;
      if (!locale) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[16][0]++;
        cov_eov4ievwz().s[76]++;
        return `${amount}`;
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[16][1]++;
      }
      const {
        decimal,
        thousands
      } =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[77]++, locale.numberFormat);
      const formatted =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[78]++, amount.toLocaleString('en-US', {
        minimumFractionDigits: 2,
        maximumFractionDigits: 2
      }).replace(',', thousands).replace('.', decimal));
      /* istanbul ignore next */
      cov_eov4ievwz().s[79]++;
      return `${locale.currency} ${formatted}`;
    } catch (error) {
      /* istanbul ignore next */
      cov_eov4ievwz().s[80]++;
      logger_1.logger.error('Error formatting currency:', error);
      /* istanbul ignore next */
      cov_eov4ievwz().s[81]++;
      return `${amount}`;
    }
  }
  // Format date
  formatDate(date, localeCode) {
    /* istanbul ignore next */
    cov_eov4ievwz().f[15]++;
    cov_eov4ievwz().s[82]++;
    try {
      const locale =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[83]++, this.supportedLocales.get(localeCode));
      /* istanbul ignore next */
      cov_eov4ievwz().s[84]++;
      if (!locale) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[17][0]++;
        cov_eov4ievwz().s[85]++;
        return date.toISOString();
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[17][1]++;
      }
      // Simple date formatting based on locale
      const year =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[86]++, date.getFullYear());
      const month =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[87]++, (date.getMonth() + 1).toString().padStart(2, '0'));
      const day =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[88]++, date.getDate().toString().padStart(2, '0'));
      /* istanbul ignore next */
      cov_eov4ievwz().s[89]++;
      switch (locale.dateFormat) {
        case 'MM/DD/YYYY':
          /* istanbul ignore next */
          cov_eov4ievwz().b[18][0]++;
          cov_eov4ievwz().s[90]++;
          return `${month}/${day}/${year}`;
        case 'DD/MM/YYYY':
          /* istanbul ignore next */
          cov_eov4ievwz().b[18][1]++;
          cov_eov4ievwz().s[91]++;
          return `${day}/${month}/${year}`;
        case 'YYYY/MM/DD':
          /* istanbul ignore next */
          cov_eov4ievwz().b[18][2]++;
          cov_eov4ievwz().s[92]++;
          return `${year}/${month}/${day}`;
        default:
          /* istanbul ignore next */
          cov_eov4ievwz().b[18][3]++;
          cov_eov4ievwz().s[93]++;
          return `${day}/${month}/${year}`;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_eov4ievwz().s[94]++;
      logger_1.logger.error('Error formatting date:', error);
      /* istanbul ignore next */
      cov_eov4ievwz().s[95]++;
      return date.toISOString();
    }
  }
  // Get supported locales
  getSupportedLocales() {
    /* istanbul ignore next */
    cov_eov4ievwz().f[16]++;
    cov_eov4ievwz().s[96]++;
    return Array.from(this.supportedLocales.values());
  }
  // Get supported countries
  getSupportedCountries() {
    /* istanbul ignore next */
    cov_eov4ievwz().f[17]++;
    cov_eov4ievwz().s[97]++;
    return Array.from(this.geoLocations.values());
  }
}
