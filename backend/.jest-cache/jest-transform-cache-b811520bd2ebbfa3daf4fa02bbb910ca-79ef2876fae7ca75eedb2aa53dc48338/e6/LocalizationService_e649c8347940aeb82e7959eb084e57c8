      if (locale) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[1][0]++;
        cov_eov4ievwz().s[25]++;
        await (0, redis_1.setCache)(`locale:${localeCode}`, locale, 3600); // 1 hour TTL
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[1][1]++;
      }
      cov_eov4ievwz().s[26]++;
      return /* istanbul ignore next */(cov_eov4ievwz().b[2][0]++, locale) ||
      /* istanbul ignore next */
      (cov_eov4ievwz().b[2][1]++, null);
    } catch (error) {
      /* istanbul ignore next */
      cov_eov4ievwz().s[27]++;
      logger_1.logger.error('Error getting locale config:', error);
      /* istanbul ignore next */
      cov_eov4ievwz().s[28]++;
      return null;
    }
  }
  // Translate text
  async translate(key, localeCode, params) {
    /* istanbul ignore next */
    cov_eov4ievwz().f[8]++;
    cov_eov4ievwz().s[29]++;
    try {
      const locale =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[30]++, await this.getLocaleConfig(localeCode));
      /* istanbul ignore next */
      cov_eov4ievwz().s[31]++;
      if (!locale) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[3][0]++;
        cov_eov4ievwz().s[32]++;
        return key; // Return key if locale not found
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[3][1]++;
      }
      let translation =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[33]++,
      /* istanbul ignore next */
      (cov_eov4ievwz().b[4][0]++, locale.translations[key]) ||
      /* istanbul ignore next */
      (cov_eov4ievwz().b[4][1]++, key));
      // Replace parameters
      /* istanbul ignore next */
      cov_eov4ievwz().s[34]++;
      if (params) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[5][0]++;
        cov_eov4ievwz().s[35]++;
        Object.keys(params).forEach(param => {
          /* istanbul ignore next */
          cov_eov4ievwz().f[9]++;
          cov_eov4ievwz().s[36]++;
          translation = translation.replace(`{${param}}`, params[param]);
        });
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[5][1]++;
      }
      cov_eov4ievwz().s[37]++;
      return translation;
    } catch (error) {
      /* istanbul ignore next */
      cov_eov4ievwz().s[38]++;
      logger_1.logger.error('Error translating text:', error);
      /* istanbul ignore next */
      cov_eov4ievwz().s[39]++;
      return key;
    }
  }
  // Get geo location by IP
  async getGeoLocation(ipAddress) {
    /* istanbul ignore next */
    cov_eov4ievwz().f[10]++;
    cov_eov4ievwz().s[40]++;
    try {
      // Check cache first
      const cached =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[41]++, await (0, redis_1.getCache)(`geo:${ipAddress}`));
      /* istanbul ignore next */
      cov_eov4ievwz().s[42]++;
      if (cached) {
        /* istanbul ignore next */
        cov_eov4ievwz().b[6][0]++;
        cov_eov4ievwz().s[43]++;
        return cached;
      } else
      /* istanbul ignore next */
      {
        cov_eov4ievwz().b[6][1]++;
      }
      // Mock geo location - in production, use a real IP geolocation service
      const mockLocation =
      /* istanbul ignore next */
      (cov_eov4ievwz().s[44]++, {
        country: 'Nepal',
        countryCode: 'NP',
        region: 'Asia',
        city: 'Kathmandu',
        timezone: 'Asia/Kathmandu',
