  // Check device
  async checkDevice(userId) {
    /* istanbul ignore next */
    cov_1qjkhz1wn4().f[16]++;
    cov_1qjkhz1wn4().s[136]++;
    try {
      let score =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[137]++, 0);
      const flags =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[138]++, []);
      // Check for new device (would require device fingerprinting)
      const rules =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[139]++, this.fraudDetectionRules.get('device'));
      // Mock new device detection
      const isNewDevice =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[140]++, Math.random() < 0.1); // 10% chance of new device
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[141]++;
      if (isNewDevice) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[22][0]++;
        cov_1qjkhz1wn4().s[142]++;
        score += rules.newDeviceRisk;
        /* istanbul ignore next */
        cov_1qjkhz1wn4().s[143]++;
        flags.push('new_device');
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[22][1]++;
      }
      // Check for VPN/Proxy (would require IP analysis)
      cov_1qjkhz1wn4().s[144]++;
      if (rules.vpnDetection) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[23][0]++;
        const isVPN =
        /* istanbul ignore next */
        (cov_1qjkhz1wn4().s[145]++, Math.random() < 0.05); // 5% chance of VPN
        /* istanbul ignore next */
        cov_1qjkhz1wn4().s[146]++;
        if (isVPN) {
          /* istanbul ignore next */
          cov_1qjkhz1wn4().b[24][0]++;
          cov_1qjkhz1wn4().s[147]++;
          score += 0.2;
          /* istanbul ignore next */
          cov_1qjkhz1wn4().s[148]++;
          flags.push('vpn_detected');
        } else
        /* istanbul ignore next */
        {
          cov_1qjkhz1wn4().b[24][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[23][1]++;
      }
      cov_1qjkhz1wn4().s[149]++;
      return {
        score,
        flags
      };
