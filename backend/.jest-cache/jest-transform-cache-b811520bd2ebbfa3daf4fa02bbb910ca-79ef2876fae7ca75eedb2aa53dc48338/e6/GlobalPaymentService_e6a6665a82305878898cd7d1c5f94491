  }
  // Check behavior
  async checkBehavior(userId) {
    /* istanbul ignore next */
    cov_1qjkhz1wn4().f[17]++;
    cov_1qjkhz1wn4().s[152]++;
    try {
      let score =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[153]++, 0);
      const flags =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[154]++, []);
      const rules =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[155]++, this.fraudDetectionRules.get('behavior'));
      // Check for unusual hours (would require user's typical activity patterns)
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[156]++;
      if (rules.unusualHours) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[25][0]++;
        const currentHour =
        /* istanbul ignore next */
        (cov_1qjkhz1wn4().s[157]++, new Date().getHours());
        const isUnusualHour =
        /* istanbul ignore next */
        (cov_1qjkhz1wn4().s[158]++,
        /* istanbul ignore next */
        (cov_1qjkhz1wn4().b[26][0]++, currentHour < 6) ||
        /* istanbul ignore next */
        (cov_1qjkhz1wn4().b[26][1]++, currentHour > 23));
        /* istanbul ignore next */
        cov_1qjkhz1wn4().s[159]++;
        if (isUnusualHour) {
          /* istanbul ignore next */
          cov_1qjkhz1wn4().b[27][0]++;
          cov_1qjkhz1wn4().s[160]++;
          score += 0.1;
          /* istanbul ignore next */
          cov_1qjkhz1wn4().s[161]++;
          flags.push('unusual_hours');
        } else
        /* istanbul ignore next */
        {
          cov_1qjkhz1wn4().b[27][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[25][1]++;
      }
      // Check for rapid succession transactions
      cov_1qjkhz1wn4().s[162]++;
      if (rules.rapidSuccession) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[28][0]++;
        const recentTransactions =
        /* istanbul ignore next */
        (cov_1qjkhz1wn4().s[163]++, await Transaction_1.Transaction.find({
          userId,
          createdAt: {
            $gte: new Date(Date.now() - 5 * 60 * 1000)
          } // Last 5 minutes
        }));
        /* istanbul ignore next */
        cov_1qjkhz1wn4().s[164]++;
        if (recentTransactions.length > 3) {
          /* istanbul ignore next */
          cov_1qjkhz1wn4().b[29][0]++;
          cov_1qjkhz1wn4().s[165]++;
          score += 0.2;
          /* istanbul ignore next */
          cov_1qjkhz1wn4().s[166]++;
          flags.push('rapid_succession');
        } else
        /* istanbul ignore next */
        {
          cov_1qjkhz1wn4().b[29][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[28][1]++;
      }
      cov_1qjkhz1wn4().s[167]++;
      return {
        score,
        flags
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[168]++;
      logger_1.logger.error('Error checking behavior:', error);
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[169]++;
      return {
        score: 0.1,
        flags: ['behavior_check_error']
      };
    }
  }
  // Get or create Stripe customer
  async getOrCreateStripeCustomer(customerInfo) {
    /* istanbul ignore next */
    cov_1qjkhz1wn4().f[18]++;
    cov_1qjkhz1wn4().s[170]++;
    try {
      // Check if customer exists
      const existingCustomers =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[171]++, await this.stripe.customers.list({
        email: customerInfo.email,
        limit: 1
      }));
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[172]++;
      if (existingCustomers.data.length > 0) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[30][0]++;
        cov_1qjkhz1wn4().s[173]++;
        return existingCustomers.data[0];
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[30][1]++;
      }
      // Create new customer
      const customer =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[174]++, await this.stripe.customers.create({
        email: customerInfo.email,
        phone: customerInfo.phone,
        address: customerInfo.address ?
        /* istanbul ignore next */
        (cov_1qjkhz1wn4().b[31][0]++, {
          line1: customerInfo.address.line1,
          line2: customerInfo.address.line2,
          city: customerInfo.address.city,
          state: customerInfo.address.state,
          postal_code: customerInfo.address.postalCode,
          country: customerInfo.address.country
        }) :
        /* istanbul ignore next */
        (cov_1qjkhz1wn4().b[31][1]++, undefined)
      }));
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[175]++;
      return customer;
    } catch (error) {
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[176]++;
      logger_1.logger.error('Error creating Stripe customer:', error);
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[177]++;
      throw error;
    }
  }
  // Create transaction record
  async createTransactionRecord(request, fraudResult) {
    /* istanbul ignore next */
    cov_1qjkhz1wn4().f[19]++;
    cov_1qjkhz1wn4().s[178]++;
    try {
      const transaction =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[179]++, new Transaction_1.Transaction({
        userId: request.userId,
        type: 'recharge',
        amount: request.amount,
        currency: request.currency,
        status: 'pending',
        paymentMethod: request.paymentMethod,
        metadata: {
          fraudScore: fraudResult.score,
          fraudFlags: fraudResult.flags,
          fraudRisk: fraudResult.risk,
          ...request.metadata
        }
      }));
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[180]++;
      await transaction.save();
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[181]++;
      return transaction;
    } catch (error) {
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[182]++;
      logger_1.logger.error('Error creating transaction record:', error);
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[183]++;
      throw error;
    }
  }
  // Get supported currencies
  getSupportedCurrencies() {
    /* istanbul ignore next */
    cov_1qjkhz1wn4().f[20]++;
    cov_1qjkhz1wn4().s[184]++;
    return Array.from(this.supportedCurrencies.values());
  }
  // Get exchange rates
  async getExchangeRates() {
    /* istanbul ignore next */
    cov_1qjkhz1wn4().f[21]++;
    cov_1qjkhz1wn4().s[185]++;
    try {
      // In production, this would fetch from a real exchange rate API
      const rates =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[186]++, new Map());
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[187]++;
      for (const [code, config] of this.supportedCurrencies) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().s[188]++;
        rates.set(code, config.exchangeRate);
      }
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[189]++;
      return rates;
    } catch (error) {
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[190]++;
      logger_1.logger.error('Error getting exchange rates:', error);
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[191]++;
      return new Map();
    }
  }
  // Convert currency
  convertCurrency(amount, fromCurrency, toCurrency) {
    /* istanbul ignore next */
    cov_1qjkhz1wn4().f[22]++;
    cov_1qjkhz1wn4().s[192]++;
    try {
      const fromConfig =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[193]++, this.supportedCurrencies.get(fromCurrency));
      const toConfig =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[194]++, this.supportedCurrencies.get(toCurrency));
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[195]++;
      if (
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().b[33][0]++, !fromConfig) ||
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().b[33][1]++, !toConfig)) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[32][0]++;
        cov_1qjkhz1wn4().s[196]++;
        throw new Error('Unsupported currency');
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[32][1]++;
      }
      // Convert to base currency (USD) first
      const amountInBaseCurrency =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[197]++, amount * fromConfig.exchangeRate);
      // Convert to target currency
      const convertedAmount =
      /* istanbul ignore next */
      (cov_1qjkhz1wn4().s[198]++, amountInBaseCurrency / toConfig.exchangeRate);
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[199]++;
      return Math.round(convertedAmount * Math.pow(10, toConfig.decimals)) / Math.pow(10, toConfig.decimals);
    } catch (error) {
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[200]++;
      logger_1.logger.error('Error converting currency:', error);
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[201]++;
      throw error;
    }
  }
}
