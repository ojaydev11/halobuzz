      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[15][1]++;
      }
      cov_1qjkhz1wn4().s[113]++;
      if (hourlyAmount > rules.maxAmountPerHour) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[16][0]++;
        cov_1qjkhz1wn4().s[114]++;
        score += 0.3;
        /* istanbul ignore next */
        cov_1qjkhz1wn4().s[115]++;
        flags.push('high_hourly_amount');
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[16][1]++;
      }
      // Check daily limits
      cov_1qjkhz1wn4().s[116]++;
      if (recentTransactions.length > rules.maxTransactionsPerDay) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[17][0]++;
        cov_1qjkhz1wn4().s[117]++;
        score += 0.2;
        /* istanbul ignore next */
        cov_1qjkhz1wn4().s[118]++;
        flags.push('high_daily_transaction_count');
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[17][1]++;
      }
      cov_1qjkhz1wn4().s[119]++;
      if (dailyAmount > rules.maxAmountPerDay) {
        /* istanbul ignore next */
        cov_1qjkhz1wn4().b[18][0]++;
        cov_1qjkhz1wn4().s[120]++;
        score += 0.2;
        /* istanbul ignore next */
        cov_1qjkhz1wn4().s[121]++;
        flags.push('high_daily_amount');
      } else
      /* istanbul ignore next */
      {
        cov_1qjkhz1wn4().b[18][1]++;
      }
      cov_1qjkhz1wn4().s[122]++;
      return {
        score,
        flags
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[123]++;
      logger_1.logger.error('Error checking velocity:', error);
      /* istanbul ignore next */
      cov_1qjkhz1wn4().s[124]++;
      return {
        score: 0.1,
        flags: ['velocity_check_error']
      };
    }
  }
