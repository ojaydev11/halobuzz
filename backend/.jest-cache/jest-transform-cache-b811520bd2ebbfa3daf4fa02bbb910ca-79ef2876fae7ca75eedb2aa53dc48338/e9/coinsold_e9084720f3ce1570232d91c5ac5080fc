router.post('/daily-reward', securityMiddleware.validateToken, async (req, res) => {
  /* istanbul ignore next */
  cov_2a6eme63y2().f[11]++;
  cov_2a6eme63y2().s[133]++;
  try {
    const userId =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[134]++, req.securityContext?.userId);
    /* istanbul ignore next */
    cov_2a6eme63y2().s[135]++;
    if (!userId) {
      /* istanbul ignore next */
      cov_2a6eme63y2().b[35][0]++;
      cov_2a6eme63y2().s[136]++;
      return res.status(401).json({
        success: false,
        error: 'User not authenticated'
      });
    } else
    /* istanbul ignore next */
    {
      cov_2a6eme63y2().b[35][1]++;
    }
    // Check if user already claimed today's reward
    const today =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[137]++, new Date().toDateString());
    const todayTransactions =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[138]++, Array.from(transactions.values()).filter(tx => {
      /* istanbul ignore next */
      cov_2a6eme63y2().f[12]++;
      cov_2a6eme63y2().s[139]++;
      return /* istanbul ignore next */(cov_2a6eme63y2().b[36][0]++, tx.userId === userId) &&
      /* istanbul ignore next */
      (cov_2a6eme63y2().b[36][1]++, tx.type === TransactionType.DAILY_REWARD) &&
      /* istanbul ignore next */
      (cov_2a6eme63y2().b[36][2]++, tx.timestamp.toDateString() === today);
    }));
    /* istanbul ignore next */
    cov_2a6eme63y2().s[140]++;
    if (todayTransactions.length > 0) {
      /* istanbul ignore next */
      cov_2a6eme63y2().b[37][0]++;
      cov_2a6eme63y2().s[141]++;
      return res.status(400).json({
        success: false,
        error: 'Daily reward already claimed today',
        nextAvailable: new Date(Date.now() + 24 * 60 * 60 * 1000).toISOString()
      });
    } else
    /* istanbul ignore next */
    {
      cov_2a6eme63y2().b[37][1]++;
    }
    // Calculate daily reward amount (base amount + streak bonus)
    const baseReward =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[142]++, 50);
    const streakDays =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[143]++, Math.min(7, todayTransactions.length + 1)); // Max 7 days streak
    const streakBonus =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[144]++, (streakDays - 1) * 10); // 10 coins per day of streak
    const totalReward =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[145]++, baseReward + streakBonus);
    // Add coins
    const addResult =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[146]++, await addCoins(userId, totalReward, TransactionType.DAILY_REWARD, `Daily reward (${streakDays} day streak)`, {
      streakDays
    }));
    /* istanbul ignore next */
    cov_2a6eme63y2().s[147]++;
    res.json({
      success: true,
      reward: {
        amount: totalReward,
        baseAmount: baseReward,
        streakBonus: streakBonus,
        streakDays: streakDays,
        description: `Daily reward (${streakDays} day streak)`
      },
      newBalance: addResult.newBalance
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2a6eme63y2().s[148]++;
    logger_1.logger.error('Failed to claim daily reward:', error);
    /* istanbul ignore next */
    cov_2a6eme63y2().s[149]++;
    res.status(500).json({
      success: false,
      error: 'Failed to claim daily reward'
    });
  }
});
// Helper function to add coins
async function addCoins(userId, amount, type, description, metadata =
/* istanbul ignore next */
(cov_2a6eme63y2().b[38][0]++, {})) {
  /* istanbul ignore next */
  cov_2a6eme63y2().f[13]++;
  let balance =
  /* istanbul ignore next */
  (cov_2a6eme63y2().s[150]++, userBalances.get(userId));
  /* istanbul ignore next */
  cov_2a6eme63y2().s[151]++;
  if (!balance) {
    /* istanbul ignore next */
    cov_2a6eme63y2().b[39][0]++;
    cov_2a6eme63y2().s[152]++;
    balance = {
      userId,
      totalCoins: 0,
      bonusCoins: 0,
      lockedCoins: 0,
      totalEarned: 0,
      totalSpent: 0,
      lastUpdated: new Date()
    };
  } else
  /* istanbul ignore next */
  {
    cov_2a6eme63y2().b[39][1]++;
  }
  const transactionId =
  /* istanbul ignore next */
  (cov_2a6eme63y2().s[153]++, `tx_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`);
  const transaction =
  /* istanbul ignore next */
  (cov_2a6eme63y2().s[154]++, {
    id: transactionId,
    userId,
    type,
    amount: Math.abs(amount),
    balance: balance.totalCoins + Math.abs(amount),
    description,
    metadata,
    timestamp: new Date(),
    isProcessed: true
  });
  /* istanbul ignore next */
  cov_2a6eme63y2().s[155]++;
  balance.totalCoins += Math.abs(amount);
  /* istanbul ignore next */
  cov_2a6eme63y2().s[156]++;
  balance.totalEarned += Math.abs(amount);
  /* istanbul ignore next */
  cov_2a6eme63y2().s[157]++;
  balance.lastUpdated = new Date();
  /* istanbul ignore next */
  cov_2a6eme63y2().s[158]++;
  transactions.set(transactionId, transaction);
  /* istanbul ignore next */
  cov_2a6eme63y2().s[159]++;
  userBalances.set(userId, balance);
  /* istanbul ignore next */
  cov_2a6eme63y2().s[160]++;
  return {
    newBalance: balance.totalCoins,
    transaction
  };
}
