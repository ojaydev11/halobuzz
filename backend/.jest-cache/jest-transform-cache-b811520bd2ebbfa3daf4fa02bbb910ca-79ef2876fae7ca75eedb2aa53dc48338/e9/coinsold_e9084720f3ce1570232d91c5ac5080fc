    cov_2a6eme63y2().s[106]++;
    if (userId === recipientId) {
      /* istanbul ignore next */
      cov_2a6eme63y2().b[30][0]++;
      cov_2a6eme63y2().s[107]++;
      return res.status(400).json({
        success: false,
        error: 'Cannot transfer coins to yourself'
      });
    } else
    /* istanbul ignore next */
    {
      cov_2a6eme63y2().b[30][1]++;
    }
    // Get sender balance
    const senderBalance =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[108]++, userBalances.get(userId));
    /* istanbul ignore next */
    cov_2a6eme63y2().s[109]++;
    if (!senderBalance) {
      /* istanbul ignore next */
      cov_2a6eme63y2().b[31][0]++;
      cov_2a6eme63y2().s[110]++;
      return res.status(400).json({
        success: false,
        error: 'Sender balance not found'
      });
    } else
    /* istanbul ignore next */
    {
      cov_2a6eme63y2().b[31][1]++;
    }
    // Check if sender has enough coins
    cov_2a6eme63y2().s[111]++;
    if (senderBalance.totalCoins < amount) {
      /* istanbul ignore next */
      cov_2a6eme63y2().b[32][0]++;
      cov_2a6eme63y2().s[112]++;
      return res.status(400).json({
        success: false,
        error: 'Insufficient coins for transfer',
        required: amount,
        available: senderBalance.totalCoins
      });
    } else
    /* istanbul ignore next */
    {
      cov_2a6eme63y2().b[32][1]++;
    }
    // Get or create recipient balance
    let recipientBalance =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[113]++, userBalances.get(recipientId));
    /* istanbul ignore next */
    cov_2a6eme63y2().s[114]++;
