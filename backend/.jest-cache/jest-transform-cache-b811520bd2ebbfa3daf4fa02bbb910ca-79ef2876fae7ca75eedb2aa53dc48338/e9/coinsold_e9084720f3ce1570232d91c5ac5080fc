router.post('/transfer', securityMiddleware.validateToken, async (req, res) => {
  /* istanbul ignore next */
  cov_2a6eme63y2().f[10]++;
  cov_2a6eme63y2().s[99]++;
  try {
    const userId =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[100]++, req.securityContext?.userId);
    const {
      recipientId,
      amount,
      description
    } =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[101]++, req.body);
    /* istanbul ignore next */
    cov_2a6eme63y2().s[102]++;
    if (!userId) {
      /* istanbul ignore next */
      cov_2a6eme63y2().b[27][0]++;
      cov_2a6eme63y2().s[103]++;
      return res.status(401).json({
        success: false,
        error: 'User not authenticated'
      });
    } else
    /* istanbul ignore next */
    {
      cov_2a6eme63y2().b[27][1]++;
    }
    cov_2a6eme63y2().s[104]++;
    if (
    /* istanbul ignore next */
    (cov_2a6eme63y2().b[29][0]++, !recipientId) ||
    /* istanbul ignore next */
    (cov_2a6eme63y2().b[29][1]++, !amount) ||
    /* istanbul ignore next */
    (cov_2a6eme63y2().b[29][2]++, amount <= 0)) {
      /* istanbul ignore next */
