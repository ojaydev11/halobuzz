      /* istanbul ignore next */
      cov_2a6eme63y2().b[33][0]++;
      cov_2a6eme63y2().s[115]++;
      recipientBalance = {
        userId: recipientId,
        totalCoins: 0,
        bonusCoins: 0,
        lockedCoins: 0,
        totalEarned: 0,
        totalSpent: 0,
        lastUpdated: new Date()
      };
    } else
    /* istanbul ignore next */
    {
      cov_2a6eme63y2().b[33][1]++;
    }
    // Create transactions
    const transferId =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[116]++, `transfer_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`);
    // Sender transaction (negative)
    const senderTransaction =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[117]++, {
      id: `${transferId}_sender`,
      userId,
      type: TransactionType.TRANSFER,
      amount: -amount,
      balance: senderBalance.totalCoins - amount,
      description: `Transferred ${amount} coins to user ${recipientId}`,
      metadata: {
        recipientId
      },
      timestamp: new Date(),
      isProcessed: true
    });
    // Recipient transaction (positive)
    const recipientTransaction =
    /* istanbul ignore next */
    (cov_2a6eme63y2().s[118]++, {
      id: `${transferId}_recipient`,
      userId: recipientId,
      type: TransactionType.TRANSFER,
      amount: amount,
      balance: recipientBalance.totalCoins + amount,
      description: `Received ${amount} coins from user ${userId}`,
      metadata: {
        senderId: userId
      },
      timestamp: new Date(),
      isProcessed: true
    });
    // Update balances
    /* istanbul ignore next */
    cov_2a6eme63y2().s[119]++;
    senderBalance.totalCoins -= amount;
    /* istanbul ignore next */
    cov_2a6eme63y2().s[120]++;
    senderBalance.totalSpent += amount;
    /* istanbul ignore next */
    cov_2a6eme63y2().s[121]++;
    senderBalance.lastUpdated = new Date();
    /* istanbul ignore next */
    cov_2a6eme63y2().s[122]++;
    recipientBalance.totalCoins += amount;
    /* istanbul ignore next */
    cov_2a6eme63y2().s[123]++;
    recipientBalance.totalEarned += amount;
    /* istanbul ignore next */
    cov_2a6eme63y2().s[124]++;
    recipientBalance.lastUpdated = new Date();
    // Store transactions and balances
    /* istanbul ignore next */
    cov_2a6eme63y2().s[125]++;
    transactions.set(senderTransaction.id, senderTransaction);
    /* istanbul ignore next */
    cov_2a6eme63y2().s[126]++;
    transactions.set(recipientTransaction.id, recipientTransaction);
    /* istanbul ignore next */
    cov_2a6eme63y2().s[127]++;
    userBalances.set(userId, senderBalance);
    /* istanbul ignore next */
    cov_2a6eme63y2().s[128]++;
    userBalances.set(recipientId, recipientBalance);
    /* istanbul ignore next */
    cov_2a6eme63y2().s[129]++;
    res.json({
      success: true,
      transfer: {
        id: transferId,
        amount,
        recipientId,
        description:
        /* istanbul ignore next */
        (cov_2a6eme63y2().b[34][0]++, description) ||
        /* istanbul ignore next */
        (cov_2a6eme63y2().b[34][1]++, `Transferred ${amount} coins`),
        timestamp: new Date()
      },
      newBalance: senderBalance.totalCoins
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2a6eme63y2().s[130]++;
    logger_1.logger.error('Failed to transfer coins:', error);
    /* istanbul ignore next */
    cov_2a6eme63y2().s[131]++;
    res.status(500).json({
      success: false,
      error: 'Failed to transfer coins'
    });
  }
