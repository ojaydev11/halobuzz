  /**
   * Calculate reputation points based on action and metadata
   */
  calculateReputationPoints(action, metadata) {
    /* istanbul ignore next */
    cov_1q0p53hvbk().f[10]++;
    const pointValues =
    /* istanbul ignore next */
    (cov_1q0p53hvbk().s[117]++, {
      // Positive actions
      'stream_start': 5,
      'stream_complete': 10,
      'gift_received': 2,
      'gift_sent': 1,
      'follow_received': 3,
      'follow_sent': 1,
      'comment_received': 1,
      'like_received': 0.5,
      'share_received': 2,
      'kyc_verified': 20,
      'phone_verified': 10,
      'email_verified': 5,
      'social_connected': 5,
      // Negative actions
      'stream_report': -10,
      'content_report': -5,
      'spam_detected': -15,
      'fraud_detected': -50,
      'account_suspended': -100,
      'payment_chargeback': -25,
      'fake_followers': -20,
      'bot_activity': -30
    });
    let basePoints =
    /* istanbul ignore next */
    (cov_1q0p53hvbk().s[118]++,
    /* istanbul ignore next */
    (cov_1q0p53hvbk().b[39][0]++, pointValues[action]) ||
    /* istanbul ignore next */
    (cov_1q0p53hvbk().b[39][1]++, 0));
    // Apply multipliers based on metadata
    /* istanbul ignore next */
    cov_1q0p53hvbk().s[119]++;
    if (metadata?.multiplier) {
      /* istanbul ignore next */
      cov_1q0p53hvbk().b[40][0]++;
      cov_1q0p53hvbk().s[120]++;
      basePoints *= metadata.multiplier;
    } else
    /* istanbul ignore next */
    {
      cov_1q0p53hvbk().b[40][1]++;
    }
    // Apply time-based bonuses
    cov_1q0p53hvbk().s[121]++;
    if (metadata?.isFirstTime) {
      /* istanbul ignore next */
      cov_1q0p53hvbk().b[41][0]++;
      cov_1q0p53hvbk().s[122]++;
      basePoints *= 1.5;
    } else
    /* istanbul ignore next */
    {
      cov_1q0p53hvbk().b[41][1]++;
    }
    // Apply streak bonuses
    cov_1q0p53hvbk().s[123]++;
    if (
    /* istanbul ignore next */
    (cov_1q0p53hvbk().b[43][0]++, metadata?.streakDays) &&
    /* istanbul ignore next */
    (cov_1q0p53hvbk().b[43][1]++, metadata.streakDays > 7)) {
      /* istanbul ignore next */
      cov_1q0p53hvbk().b[42][0]++;
      cov_1q0p53hvbk().s[124]++;
      basePoints *= 1.2;
    } else
    /* istanbul ignore next */
    {
      cov_1q0p53hvbk().b[42][1]++;
    }
    cov_1q0p53hvbk().s[125]++;
    return Math.round(basePoints);
  }
}
