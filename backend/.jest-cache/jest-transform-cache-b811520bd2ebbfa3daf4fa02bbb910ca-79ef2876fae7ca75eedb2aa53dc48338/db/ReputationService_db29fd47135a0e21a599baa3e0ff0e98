  async updateUserTrustScore(userId) {
    /* istanbul ignore next */
    cov_1q0p53hvbk().f[3]++;
    cov_1q0p53hvbk().s[65]++;
    try {
      // Get total reputation
      const reputationResult =
      /* istanbul ignore next */
      (cov_1q0p53hvbk().s[66]++, await ReputationEvent_1.ReputationEvent.aggregate([{
        $match: {
          userId
        }
      }, {
        $group: {
          _id: null,
          totalReputation: {
            $sum: '$points'
          }
        }
      }]));
      const totalReputation =
      /* istanbul ignore next */
      (cov_1q0p53hvbk().s[67]++,
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[25][0]++, reputationResult[0]?.totalReputation) ||
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[25][1]++, 0));
      // Get user's reputation events summary
      const summary =
      /* istanbul ignore next */
      (cov_1q0p53hvbk().s[68]++, await ReputationEvent_1.ReputationEvent.aggregate([{
        $match: {
          userId
        }
      }, {
        $group: {
          _id: '$type',
          count: {
            $sum: 1
          },
          totalPoints: {
            $sum: '$points'
          }
        }
      }]));
      // Calculate trust factors
      const factors =
      /* istanbul ignore next */
      (cov_1q0p53hvbk().s[69]++, await this.calculateTrustFactors(userId, summary));
      // Calculate trust score (0-100)
      const trustScore =
      /* istanbul ignore next */
      (cov_1q0p53hvbk().s[70]++, Math.max(0, Math.min(100, totalReputation / 10)));
      // Apply trust level
      let trustLevel =
      /* istanbul ignore next */
      (cov_1q0p53hvbk().s[71]++, 'low');
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[72]++;
      if (trustScore >= 80) {
        /* istanbul ignore next */
        cov_1q0p53hvbk().b[26][0]++;
        cov_1q0p53hvbk().s[73]++;
        trustLevel = 'verified';
      } else {
        /* istanbul ignore next */
        cov_1q0p53hvbk().b[26][1]++;
        cov_1q0p53hvbk().s[74]++;
        if (trustScore >= 60) {
          /* istanbul ignore next */
          cov_1q0p53hvbk().b[27][0]++;
          cov_1q0p53hvbk().s[75]++;
          trustLevel = 'high';
        } else {
          /* istanbul ignore next */
          cov_1q0p53hvbk().b[27][1]++;
          cov_1q0p53hvbk().s[76]++;
          if (trustScore >= 30) {
            /* istanbul ignore next */
            cov_1q0p53hvbk().b[28][0]++;
            cov_1q0p53hvbk().s[77]++;
            trustLevel = 'medium';
          } else
          /* istanbul ignore next */
          {
            cov_1q0p53hvbk().b[28][1]++;
          }
        }
      }
      // Update user
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[78]++;
      await User_1.User.findByIdAndUpdate(userId, {
        'trust.score': trustScore,
        'trust.level': trustLevel,
        'trust.factors': factors
      });
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[79]++;
      logger.info(`Trust score updated for user ${userId}: ${trustScore} (${trustLevel})`);
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[80]++;
      return {
        success: true,
        trustScore,
        trustLevel
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[81]++;
      logger.error('Failed to update trust score:', error);
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[82]++;
      return {
        success: false,
        error: error.message
      };
    }
  }
  async calculateTrustFactors(userId, summary) {
    /* istanbul ignore next */
    cov_1q0p53hvbk().f[4]++;
    const user =
    /* istanbul ignore next */
    (cov_1q0p53hvbk().s[83]++, await User_1.User.findById(userId));
    /* istanbul ignore next */
    cov_1q0p53hvbk().s[84]++;
    if (!user) {
      /* istanbul ignore next */
      cov_1q0p53hvbk().b[29][0]++;
      cov_1q0p53hvbk().s[85]++;
      return {};
    } else
    /* istanbul ignore next */
    {
      cov_1q0p53hvbk().b[29][1]++;
    }
    const factors =
    /* istanbul ignore next */
    (cov_1q0p53hvbk().s[86]++, {
      kycVerified: user.kycStatus === 'verified',
      phoneVerified: !!user.phone,
      emailVerified: !!user.email,
      socialConnected: !!(
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[30][0]++, user.socialLogin?.google) ||
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[30][1]++, user.socialLogin?.facebook) ||
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[30][2]++, user.socialLogin?.apple)),
      activeDays: Math.floor((Date.now() - user.createdAt.getTime()) / (1000 * 60 * 60 * 24)),
      totalStreams:
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[31][0]++, user.trust?.factors?.totalStreams) ||
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[31][1]++, 0),
      totalGifts:
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[32][0]++, user.trust?.factors?.totalGifts) ||
      /* istanbul ignore next */
      (cov_1q0p53hvbk().b[32][1]++, 0),
      reportCount: 0
    });
    // Get report count from summary
    const reportEvent =
    /* istanbul ignore next */
    (cov_1q0p53hvbk().s[87]++, summary.find(s => {
      /* istanbul ignore next */
      cov_1q0p53hvbk().f[5]++;
      cov_1q0p53hvbk().s[88]++;
      return s._id === 'report_received';
    }));
    /* istanbul ignore next */
    cov_1q0p53hvbk().s[89]++;
    if (reportEvent) {
      /* istanbul ignore next */
      cov_1q0p53hvbk().b[33][0]++;
      cov_1q0p53hvbk().s[90]++;
      factors.reportCount = reportEvent.count;
    } else
    /* istanbul ignore next */
    {
      cov_1q0p53hvbk().b[33][1]++;
    }
    cov_1q0p53hvbk().s[91]++;
    return factors;
  }
  async getUserReputation(userId) {
    /* istanbul ignore next */
    cov_1q0p53hvbk().f[6]++;
    cov_1q0p53hvbk().s[92]++;
    try {
      const [totalReputation, summary, recentEvents] =
      /* istanbul ignore next */
      (cov_1q0p53hvbk().s[93]++, await Promise.all([ReputationEvent_1.ReputationEvent.aggregate([{
        $match: {
          userId
        }
      }, {
        $group: {
          _id: null,
          totalReputation: {
            $sum: '$points'
          }
        }
      }]), ReputationEvent_1.ReputationEvent.aggregate([{
        $match: {
          userId
        }
      }, {
        $group: {
          _id: '$type',
          count: {
            $sum: 1
          },
          totalPoints: {
            $sum: '$points'
          }
        }
      }]), ReputationEvent_1.ReputationEvent.find({
        userId
      }).sort({
        createdAt: -1
      }).limit(10)]));
      const user =
      /* istanbul ignore next */
      (cov_1q0p53hvbk().s[94]++, await User_1.User.findById(userId));
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[95]++;
      if (!user) {
        /* istanbul ignore next */
        cov_1q0p53hvbk().b[34][0]++;
        cov_1q0p53hvbk().s[96]++;
        return {
          success: false,
          error: 'User not found'
        };
      } else
      /* istanbul ignore next */
      {
        cov_1q0p53hvbk().b[34][1]++;
      }
      cov_1q0p53hvbk().s[97]++;
      return {
        success: true,
        data: {
          totalReputation:
          /* istanbul ignore next */
          (cov_1q0p53hvbk().b[35][0]++, totalReputation[0]?.totalReputation) ||
          /* istanbul ignore next */
          (cov_1q0p53hvbk().b[35][1]++, 0),
          trustScore:
          /* istanbul ignore next */
          (cov_1q0p53hvbk().b[36][0]++, user.trust?.score) ||
          /* istanbul ignore next */
          (cov_1q0p53hvbk().b[36][1]++, 0),
          trustLevel:
          /* istanbul ignore next */
          (cov_1q0p53hvbk().b[37][0]++, user.trust?.level) ||
          /* istanbul ignore next */
          (cov_1q0p53hvbk().b[37][1]++, 'low'),
          summary,
          recentEvents
        }
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[98]++;
      logger.error('Failed to get user reputation:', error);
      /* istanbul ignore next */
      cov_1q0p53hvbk().s[99]++;
      return {
        success: false,
        error: error.message
      };
    }
  }
