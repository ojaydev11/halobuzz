  /**
   * Join group buy
   */
  async joinGroupBuy(groupBuyId, userId, quantity) {
    /* istanbul ignore next */
    cov_2nvd6e5zhd().f[6]++;
    cov_2nvd6e5zhd().s[90]++;
    try {
      const groupBuy =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[91]++, await GroupBuyModel.findOne({
        id: groupBuyId
      }));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[92]++;
      if (!groupBuy) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[17][0]++;
        cov_2nvd6e5zhd().s[93]++;
        throw new Error(`Group buy ${groupBuyId} not found`);
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[17][1]++;
      }
      cov_2nvd6e5zhd().s[94]++;
      if (groupBuy.status !== 'active') {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[18][0]++;
        cov_2nvd6e5zhd().s[95]++;
        throw new Error(`Group buy ${groupBuyId} is not active`);
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[18][1]++;
      }
      cov_2nvd6e5zhd().s[96]++;
      if (new Date() > groupBuy.expiresAt) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[19][0]++;
        cov_2nvd6e5zhd().s[97]++;
        groupBuy.status = 'expired';
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[98]++;
        await groupBuy.save();
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[99]++;
        throw new Error(`Group buy ${groupBuyId} has expired`);
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[19][1]++;
      }
      // Check if user already joined
      const existingParticipant =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[100]++, groupBuy.participants.find(p => {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().f[7]++;
        cov_2nvd6e5zhd().s[101]++;
        return p.userId.toString() === userId;
      }));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[102]++;
      if (existingParticipant) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[20][0]++;
        cov_2nvd6e5zhd().s[103]++;
        existingParticipant.quantity += quantity;
      } else {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[20][1]++;
        cov_2nvd6e5zhd().s[104]++;
        groupBuy.participants.push({
          userId,
          quantity,
          joinedAt: new Date()
        });
      }
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[105]++;
      groupBuy.currentQuantity += quantity;
      // Update discount based on current quantity
      const currentDiscount =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[106]++, this.calculateGroupBuyDiscount(groupBuy.currentQuantity, groupBuy.discountTiers));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[107]++;
      groupBuy.currentDiscount = currentDiscount;
      // Calculate savings
      const product =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[108]++, await ProductModel.findById(groupBuy.productId));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[109]++;
      if (product) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[21][0]++;
        const savingsPerUnit =
        /* istanbul ignore next */
        (cov_2nvd6e5zhd().s[110]++, product.price * (currentDiscount / 100));
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[111]++;
        groupBuy.savings = savingsPerUnit * groupBuy.currentQuantity;
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[21][1]++;
      }
      cov_2nvd6e5zhd().s[112]++;
      await groupBuy.save();
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[113]++;
      this.activeGroupBuys.set(groupBuyId, this.mapToGroupBuy(groupBuy));
      // Check if target reached
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[114]++;
      if (groupBuy.currentQuantity >= groupBuy.targetQuantity) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[22][0]++;
        cov_2nvd6e5zhd().s[115]++;
        await this.completeGroupBuy(groupBuyId);
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[22][1]++;
      }
      cov_2nvd6e5zhd().s[116]++;
      logger_1.logger.info('User joined group buy', {
        groupBuyId,
        userId,
        quantity,
        currentQuantity: groupBuy.currentQuantity,
        currentDiscount: groupBuy.currentDiscount
      });
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[117]++;
      return true;
    } catch (error) {
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[118]++;
      logger_1.logger.error('Error joining group buy:', error);
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[119]++;
      throw error;
    }
  }
