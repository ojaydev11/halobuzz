  calculateGroupBuyDiscount(currentQuantity, discountTiers) {
    /* istanbul ignore next */
    cov_2nvd6e5zhd().f[22]++;
    let discount =
    /* istanbul ignore next */
    (cov_2nvd6e5zhd().s[194]++, 0);
    /* istanbul ignore next */
    cov_2nvd6e5zhd().s[195]++;
    for (const tier of discountTiers.sort((a, b) => {
      /* istanbul ignore next */
      cov_2nvd6e5zhd().f[23]++;
      cov_2nvd6e5zhd().s[196]++;
      return b.quantity - a.quantity;
    })) {
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[197]++;
      if (currentQuantity >= tier.quantity) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[32][0]++;
        cov_2nvd6e5zhd().s[198]++;
        discount = tier.discountPercentage;
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[199]++;
        break;
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[32][1]++;
      }
    }
    /* istanbul ignore next */
    cov_2nvd6e5zhd().s[200]++;
    return discount;
  }
  async completeGroupBuy(groupBuyId) {
    /* istanbul ignore next */
    cov_2nvd6e5zhd().f[24]++;
    cov_2nvd6e5zhd().s[201]++;
    try {
      const groupBuy =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[202]++, await GroupBuyModel.findOne({
        id: groupBuyId
      }));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[203]++;
      if (!groupBuy) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[33][0]++;
        cov_2nvd6e5zhd().s[204]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[33][1]++;
      }
      cov_2nvd6e5zhd().s[205]++;
      groupBuy.status = 'completed';
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[206]++;
      await groupBuy.save();
      // Process all participants' orders
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[207]++;
      for (const participant of groupBuy.participants) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[208]++;
        // Create checkout for each participant
        // This would integrate with the checkout system
        logger_1.logger.info('Processing group buy order', {
          groupBuyId,
          userId: participant.userId,
          quantity: participant.quantity
        });
      }
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[209]++;
      this.activeGroupBuys.delete(groupBuyId);
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[210]++;
      logger_1.logger.info('Group buy completed', {
        groupBuyId,
        totalParticipants: groupBuy.participants.length,
        totalQuantity: groupBuy.currentQuantity,
        finalDiscount: groupBuy.currentDiscount
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[211]++;
      logger_1.logger.error('Error completing group buy:', error);
    }
  }
  initializeSessionMonitoring() {
    /* istanbul ignore next */
    cov_2nvd6e5zhd().f[25]++;
    cov_2nvd6e5zhd().s[212]++;
    // Monitor active sessions every 5 minutes
    setInterval(async () => {
      /* istanbul ignore next */
      cov_2nvd6e5zhd().f[26]++;
      cov_2nvd6e5zhd().s[213]++;
      try {
        const now =
        /* istanbul ignore next */
        (cov_2nvd6e5zhd().s[214]++, new Date());
        const expiredSessions =
        /* istanbul ignore next */
        (cov_2nvd6e5zhd().s[215]++, await LiveShoppingSessionModel.find({
          isActive: true,
          endTime: {
            $lte: now
          }
        }));
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[216]++;
        for (const session of expiredSessions) {
          /* istanbul ignore next */
          cov_2nvd6e5zhd().s[217]++;
          session.isActive = false;
          /* istanbul ignore next */
          cov_2nvd6e5zhd().s[218]++;
          await session.save();
          /* istanbul ignore next */
          cov_2nvd6e5zhd().s[219]++;
          this.activeSessions.delete(session.streamId.toString());
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[220]++;
        logger_1.logger.error('Error monitoring sessions:', error);
      }
    }, 300000); // Check every 5 minutes
  }
