  }
  /**
   * Add products to live streams
   */
  async addProductToStream(streamId, productId) {
    /* istanbul ignore next */
    cov_2nvd6e5zhd().f[3]++;
    cov_2nvd6e5zhd().s[23]++;
    try {
      const product =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[24]++, await ProductModel.findOne({
        id: productId
      }));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[25]++;
      if (!product) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[4][0]++;
        cov_2nvd6e5zhd().s[26]++;
        throw new Error(`Product ${productId} not found`);
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[4][1]++;
      }
      // Get or create live shopping session
      let session =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[27]++, await LiveShoppingSessionModel.findOne({
        streamId,
        isActive: true
      }));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[28]++;
      if (!session) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[5][0]++;
        const sessionId =
        /* istanbul ignore next */
        (cov_2nvd6e5zhd().s[29]++, this.generateSessionId());
        const stream =
        /* istanbul ignore next */
        (cov_2nvd6e5zhd().s[30]++, await mongoose_1.default.model('LiveStream').findById(streamId));
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[31]++;
        if (!stream) {
          /* istanbul ignore next */
          cov_2nvd6e5zhd().b[6][0]++;
          cov_2nvd6e5zhd().s[32]++;
          throw new Error(`Stream ${streamId} not found`);
        } else
        /* istanbul ignore next */
        {
          cov_2nvd6e5zhd().b[6][1]++;
        }
        cov_2nvd6e5zhd().s[33]++;
        session = new LiveShoppingSessionModel({
          id: sessionId,
          streamId,
          creatorId: stream.hostId,
          products: [product._id],
          startTime: new Date(),
          isActive: true
        });
      } else {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[5][1]++;
        cov_2nvd6e5zhd().s[34]++;
        if (!session.products.includes(product._id)) {
          /* istanbul ignore next */
          cov_2nvd6e5zhd().b[7][0]++;
          cov_2nvd6e5zhd().s[35]++;
          session.products.push(product._id);
        } else
        /* istanbul ignore next */
        {
          cov_2nvd6e5zhd().b[7][1]++;
        }
      }
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[36]++;
      await session.save();
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[37]++;
      this.activeSessions.set(streamId, this.mapToLiveShoppingSession(session));
      // Update product stream association
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[38]++;
      product.streamId = streamId;
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[39]++;
      await ProductModel.findByIdAndUpdate(product._id, {
        streamId
      });
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[40]++;
      logger_1.logger.info('Product added to stream', {
        streamId,
        productId,
        sessionId: session.id
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[41]++;
      logger_1.logger.error('Error adding product to stream:', error);
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[42]++;
      throw error;
    }
  }
  /**
   * Handle instant checkout during streams
   */
  async processLiveCheckout(productId, buyerId, streamId, quantity =
  /* istanbul ignore next */
  (cov_2nvd6e5zhd().b[8][0]++, 1), paymentMethod) {
    /* istanbul ignore next */
    cov_2nvd6e5zhd().f[4]++;
    cov_2nvd6e5zhd().s[43]++;
    try {
      const product =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[44]++, await ProductModel.findOne({
        id: productId
      }));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[45]++;
      if (!product) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[9][0]++;
        cov_2nvd6e5zhd().s[46]++;
        throw new Error(`Product ${productId} not found`);
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[9][1]++;
      }
      cov_2nvd6e5zhd().s[47]++;
      if (!product.isActive) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[10][0]++;
        cov_2nvd6e5zhd().s[48]++;
        throw new Error(`Product ${productId} is not available`);
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[10][1]++;
      }
      cov_2nvd6e5zhd().s[49]++;
      if (product.inventory < quantity) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[11][0]++;
        cov_2nvd6e5zhd().s[50]++;
        throw new Error(`Insufficient inventory for product ${productId}`);
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[11][1]++;
      }
      // Calculate total amount
      const unitPrice =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[51]++,
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().b[13][0]++, product.discount) &&
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().b[13][1]++, product.discount.validUntil > new Date()) ?
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().b[12][0]++, product.price * (1 - product.discount.percentage / 100)) :
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().b[12][1]++, product.price));
      const totalAmount =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[52]++, unitPrice * quantity);
      // Create checkout record
      const checkoutId =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[53]++, this.generateCheckoutId());
      const orderNumber =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[54]++, this.generateOrderNumber());
      const checkout =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[55]++, new LiveCheckoutModel({
        id: checkoutId,
        userId: buyerId,
        productId: product._id,
        streamId,
        quantity,
        totalAmount,
        currency: product.currency,
        paymentMethod,
        status: 'pending',
        orderNumber,
        estimatedDelivery: new Date(Date.now() + product.shipping.estimatedDays * 24 * 60 * 60 * 1000)
      }));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[56]++;
      await checkout.save();
      // Process payment (mock implementation)
      const paymentSuccess =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[57]++, await this.processPayment(checkout, paymentMethod));
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[58]++;
      if (paymentSuccess) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[14][0]++;
        cov_2nvd6e5zhd().s[59]++;
        checkout.status = 'completed';
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[60]++;
        checkout.transactionId = this.generateTransactionId();
        // Update inventory
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[61]++;
        product.inventory -= quantity;
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[62]++;
        product.analytics.purchases += quantity;
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[63]++;
        product.analytics.revenue += totalAmount;
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[64]++;
        product.analytics.conversionRate = product.analytics.purchases / Math.max(1, product.analytics.views);
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[65]++;
        await product.save();
        // Update session stats
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[66]++;
        await this.updateSessionStats(streamId, totalAmount, quantity);
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[67]++;
        await checkout.save();
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[68]++;
        logger_1.logger.info('Live checkout completed', {
          checkoutId,
          productId,
          buyerId,
          streamId,
          totalAmount,
          quantity
        });
      } else {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[14][1]++;
        cov_2nvd6e5zhd().s[69]++;
        checkout.status = 'failed';
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[70]++;
        await checkout.save();
        /* istanbul ignore next */
        cov_2nvd6e5zhd().s[71]++;
        throw new Error('Payment processing failed');
      }
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[72]++;
      return this.mapToLiveCheckout(checkout);
    } catch (error) {
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[73]++;
      logger_1.logger.error('Error processing live checkout:', error);
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[74]++;
      throw error;
    }
  }
