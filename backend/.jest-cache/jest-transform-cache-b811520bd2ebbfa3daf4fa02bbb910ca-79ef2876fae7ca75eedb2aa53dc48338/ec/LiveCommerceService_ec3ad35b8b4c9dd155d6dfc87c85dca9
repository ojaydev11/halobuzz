  /**
   * Get commerce analytics
   */
  async getCommerceAnalytics(creatorId, timeframe =
  /* istanbul ignore next */
  (cov_2nvd6e5zhd().b[26][0]++, '30days')) {
    /* istanbul ignore next */
    cov_2nvd6e5zhd().f[10]++;
    cov_2nvd6e5zhd().s[138]++;
    try {
      const days =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[139]++, parseInt(timeframe.replace('days', '')));
      const startDate =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[140]++, new Date(Date.now() - days * 24 * 60 * 60 * 1000));
      // Get completed checkouts
      const query =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[141]++, {
        status: 'completed',
        createdAt: {
          $gte: startDate
        }
      });
      /* istanbul ignore next */
      cov_2nvd6e5zhd().s[142]++;
      if (creatorId) {
        /* istanbul ignore next */
        cov_2nvd6e5zhd().b[27][0]++;
        cov_2nvd6e5zhd().s[143]++;
        query.creatorId = creatorId;
      } else
      /* istanbul ignore next */
      {
        cov_2nvd6e5zhd().b[27][1]++;
      }
      const checkouts =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[144]++, await LiveCheckoutModel.find(query).populate('productId'));
      // Calculate analytics
      const totalSales =
      /* istanbul ignore next */
      (cov_2nvd6e5zhd().s[145]++, checkouts.reduce((sum, checkout) => {
        /* istanbul ignore next */
