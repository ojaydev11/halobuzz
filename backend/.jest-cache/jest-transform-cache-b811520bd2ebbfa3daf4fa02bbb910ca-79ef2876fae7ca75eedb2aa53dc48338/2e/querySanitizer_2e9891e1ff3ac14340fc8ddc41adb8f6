const sanitizeRequest = (req, res, next) => {
  /* istanbul ignore next */
  cov_1s1w3zrta4().f[8]++;
  cov_1s1w3zrta4().s[77]++;
  try {
    /* istanbul ignore next */
    cov_1s1w3zrta4().s[78]++;
    if (req.body) {
      /* istanbul ignore next */
      cov_1s1w3zrta4().b[33][0]++;
      cov_1s1w3zrta4().s[79]++;
      req.body = QuerySanitizer.sanitizeObject(req.body);
    } else
    /* istanbul ignore next */
    {
      cov_1s1w3zrta4().b[33][1]++;
    }
    cov_1s1w3zrta4().s[80]++;
    if (req.query) {
      /* istanbul ignore next */
      cov_1s1w3zrta4().b[34][0]++;
      cov_1s1w3zrta4().s[81]++;
      req.query = QuerySanitizer.sanitizeObject(req.query);
    } else
    /* istanbul ignore next */
    {
      cov_1s1w3zrta4().b[34][1]++;
    }
    cov_1s1w3zrta4().s[82]++;
    if (req.params) {
      /* istanbul ignore next */
      cov_1s1w3zrta4().b[35][0]++;
      cov_1s1w3zrta4().s[83]++;
      req.params = QuerySanitizer.sanitizeObject(req.params);
    } else
    /* istanbul ignore next */
    {
      cov_1s1w3zrta4().b[35][1]++;
    }
    cov_1s1w3zrta4().s[84]++;
    next();
  } catch (error) {
    /* istanbul ignore next */
    cov_1s1w3zrta4().s[85]++;
    logger_1.logger.error('Request sanitization failed:', error);
    /* istanbul ignore next */
    cov_1s1w3zrta4().s[86]++;
