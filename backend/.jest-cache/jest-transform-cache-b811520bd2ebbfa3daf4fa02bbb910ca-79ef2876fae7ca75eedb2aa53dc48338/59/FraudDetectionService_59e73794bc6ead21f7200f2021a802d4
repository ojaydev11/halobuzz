      // Store in fraud detection database
      /* istanbul ignore next */
      cov_12dy2qwfkw().s[128]++;
      await (0, redis_1.setCache)(`fraud_alert:${userId}:${Date.now()}`, {
        userId,
        amount,
        paymentMethod,
