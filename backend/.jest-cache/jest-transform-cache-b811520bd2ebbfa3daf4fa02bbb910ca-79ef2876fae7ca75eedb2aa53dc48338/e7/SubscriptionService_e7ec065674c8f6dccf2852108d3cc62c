  initializeBillingMonitoring() {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[36]++;
    cov_1l6x9xjyrc().s[189]++;
    // Monitor billing dates every hour
    setInterval(async () => {
      /* istanbul ignore next */
      cov_1l6x9xjyrc().f[37]++;
      cov_1l6x9xjyrc().s[190]++;
      try {
        const now =
        /* istanbul ignore next */
        (cov_1l6x9xjyrc().s[191]++, new Date());
        const dueSubscriptions =
        /* istanbul ignore next */
        (cov_1l6x9xjyrc().s[192]++, await SubscriptionModel.find({
          status: 'active',
          nextBillingDate: {
            $lte: now
          },
          autoRenew: true
        }));
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[193]++;
        for (const subscription of dueSubscriptions) {
          /* istanbul ignore next */
          cov_1l6x9xjyrc().s[194]++;
          await this.processBilling(subscription);
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[195]++;
        logger_1.default.error('Error monitoring billing:', error);
      }
    }, 3600000); // Check every hour
  }
  async processBilling(subscription) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[38]++;
    cov_1l6x9xjyrc().s[196]++;
    try {
      // Process payment
      const paymentSuccess =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[197]++, await this.processPayment(subscription, subscription.paymentMethod));
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[198]++;
      if (paymentSuccess) {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().b[29][0]++;
        // Extend subscription
        const newEndDate =
        /* istanbul ignore next */
        (cov_1l6x9xjyrc().s[199]++, new Date(subscription.endDate));
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[200]++;
        if (subscription.billingCycle === 'monthly') {
          /* istanbul ignore next */
          cov_1l6x9xjyrc().b[30][0]++;
          cov_1l6x9xjyrc().s[201]++;
          newEndDate.setMonth(newEndDate.getMonth() + 1);
        } else {
          /* istanbul ignore next */
          cov_1l6x9xjyrc().b[30][1]++;
          cov_1l6x9xjyrc().s[202]++;
          newEndDate.setFullYear(newEndDate.getFullYear() + 1);
        }
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[203]++;
        subscription.endDate = newEndDate;
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[204]++;
        subscription.nextBillingDate = newEndDate;
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[205]++;
        await subscription.save();
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[206]++;
        logger_1.default.info('Subscription renewed successfully', {
          subscriptionId: subscription.id,
          newEndDate
        });
      } else {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().b[29][1]++;
        cov_1l6x9xjyrc().s[207]++;
        // Payment failed - mark as expired
        subscription.status = 'expired';
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[208]++;
        await subscription.save();
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[209]++;
        logger_1.default.warn('Subscription payment failed', {
          subscriptionId: subscription.id
        });
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[210]++;
      logger_1.default.error('Error processing billing:', error);
    }
  }
