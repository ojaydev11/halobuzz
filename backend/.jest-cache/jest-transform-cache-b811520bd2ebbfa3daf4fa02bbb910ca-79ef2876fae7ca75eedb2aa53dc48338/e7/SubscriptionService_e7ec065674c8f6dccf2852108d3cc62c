        $gte: lastMonth,
        $lt: now
      }
    }));
    const twoMonthsAgoSubs =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[170]++, await SubscriptionModel.countDocuments({
      creatorId,
      createdAt: {
        $gte: twoMonthsAgo,
        $lt: lastMonth
      }
    }));
    /* istanbul ignore next */
    cov_1l6x9xjyrc().s[171]++;
    return twoMonthsAgoSubs > 0 ?
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().b[25][0]++, (lastMonthSubs - twoMonthsAgoSubs) / twoMonthsAgoSubs * 100) :
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().b[25][1]++, 0);
  }
  async calculateChurnRate(creatorId) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[30]++;
    const totalSubscriptions =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[172]++, await SubscriptionModel.countDocuments({
      creatorId
    }));
    const cancelledSubscriptions =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[173]++, await SubscriptionModel.countDocuments({
      creatorId,
      status: 'cancelled'
    }));
    /* istanbul ignore next */
    cov_1l6x9xjyrc().s[174]++;
    return totalSubscriptions > 0 ?
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().b[26][0]++, cancelledSubscriptions / totalSubscriptions * 100) :
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().b[26][1]++, 0);
  }
  calculateProjections(currentMRR, growthRate, churnRate) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[31]++;
    const netGrowthRate =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[175]++, (growthRate - churnRate) / 100);
    /* istanbul ignore next */
    cov_1l6x9xjyrc().s[176]++;
    return {
      nextMonth: currentMRR * (1 + netGrowthRate),
      next3Months: currentMRR * Math.pow(1 + netGrowthRate, 3),
      next6Months: currentMRR * Math.pow(1 + netGrowthRate, 6),
      nextYear: currentMRR * Math.pow(1 + netGrowthRate, 12)
    };
  }
  async projectNewSubscribers(creatorId, growthRate) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[32]++;
    const currentSubscribers =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[177]++, await SubscriptionModel.countDocuments({
      creatorId,
      status: 'active'
    }));
    const monthlyGrowth =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[178]++, growthRate / 100 * currentSubscribers);
    /* istanbul ignore next */
    cov_1l6x9xjyrc().s[179]++;
    return {
      nextMonth: Math.round(monthlyGrowth),
      next3Months: Math.round(monthlyGrowth * 3),
      next6Months: Math.round(monthlyGrowth * 6)
    };
  }
  calculateConfidence(growthRate, churnRate) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[33]++;
    // Confidence based on stability of growth and churn rates
    const stability =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[180]++, 1 - Math.abs(growthRate - churnRate) / 100);
    /* istanbul ignore next */
    cov_1l6x9xjyrc().s[181]++;
    return Math.max(0.3, Math.min(0.95, stability));
  }
  generateAssumptions(growthRate, churnRate) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[34]++;
    const assumptions =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[182]++, ['Current growth and churn rates will remain consistent', 'No major market changes or competitor actions', 'Creator maintains current content quality and engagement']);
    /* istanbul ignore next */
    cov_1l6x9xjyrc().s[183]++;
    if (growthRate > 20) {
      /* istanbul ignore next */
      cov_1l6x9xjyrc().b[27][0]++;
      cov_1l6x9xjyrc().s[184]++;
      assumptions.push('High growth rate may not be sustainable long-term');
    } else
    /* istanbul ignore next */
    {
      cov_1l6x9xjyrc().b[27][1]++;
    }
    cov_1l6x9xjyrc().s[185]++;
    if (churnRate > 15) {
      /* istanbul ignore next */
      cov_1l6x9xjyrc().b[28][0]++;
      cov_1l6x9xjyrc().s[186]++;
      assumptions.push('High churn rate indicates potential retention issues');
    } else
    /* istanbul ignore next */
    {
      cov_1l6x9xjyrc().b[28][1]++;
    }
    cov_1l6x9xjyrc().s[187]++;
    return assumptions;
  }
