  async getSubscriberGrowth(creatorId, days) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[23]++;
    const growth =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[148]++, []);
    const now =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[149]++, new Date());
    /* istanbul ignore next */
    cov_1l6x9xjyrc().s[150]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[151]++, days - 1); i >= 0; i--) {
      const date =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[152]++, new Date(now.getTime() - i * 24 * 60 * 60 * 1000));
      const startOfDay =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[153]++, new Date(date.setHours(0, 0, 0, 0)));
      const endOfDay =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[154]++, new Date(date.setHours(23, 59, 59, 999)));
      const newSubscribers =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[155]++, await SubscriptionModel.countDocuments({
        creatorId,
        createdAt: {
          $gte: startOfDay,
          $lte: endOfDay
        }
      }));
      const totalSubscribers =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[156]++, await SubscriptionModel.countDocuments({
        creatorId,
        createdAt: {
          $lte: endOfDay
        }
      }));
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[157]++;
      growth.push({
        date: startOfDay.toISOString().split('T')[0],
        newSubscribers,
        totalSubscribers
      });
    }
    /* istanbul ignore next */
    cov_1l6x9xjyrc().s[158]++;
    return growth;
  }
