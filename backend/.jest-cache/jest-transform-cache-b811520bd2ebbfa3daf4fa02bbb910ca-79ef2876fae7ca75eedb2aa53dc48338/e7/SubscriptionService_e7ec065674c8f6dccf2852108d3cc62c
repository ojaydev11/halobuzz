  /**
   * Cancel subscription
   */
  async cancelSubscription(subscriptionId, reason) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[5]++;
    cov_1l6x9xjyrc().s[59]++;
    try {
      const subscription =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[60]++, await SubscriptionModel.findOne({
        id: subscriptionId
      }));
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[61]++;
      if (!subscription) {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().b[14][0]++;
        cov_1l6x9xjyrc().s[62]++;
        throw new Error(`Subscription ${subscriptionId} not found`);
      } else
      /* istanbul ignore next */
      {
        cov_1l6x9xjyrc().b[14][1]++;
      }
      cov_1l6x9xjyrc().s[63]++;
      if (subscription.status !== 'active') {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().b[15][0]++;
        cov_1l6x9xjyrc().s[64]++;
        throw new Error(`Subscription ${subscriptionId} is not active`);
      } else
      /* istanbul ignore next */
      {
        cov_1l6x9xjyrc().b[15][1]++;
      }
      cov_1l6x9xjyrc().s[65]++;
      subscription.status = 'cancelled';
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[66]++;
      subscription.cancellationReason =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().b[16][0]++, reason) ||
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().b[16][1]++, 'User requested');
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[67]++;
      subscription.cancelledAt = new Date();
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[68]++;
      subscription.autoRenew = false;
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[69]++;
      await subscription.save();
      // Update tier subscriber count
      const tier =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[70]++, await CreatorTierModel.findById(subscription.tierId));
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[71]++;
      if (tier) {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().b[17][0]++;
        cov_1l6x9xjyrc().s[72]++;
        tier.subscriberCount = Math.max(0, tier.subscriberCount - 1);
        /* istanbul ignore next */
        cov_1l6x9xjyrc().s[73]++;
        await tier.save();
      } else
      /* istanbul ignore next */
      {
        cov_1l6x9xjyrc().b[17][1]++;
      }
      cov_1l6x9xjyrc().s[74]++;
      logger_1.default.info('Subscription cancelled', {
        subscriptionId,
        subscriberId: subscription.subscriberId,
        creatorId: subscription.creatorId,
        reason
      });
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[75]++;
      return true;
    } catch (error) {
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[76]++;
      logger_1.default.error('Error cancelling subscription:', error);
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[77]++;
      throw error;
    }
  }
  /**
   * Subscriber analytics and insights
   */
  async getSubscriberInsights(creatorId) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[6]++;
    cov_1l6x9xjyrc().s[78]++;
    try {
      const subscriptions =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[79]++, await SubscriptionModel.find({
        creatorId
      }));
      const activeSubscriptions =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[80]++, subscriptions.filter(sub => {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().f[7]++;
        cov_1l6x9xjyrc().s[81]++;
        return sub.status === 'active';
      }));
      const totalSubscribers =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[82]++, subscriptions.length);
      const activeSubscribers =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[83]++, activeSubscriptions.length);
      // Calculate new subscribers (last 30 days)
      const thirtyDaysAgo =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[84]++, new Date(Date.now() - 30 * 24 * 60 * 60 * 1000));
      const newSubscribers =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[85]++, subscriptions.filter(sub => {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().f[8]++;
        cov_1l6x9xjyrc().s[86]++;
        return sub.createdAt >= thirtyDaysAgo;
      }).length);
      // Calculate churn rate
      const cancelledSubscriptions =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[87]++, subscriptions.filter(sub => {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().f[9]++;
        cov_1l6x9xjyrc().s[88]++;
        return sub.status === 'cancelled';
      }));
      const churnRate =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[89]++, totalSubscribers > 0 ?
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().b[18][0]++, cancelledSubscriptions.length / totalSubscribers * 100) :
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().b[18][1]++, 0));
      // Calculate ARPU and MRR
      const totalRevenue =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[90]++, activeSubscriptions.reduce((sum, sub) => {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().f[10]++;
        cov_1l6x9xjyrc().s[91]++;
        return sum + sub.amount;
      }, 0));
      const averageRevenuePerUser =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[92]++, activeSubscribers > 0 ?
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().b[19][0]++, totalRevenue / activeSubscribers) :
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().b[19][1]++, 0));
      const monthlyRecurringRevenue =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[93]++, activeSubscriptions.reduce((sum, sub) => {
        /* istanbul ignore next */
        cov_1l6x9xjyrc().f[11]++;
        cov_1l6x9xjyrc().s[94]++;
        return sum + (sub.billingCycle === 'monthly' ?
        /* istanbul ignore next */
        (cov_1l6x9xjyrc().b[20][0]++, sub.amount) :
        /* istanbul ignore next */
        (cov_1l6x9xjyrc().b[20][1]++, sub.amount / 12));
      }, 0));
      // Get tier distribution
      const tierDistribution =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[95]++, await this.getTierDistribution(creatorId));
      // Get subscriber growth data
      const subscriberGrowth =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[96]++, await this.getSubscriberGrowth(creatorId, 30));
      // Get top subscribers
      const topSubscribers =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[97]++, await this.getTopSubscribers(creatorId));
      // Get demographics (mock implementation)
      const demographics =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[98]++, await this.getSubscriberDemographics(creatorId));
      const analytics =
      /* istanbul ignore next */
      (cov_1l6x9xjyrc().s[99]++, {
        totalSubscribers,
        activeSubscribers,
        newSubscribers,
        churnRate,
        averageRevenuePerUser,
        monthlyRecurringRevenue,
        tierDistribution,
        subscriberGrowth,
        topSubscribers,
        demographics
      });
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[100]++;
      logger_1.default.info('Subscriber insights generated', {
        creatorId,
        totalSubscribers,
        activeSubscribers,
        monthlyRecurringRevenue
      });
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[101]++;
      return analytics;
    } catch (error) {
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[102]++;
      logger_1.default.error('Error getting subscriber insights:', error);
      /* istanbul ignore next */
      cov_1l6x9xjyrc().s[103]++;
      throw error;
    }
  }
