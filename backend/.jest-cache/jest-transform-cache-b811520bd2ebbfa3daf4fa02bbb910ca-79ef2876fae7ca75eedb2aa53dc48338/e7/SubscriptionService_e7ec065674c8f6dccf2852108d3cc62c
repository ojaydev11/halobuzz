  async calculateGrowthRate(creatorId) {
    /* istanbul ignore next */
    cov_1l6x9xjyrc().f[29]++;
    const now =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[166]++, new Date());
    const lastMonth =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[167]++, new Date(now.getTime() - 30 * 24 * 60 * 60 * 1000));
    const twoMonthsAgo =
    /* istanbul ignore next */
    (cov_1l6x9xjyrc().s[168]++, new Date(now.getTime() - 60 * 24 * 60 * 60 * 1000));
