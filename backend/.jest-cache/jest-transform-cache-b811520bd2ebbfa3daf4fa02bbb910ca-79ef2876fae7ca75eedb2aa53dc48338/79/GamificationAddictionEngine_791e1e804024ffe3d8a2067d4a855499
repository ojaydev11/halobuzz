      /* istanbul ignore next */
      (cov_1ewugi22qv().s[298]++, i >= progressBar.design.segmentCount - 2);
      /* istanbul ignore next */
      cov_1ewugi22qv().s[299]++;
      segments.push({
        threshold,
        reward: isEarly ?
        /* istanbul ignore next */
        (cov_1ewugi22qv().b[48][0]++, 'quick_start_reward') :
        /* istanbul ignore next */
        (cov_1ewugi22qv().b[48][1]++, isNearEnd ?
        /* istanbul ignore next */
        (cov_1ewugi22qv().b[49][0]++, 'completion_reward') :
        /* istanbul ignore next */
        (cov_1ewugi22qv().b[49][1]++, 'standard_reward')),
        claimed: false
      });
    }
    /* istanbul ignore next */
    cov_1ewugi22qv().s[300]++;
    return segments;
  }
