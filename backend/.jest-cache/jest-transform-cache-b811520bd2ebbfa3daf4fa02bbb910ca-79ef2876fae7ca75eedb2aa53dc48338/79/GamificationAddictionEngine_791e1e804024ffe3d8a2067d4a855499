    /* istanbul ignore next */
    (cov_1ewugi22qv().s[330]++, new Date().toDateString());
    /* istanbul ignore next */
    cov_1ewugi22qv().s[331]++;
    if (profile.lastDailyReward.toDateString() === today) {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[55][0]++;
      cov_1ewugi22qv().s[332]++;
      throw new Error('Daily reward already claimed');
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[55][1]++;
    }
    cov_1ewugi22qv().s[333]++;
    profile.lastDailyReward = new Date();
    /* istanbul ignore next */
    cov_1ewugi22qv().s[334]++;
    profile.dailyRewardStreak++;
    /* istanbul ignore next */
    cov_1ewugi22qv().s[335]++;
