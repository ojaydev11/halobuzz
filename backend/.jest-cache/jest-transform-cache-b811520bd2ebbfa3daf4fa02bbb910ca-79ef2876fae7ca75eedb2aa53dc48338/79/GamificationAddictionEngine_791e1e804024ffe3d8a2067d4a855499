      weeklyStreak: 0,
      monthlyStreak: 0,
      longestStreak: 0
    });
    // Calculate overall addiction risk level
    /* istanbul ignore next */
    cov_1ewugi22qv().s[113]++;
    profile.safetyMetrics.addictionRiskLevel = this.calculateAddictionRisk(profile);
    /* istanbul ignore next */
    cov_1ewugi22qv().s[114]++;
    return profile;
  }
  async performSafetyCheck(userId, loopId, profile) {
    /* istanbul ignore next */
    cov_1ewugi22qv().f[14]++;
    const loop =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[115]++, this.addictiveLoops.get(loopId));
    /* istanbul ignore next */
    cov_1ewugi22qv().s[116]++;
    if (!loop) {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[13][0]++;
      cov_1ewugi22qv().s[117]++;
      return {
        safe: false,
        reason: 'Loop not found'
      };
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[13][1]++;
    }
    // Check daily trigger limits
    const activeLoop =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[118]++, profile.activeLoops[loopId]);
    /* istanbul ignore next */
    cov_1ewugi22qv().s[119]++;
    if (
    /* istanbul ignore next */
    (cov_1ewugi22qv().b[15][0]++, activeLoop) &&
    /* istanbul ignore next */
    (cov_1ewugi22qv().b[15][1]++, activeLoop.triggerCount >= loop.monitoringFlags.maxDailyTriggers)) {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[14][0]++;
      cov_1ewugi22qv().s[120]++;
      return {
        safe: false,
        reason: 'Daily trigger limit exceeded',
        nextAllowedTime: new Date(Date.now() + 24 * 60 * 60 * 1000) // Tomorrow
      };
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[14][1]++;
    }
    // Check cooling off period
    cov_1ewugi22qv().s[121]++;
    if (
    /* istanbul ignore next */
    (cov_1ewugi22qv().b[17][0]++, activeLoop) &&
    /* istanbul ignore next */
    (cov_1ewugi22qv().b[17][1]++, activeLoop.lastTriggered)) {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[16][0]++;
      const timeSinceLastTrigger =
      /* istanbul ignore next */
      (cov_1ewugi22qv().s[122]++, Date.now() - activeLoop.lastTriggered.getTime());
      const coolingOffMs =
      /* istanbul ignore next */
      (cov_1ewugi22qv().s[123]++, loop.monitoringFlags.coolingOffPeriod * 60 * 1000);
      /* istanbul ignore next */
      cov_1ewugi22qv().s[124]++;
      if (timeSinceLastTrigger < coolingOffMs) {
        /* istanbul ignore next */
        cov_1ewugi22qv().b[18][0]++;
        cov_1ewugi22qv().s[125]++;
        return {
          safe: false,
          reason: 'Cooling off period active',
          nextAllowedTime: new Date(activeLoop.lastTriggered.getTime() + coolingOffMs)
        };
      } else
      /* istanbul ignore next */
      {
        cov_1ewugi22qv().b[18][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[16][1]++;
    }
    // Check critical addiction level
    cov_1ewugi22qv().s[126]++;
    if (profile.safetyMetrics.addictionRiskLevel === 'critical') {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[19][0]++;
      cov_1ewugi22qv().s[127]++;
      return {
        safe: false,
        reason: 'Critical addiction risk level - mandatory break required',
        nextAllowedTime: new Date(Date.now() + this.ADDICTION_SAFEGUARDS.MANDATORY_BREAK_DURATION * 60000)
      };
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[19][1]++;
    }
    // Check spending velocity
    cov_1ewugi22qv().s[128]++;
    if (profile.safetyMetrics.dailySpending > this.ADDICTION_SAFEGUARDS.SPENDING_VELOCITY_ALERT) {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[20][0]++;
      cov_1ewugi22qv().s[129]++;
      return {
        safe: false,
        reason: 'High spending velocity detected - cooling off required',
        nextAllowedTime: new Date(Date.now() + 2 * 60 * 60 * 1000) // 2 hours
      };
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[20][1]++;
    }
    cov_1ewugi22qv().s[130]++;
    return {
      safe: true
    };
  }
  calculateLoopEffectiveness(loop, profile) {
    /* istanbul ignore next */
    cov_1ewugi22qv().f[15]++;
    let effectiveness =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[131]++, 0.5); // Base effectiveness
    // Match loop triggers with user vulnerabilities
    /* istanbul ignore next */
    cov_1ewugi22qv().s[132]++;
    effectiveness += loop.triggers.dopamineRelease * profile.vulnerability.rewardSensitivity * 0.3;
    /* istanbul ignore next */
    cov_1ewugi22qv().s[133]++;
    effectiveness += loop.triggers.socialValidation * profile.vulnerability.socialValidationNeed * 0.25;
    /* istanbul ignore next */
    cov_1ewugi22qv().s[134]++;
    effectiveness += loop.triggers.lossAversion * profile.vulnerability.lossAversionStrength * 0.2;
    /* istanbul ignore next */
    cov_1ewugi22qv().s[135]++;
    effectiveness += loop.triggers.collectibleUrge * profile.vulnerability.compulsivityScore * 0.15;
    /* istanbul ignore next */
    cov_1ewugi22qv().s[136]++;
    effectiveness += loop.triggers.statusThreat * profile.vulnerability.socialValidationNeed * 0.1;
    // Account for current state
    /* istanbul ignore next */
    cov_1ewugi22qv().s[137]++;
    effectiveness *= 0.7 + profile.currentState.engagementHeat / 100 * 0.3;
    // Account for resistance buildup
    const activeLoop =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[138]++, profile.activeLoops[loop.id]);
    /* istanbul ignore next */
    cov_1ewugi22qv().s[139]++;
    if (activeLoop) {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[21][0]++;
      cov_1ewugi22qv().s[140]++;
      effectiveness *= 1 - activeLoop.resistanceLevel * 0.3;
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[21][1]++;
    }
    cov_1ewugi22qv().s[141]++;
    return Math.max(0.1, Math.min(1.0, effectiveness));
  }
  async generateAddictiveEffect(userId, loop, effectiveness, context) {
    /* istanbul ignore next */
    cov_1ewugi22qv().f[16]++;
    const profile =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[142]++, await this.getUserAddictionProfile(userId));
    const effect =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[143]++, {
      type: loop.type,
      intensity: effectiveness,
      duration: Math.floor(effectiveness * 300000),
      // Up to 5 minutes
      triggers: [],
      rewards: [],
      pressures: [],
      visuals: [],
      sounds: []
    });
    // Generate specific effects based on loop type
    /* istanbul ignore next */
    cov_1ewugi22qv().s[144]++;
    switch (loop.type) {
      case 'variable_reward':
        /* istanbul ignore next */
        cov_1ewugi22qv().b[22][0]++;
        cov_1ewugi22qv().s[145]++;
        effect.rewards = this.generateVariableRewards(effectiveness, profile);
        /* istanbul ignore next */
        cov_1ewugi22qv().s[146]++;
        effect.visuals = ['slot_machine_effect', 'reward_burst'];
        /* istanbul ignore next */
        cov_1ewugi22qv().s[147]++;
        effect.sounds = ['reward_chime', 'celebration'];
        /* istanbul ignore next */
        cov_1ewugi22qv().s[148]++;
        break;
      case 'social_validation':
        /* istanbul ignore next */
        cov_1ewugi22qv().b[22][1]++;
        cov_1ewugi22qv().s[149]++;
        effect.pressures = this.generateSocialValidationPressures(effectiveness, profile);
        /* istanbul ignore next */
        cov_1ewugi22qv().s[150]++;
        effect.visuals = ['social_spotlight', 'peer_comparison'];
        /* istanbul ignore next */
        cov_1ewugi22qv().s[151]++;
        effect.sounds = ['social_notification', 'achievement_unlock'];
        /* istanbul ignore next */
        cov_1ewugi22qv().s[152]++;
        break;
      case 'fear_missing_out':
        /* istanbul ignore next */
        cov_1ewugi22qv().b[22][2]++;
        cov_1ewugi22qv().s[153]++;
        effect.triggers = this.generateFOMOTriggers(effectiveness, profile);
        /* istanbul ignore next */
        cov_1ewugi22qv().s[154]++;
        effect.visuals = ['countdown_timer', 'scarcity_indicator'];
        /* istanbul ignore next */
        cov_1ewugi22qv().s[155]++;
        effect.sounds = ['urgency_alert', 'ticking_clock'];
        /* istanbul ignore next */
        cov_1ewugi22qv().s[156]++;
        break;
      case 'sunk_cost':
        /* istanbul ignore next */
        cov_1ewugi22qv().b[22][3]++;
        cov_1ewugi22qv().s[157]++;
        effect.pressures = this.generateSunkCostPressures(effectiveness, profile);
        /* istanbul ignore next */
        cov_1ewugi22qv().s[158]++;
        effect.visuals = ['progress_bar_near_complete', 'investment_display'];
        /* istanbul ignore next */
        cov_1ewugi22qv().s[159]++;
        effect.sounds = ['progress_notification'];
        /* istanbul ignore next */
        cov_1ewugi22qv().s[160]++;
        break;
    }
    /* istanbul ignore next */
    cov_1ewugi22qv().s[161]++;
    return effect;
  }
  async updateAddictionProfile(userId, loopId, effect) {
    /* istanbul ignore next */
    cov_1ewugi22qv().f[17]++;
    const profile =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[162]++, await this.getUserAddictionProfile(userId));
    // Initialize loop tracking if not exists
    /* istanbul ignore next */
    cov_1ewugi22qv().s[163]++;
    if (!profile.activeLoops[loopId]) {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[23][0]++;
      cov_1ewugi22qv().s[164]++;
      profile.activeLoops[loopId] = {
        effectiveness: 0,
        triggerCount: 0,
        lastTriggered: new Date(),
        resistanceLevel: 0
      };
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[23][1]++;
    }
    const activeLoop =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[165]++, profile.activeLoops[loopId]);
    // Update loop statistics
    /* istanbul ignore next */
    cov_1ewugi22qv().s[166]++;
    activeLoop.triggerCount += 1;
    /* istanbul ignore next */
    cov_1ewugi22qv().s[167]++;
    activeLoop.lastTriggered = new Date();
    /* istanbul ignore next */
    cov_1ewugi22qv().s[168]++;
    activeLoop.effectiveness = activeLoop.effectiveness * 0.9 + effect.intensity * 0.1; // Weighted average
    // Build resistance over time (prevents oversaturation)
    /* istanbul ignore next */
    cov_1ewugi22qv().s[169]++;
    activeLoop.resistanceLevel = Math.min(0.8, activeLoop.resistanceLevel + 0.02);
    // Update current state based on effect
    /* istanbul ignore next */
    cov_1ewugi22qv().s[170]++;
    profile.currentState.engagementHeat = Math.min(100, profile.currentState.engagementHeat + effect.intensity * 10);
    /* istanbul ignore next */
    cov_1ewugi22qv().s[171]++;
    profile.currentState.dopamineDepletion += effect.intensity * 5; // Dopamine gets depleted with use
    /* istanbul ignore next */
    cov_1ewugi22qv().s[172]++;
    profile.lastUpdated = new Date();
    /* istanbul ignore next */
    cov_1ewugi22qv().s[173]++;
    this.userProfiles.set(userId, profile);
  }
  // Helper methods for calculations (simplified implementations)
  calculateCompulsivity(transactions) {
    /* istanbul ignore next */
    cov_1ewugi22qv().f[18]++;
    // Analyze transaction patterns for compulsive behavior
    let compulsivityScore =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[174]++, 0);
    // Rapid consecutive transactions
    let rapidTransactions =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[175]++, 0);
    /* istanbul ignore next */
    cov_1ewugi22qv().s[176]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[177]++, 1); i < transactions.length; i++) {
      const timeDiff =
      /* istanbul ignore next */
      (cov_1ewugi22qv().s[178]++, transactions[i - 1].createdAt.getTime() - transactions[i].createdAt.getTime());
      /* istanbul ignore next */
      cov_1ewugi22qv().s[179]++;
      if (timeDiff < 60000) {
        /* istanbul ignore next */
        cov_1ewugi22qv().b[24][0]++;
        cov_1ewugi22qv().s[180]++;
        // Less than 1 minute apart
        rapidTransactions++;
      } else
      /* istanbul ignore next */
      {
        cov_1ewugi22qv().b[24][1]++;
      }
    }
    /* istanbul ignore next */
    cov_1ewugi22qv().s[181]++;
    compulsivityScore += Math.min(0.5, rapidTransactions / transactions.length);
    // Late night activity (compulsive behavior indicator)
    const lateNightTransactions =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[182]++, transactions.filter(tx => {
      /* istanbul ignore next */
      cov_1ewugi22qv().f[19]++;
      const hour =
      /* istanbul ignore next */
      (cov_1ewugi22qv().s[183]++, new Date(tx.createdAt).getHours());
      /* istanbul ignore next */
      cov_1ewugi22qv().s[184]++;
      return /* istanbul ignore next */(cov_1ewugi22qv().b[25][0]++, hour < 6) ||
      /* istanbul ignore next */
      (cov_1ewugi22qv().b[25][1]++, hour > 23);
    }).length);
    /* istanbul ignore next */
    cov_1ewugi22qv().s[185]++;
    compulsivityScore += Math.min(0.3, lateNightTransactions / transactions.length);
    /* istanbul ignore next */
    cov_1ewugi22qv().s[186]++;
    return Math.min(1.0, compulsivityScore);
  }
  calculateRewardSensitivity(transactions) {
    /* istanbul ignore next */
    cov_1ewugi22qv().f[20]++;
    // Analyze how responsive user is to rewards
    const rewardTransactions =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[187]++, transactions.filter(tx => {
      /* istanbul ignore next */
      cov_1ewugi22qv().f[21]++;
      cov_1ewugi22qv().s[188]++;
      return tx.type === 'reward';
    }));
    /* istanbul ignore next */
    cov_1ewugi22qv().s[189]++;
    if (rewardTransactions.length === 0) {
      /* istanbul ignore next */
      cov_1ewugi22qv().b[26][0]++;
      cov_1ewugi22qv().s[190]++;
      return 0.5;
    } else
    /* istanbul ignore next */
    {
      cov_1ewugi22qv().b[26][1]++;
    }
    // Users who frequently claim rewards are more reward-sensitive
    const rewardRatio =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[191]++, rewardTransactions.length / transactions.length);
    /* istanbul ignore next */
    cov_1ewugi22qv().s[192]++;
    return Math.min(1.0, rewardRatio * 2);
  }
  calculateStreakPressure(userId) {
    /* istanbul ignore next */
    cov_1ewugi22qv().f[22]++;
    const userStreaks =
    /* istanbul ignore next */
    (cov_1ewugi22qv().s[193]++, this.streakSystems.get(userId));
    /* istanbul ignore next */
    cov_1ewugi22qv().s[194]++;
