          cov_1tfm6k51r2().b[12][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_1tfm6k51r2().b[9][1]++;
      }
      // Check game-specific limits
      const game =
      /* istanbul ignore next */
      (cov_1tfm6k51r2().s[41]++, await Game_1.Game.findById(gameId));
      /* istanbul ignore next */
      cov_1tfm6k51r2().s[42]++;
      if (game) {
        /* istanbul ignore next */
        cov_1tfm6k51r2().b[13][0]++;
        cov_1tfm6k51r2().s[43]++;
        // Enforce AI win rate bounds (35-55%)
        if (
        /* istanbul ignore next */
        (cov_1tfm6k51r2().b[15][0]++, game.aiWinRate < 35) ||
        /* istanbul ignore next */
        (cov_1tfm6k51r2().b[15][1]++, game.aiWinRate > 55)) {
          /* istanbul ignore next */
          cov_1tfm6k51r2().b[14][0]++;
          cov_1tfm6k51r2().s[44]++;
          return {
            allowed: false,
            reason: 'Invalid game configuration'
          };
        } else
        /* istanbul ignore next */
        {
          cov_1tfm6k51r2().b[14][1]++;
        }
        // Check if game is active
        cov_1tfm6k51r2().s[45]++;
        if (!game.isActive) {
          /* istanbul ignore next */
          cov_1tfm6k51r2().b[16][0]++;
          cov_1tfm6k51r2().s[46]++;
          return {
            allowed: false,
            reason: 'Game is not available'
          };
        } else
        /* istanbul ignore next */
        {
          cov_1tfm6k51r2().b[16][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_1tfm6k51r2().b[13][1]++;
      }
      cov_1tfm6k51r2().s[47]++;
