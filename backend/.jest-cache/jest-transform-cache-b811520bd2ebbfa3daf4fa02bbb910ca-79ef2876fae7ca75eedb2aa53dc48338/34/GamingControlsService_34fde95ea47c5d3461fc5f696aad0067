  /**
   * Calculate age from date of birth
   */
  calculateAge(dateOfBirth) {
    /* istanbul ignore next */
    cov_1tfm6k51r2().f[11]++;
    const today =
    /* istanbul ignore next */
    (cov_1tfm6k51r2().s[75]++, new Date());
    const birthDate =
    /* istanbul ignore next */
    (cov_1tfm6k51r2().s[76]++, new Date(dateOfBirth));
    let age =
    /* istanbul ignore next */
    (cov_1tfm6k51r2().s[77]++, today.getFullYear() - birthDate.getFullYear());
    const monthDiff =
    /* istanbul ignore next */
    (cov_1tfm6k51r2().s[78]++, today.getMonth() - birthDate.getMonth());
    /* istanbul ignore next */
    cov_1tfm6k51r2().s[79]++;
    if (
    /* istanbul ignore next */
    (cov_1tfm6k51r2().b[22][0]++, monthDiff < 0) ||
    /* istanbul ignore next */
    (cov_1tfm6k51r2().b[22][1]++, monthDiff === 0) &&
    /* istanbul ignore next */
    (cov_1tfm6k51r2().b[22][2]++, today.getDate() < birthDate.getDate())) {
      /* istanbul ignore next */
      cov_1tfm6k51r2().b[21][0]++;
      cov_1tfm6k51r2().s[80]++;
      age--;
    } else
    /* istanbul ignore next */
    {
      cov_1tfm6k51r2().b[21][1]++;
    }
    cov_1tfm6k51r2().s[81]++;
    return age;
  }
