  calculateProgressionRate(hierarchy) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[37]++;
    // Calculate based on recent activity - simplified implementation
    const recentHistory =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[147]++, hierarchy.reputation.history.slice(-30)); // Last 30 activities
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[148]++;
