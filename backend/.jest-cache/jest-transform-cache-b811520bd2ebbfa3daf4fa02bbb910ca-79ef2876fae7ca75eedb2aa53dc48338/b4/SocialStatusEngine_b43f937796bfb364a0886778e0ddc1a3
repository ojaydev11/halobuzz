    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[150]++, recentHistory.reduce((sum, h) => {
      /* istanbul ignore next */
      cov_22r3qz0ihd().f[38]++;
      cov_22r3qz0ihd().s[151]++;
      return sum + h.impact;
    }, 0));
    const daySpan =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[152]++, Math.max(1, (Date.now() - recentHistory[0].date.getTime()) / (1000 * 60 * 60 * 24)));
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[153]++;
    return totalPoints / daySpan;
  }
