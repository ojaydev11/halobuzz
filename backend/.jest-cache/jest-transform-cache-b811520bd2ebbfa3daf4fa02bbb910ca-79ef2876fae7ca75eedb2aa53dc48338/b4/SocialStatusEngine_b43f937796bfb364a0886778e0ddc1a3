  async getUserHierarchy(userId) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[7]++;
    cov_22r3qz0ihd().s[22]++;
    if (!this.userHierarchies.has(userId)) {
      /* istanbul ignore next */
      cov_22r3qz0ihd().b[1][0]++;
      cov_22r3qz0ihd().s[23]++;
      await this.initializeUserHierarchy(userId);
    } else
    /* istanbul ignore next */
    {
      cov_22r3qz0ihd().b[1][1]++;
    }
    cov_22r3qz0ihd().s[24]++;
    return this.userHierarchies.get(userId);
  }
  async updateUserPoints(userId, points, reason) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[8]++;
    const hierarchy =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[25]++, await this.getUserHierarchy(userId));
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[26]++;
    hierarchy.points += points;
    // Update reputation based on the reason
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[27]++;
    this.updateReputation(hierarchy, reason, points);
    // Check for tier progression
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[28]++;
    await this.checkTierProgression(hierarchy);
    // Check for achievement unlocks
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[29]++;
    await this.checkAchievements(userId, hierarchy);
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[30]++;
    this.emit('points_updated', {
      userId,
      points,
      reason,
      newTotal: hierarchy.points,
      tier: hierarchy.currentTier
    });
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[31]++;
    return hierarchy;
  }
