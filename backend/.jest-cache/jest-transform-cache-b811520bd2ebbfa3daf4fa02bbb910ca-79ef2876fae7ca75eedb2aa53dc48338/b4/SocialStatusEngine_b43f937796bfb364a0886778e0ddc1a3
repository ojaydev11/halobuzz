  async grantAchievement(userId, achievementId) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[10]++;
    const hierarchy =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[39]++, await this.getUserHierarchy(userId));
    const achievement =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[40]++, this.achievements.get(achievementId));
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[41]++;
    if (
    /* istanbul ignore next */
    (cov_22r3qz0ihd().b[4][0]++, !achievement) ||
    /* istanbul ignore next */
    (cov_22r3qz0ihd().b[4][1]++, hierarchy.achievements.some(a => {
      /* istanbul ignore next */
      cov_22r3qz0ihd().f[11]++;
      cov_22r3qz0ihd().s[42]++;
      return a.id === achievementId;
    }))) {
      /* istanbul ignore next */
      cov_22r3qz0ihd().b[3][0]++;
      cov_22r3qz0ihd().s[43]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_22r3qz0ihd().b[3][1]++;
    }
    // Add achievement to user
    cov_22r3qz0ihd().s[44]++;
    achievement.unlockedAt = new Date();
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[45]++;
    hierarchy.achievements.push(achievement);
    // Grant rewards
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[46]++;
    hierarchy.points += achievement.rewards.points;
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[47]++;
    hierarchy.reputation.overall += Object.values(achievement.rewards.reputation).reduce((sum, val) => {
      /* istanbul ignore next */
      cov_22r3qz0ihd().f[12]++;
      cov_22r3qz0ihd().s[48]++;
      return sum + val;
    }, 0);
    // Update individual reputation aspects
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[49]++;
    Object.entries(achievement.rewards.reputation).forEach(([aspect, value]) => {
      /* istanbul ignore next */
      cov_22r3qz0ihd().f[13]++;
      cov_22r3qz0ihd().s[50]++;
      if (aspect in hierarchy.reputation.breakdown) {
        /* istanbul ignore next */
        cov_22r3qz0ihd().b[5][0]++;
        cov_22r3qz0ihd().s[51]++;
        hierarchy.reputation.breakdown[aspect] += value;
      } else
      /* istanbul ignore next */
      {
        cov_22r3qz0ihd().b[5][1]++;
      }
    });
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[52]++;
    this.emit('achievement_unlocked', {
      userId,
      achievement,
      rewards: achievement.rewards
    });
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[53]++;
    return true;
  }
