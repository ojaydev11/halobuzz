  checkAchievementRequirements(hierarchy, achievement) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[48]++;
    cov_22r3qz0ihd().s[202]++;
    return achievement.requirements.every(req => {
      /* istanbul ignore next */
      cov_22r3qz0ihd().f[49]++;
      cov_22r3qz0ihd().s[203]++;
      switch (req.type) {
        case 'gifts_sent':
          /* istanbul ignore next */
          cov_22r3qz0ihd().b[42][0]++;
          cov_22r3qz0ihd().s[204]++;
          return hierarchy.socialMetrics.totalGiftsGiven >= Number(req.value);
        case 'followers':
          /* istanbul ignore next */
          cov_22r3qz0ihd().b[42][1]++;
          cov_22r3qz0ihd().s[205]++;
          return hierarchy.socialMetrics.followers >= Number(req.value);
        case 'coins_spent':
          /* istanbul ignore next */
          cov_22r3qz0ihd().b[42][2]++;
          cov_22r3qz0ihd().s[206]++;
          // Would need to track total spending
          return false;
        case 'login_streak':
          /* istanbul ignore next */
          cov_22r3qz0ihd().b[42][3]++;
          cov_22r3qz0ihd().s[207]++;
          // Would need to track streaks
          return false;
        case 'content_reach':
          /* istanbul ignore next */
