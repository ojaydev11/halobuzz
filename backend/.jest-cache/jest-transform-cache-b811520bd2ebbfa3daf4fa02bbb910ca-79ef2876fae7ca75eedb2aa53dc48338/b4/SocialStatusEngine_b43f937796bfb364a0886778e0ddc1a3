  updateReputation(hierarchy, reason, points) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[25]++;
    const reputationMapping =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[86]++, {
      'gift_sent': 'generosity',
      'stream_hosted': 'creativity',
      'daily_login': 'reliability',
      'community_interaction': 'engagement',
      'referral_successful': 'influence',
      'event_participation': 'sportsmanship'
    });
    const aspect =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[87]++, reputationMapping[reason]);
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[88]++;
    if (aspect) {
      /* istanbul ignore next */
      cov_22r3qz0ihd().b[10][0]++;
      const reputationGain =
      /* istanbul ignore next */
      (cov_22r3qz0ihd().s[89]++, Math.floor(points * 0.1));
      /* istanbul ignore next */
      cov_22r3qz0ihd().s[90]++;
      hierarchy.reputation.breakdown[aspect] += reputationGain;
      /* istanbul ignore next */
      cov_22r3qz0ihd().s[91]++;
      hierarchy.reputation.overall += reputationGain;
      /* istanbul ignore next */
      cov_22r3qz0ihd().s[92]++;
      hierarchy.reputation.history.push({
        date: new Date(),
        score: hierarchy.reputation.overall,
        reason,
        impact: reputationGain
      });
      // Keep only last 100 history entries
      /* istanbul ignore next */
      cov_22r3qz0ihd().s[93]++;
      if (hierarchy.reputation.history.length > 100) {
        /* istanbul ignore next */
        cov_22r3qz0ihd().b[11][0]++;
        cov_22r3qz0ihd().s[94]++;
        hierarchy.reputation.history = hierarchy.reputation.history.slice(-100);
      } else
      /* istanbul ignore next */
      {
        cov_22r3qz0ihd().b[11][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_22r3qz0ihd().b[10][1]++;
    }
  }
  async checkTierProgression(hierarchy) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[26]++;
    const tiers =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[95]++, Array.from(this.statusTiers.values()).sort((a, b) => {
      /* istanbul ignore next */
      cov_22r3qz0ihd().f[27]++;
      cov_22r3qz0ihd().s[96]++;
      return a.numericLevel - b.numericLevel;
    }));
    const currentTierIndex =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[97]++, tiers.findIndex(t => {
      /* istanbul ignore next */
      cov_22r3qz0ihd().f[28]++;
      cov_22r3qz0ihd().s[98]++;
      return t.tier === hierarchy.currentTier;
    }));
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[99]++;
    if (currentTierIndex < tiers.length - 1) {
      /* istanbul ignore next */
      cov_22r3qz0ihd().b[12][0]++;
      const nextTier =
      /* istanbul ignore next */
      (cov_22r3qz0ihd().s[100]++, tiers[currentTierIndex + 1]);
      /* istanbul ignore next */
      cov_22r3qz0ihd().s[101]++;
      if (this.meetsRequirements(hierarchy, nextTier.requirements)) {
        /* istanbul ignore next */
        cov_22r3qz0ihd().b[13][0]++;
        const previousTier =
        /* istanbul ignore next */
        (cov_22r3qz0ihd().s[102]++, hierarchy.currentTier);
        /* istanbul ignore next */
        cov_22r3qz0ihd().s[103]++;
        hierarchy.currentTier = nextTier.tier;
        /* istanbul ignore next */
        cov_22r3qz0ihd().s[104]++;
        hierarchy.privileges = nextTier.privileges;
        // Update progression info
        /* istanbul ignore next */
        cov_22r3qz0ihd().s[105]++;
        if (currentTierIndex + 2 < tiers.length) {
          /* istanbul ignore next */
          cov_22r3qz0ihd().b[14][0]++;
