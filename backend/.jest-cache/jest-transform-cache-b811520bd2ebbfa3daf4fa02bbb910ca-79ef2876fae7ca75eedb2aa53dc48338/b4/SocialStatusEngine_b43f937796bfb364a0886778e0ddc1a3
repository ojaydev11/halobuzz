        /* istanbul ignore next */
        cov_22r3qz0ihd().b[40][0]++;
        cov_22r3qz0ihd().s[200]++;
        if (this.checkAchievementRequirements(hierarchy, achievement)) {
          /* istanbul ignore next */
          cov_22r3qz0ihd().b[41][0]++;
