  calculateProgressionProbability(hierarchy, predictedTier) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[41]++;
    const currentTierLevel =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[160]++, this.statusTiers.get(hierarchy.currentTier).numericLevel);
    const predictedTierLevel =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[161]++, this.statusTiers.get(predictedTier).numericLevel);
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[162]++;
    if (predictedTierLevel <= currentTierLevel) {
      /* istanbul ignore next */
      cov_22r3qz0ihd().b[31][0]++;
      cov_22r3qz0ihd().s[163]++;
      return 0.9;
    } else
    /* istanbul ignore next */
    {
      cov_22r3qz0ihd().b[31][1]++;
    } // High probability if staying same or going down
    const levelDifference =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[164]++, predictedTierLevel - currentTierLevel);
    const baseProbability =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[165]++, Math.max(0.1, 0.8 - levelDifference * 0.2));
    // Adjust based on recent activity
    const recentActivityMultiplier =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[166]++, hierarchy.reputation.modifiers.recentActivity);
    /* istanbul ignore next */
    cov_22r3qz0ihd().s[167]++;
    return Math.min(0.95, baseProbability * recentActivityMultiplier);
  }
