  async checkAchievements(userId, hierarchy) {
    /* istanbul ignore next */
    cov_22r3qz0ihd().f[46]++;
    const unlockedIds =
    /* istanbul ignore next */
    (cov_22r3qz0ihd().s[196]++, hierarchy.achievements.map(a => {
      /* istanbul ignore next */
      cov_22r3qz0ihd().f[47]++;
      cov_22r3qz0ihd().s[197]++;
      return a.id;
