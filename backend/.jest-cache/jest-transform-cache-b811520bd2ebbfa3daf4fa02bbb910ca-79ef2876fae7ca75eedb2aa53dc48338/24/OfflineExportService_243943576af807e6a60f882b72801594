  async syncExports(year, month, weekNumber) {
    /* istanbul ignore next */
    cov_1cw7uf323q().f[29]++;
    const sourceDir =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[170]++, path.join(this.exportsDir, 'empire', String(year), month));
    /* istanbul ignore next */
    cov_1cw7uf323q().s[171]++;
    if (!fs.existsSync(sourceDir)) {
      /* istanbul ignore next */
      cov_1cw7uf323q().b[34][0]++;
      cov_1cw7uf323q().s[172]++;
      logger_1.logger.warn('Source export directory does not exist', {
        sourceDir
      });
      /* istanbul ignore next */
      cov_1cw7uf323q().s[173]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1cw7uf323q().b[34][1]++;
    }
    // Sync to Drive directory
    cov_1cw7uf323q().s[174]++;
    if (this.syncConfig.driveSyncDir) {
      /* istanbul ignore next */
      cov_1cw7uf323q().b[35][0]++;
      cov_1cw7uf323q().s[175]++;
      await this.syncToDirectory(sourceDir, this.syncConfig.driveSyncDir, year, month, weekNumber);
    } else
