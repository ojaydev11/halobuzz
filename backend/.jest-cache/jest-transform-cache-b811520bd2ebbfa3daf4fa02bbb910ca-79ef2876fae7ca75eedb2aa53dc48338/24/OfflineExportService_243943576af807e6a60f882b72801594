  async getEmpireDashboardData(period) {
    /* istanbul ignore next */
    cov_1cw7uf323q().f[16]++;
    const now =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[102]++, new Date());
    const to =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[103]++, new Date(now));
    /* istanbul ignore next */
    cov_1cw7uf323q().s[104]++;
    to.setHours(23, 59, 59, 999);
    const from =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[105]++, new Date(now));
    /* istanbul ignore next */
    cov_1cw7uf323q().s[106]++;
    if (period === 'weekly') {
      /* istanbul ignore next */
      cov_1cw7uf323q().b[25][0]++;
      cov_1cw7uf323q().s[107]++;
      from.setDate(from.getDate() - 7);
    } else {
      /* istanbul ignore next */
      cov_1cw7uf323q().b[25][1]++;
      cov_1cw7uf323q().s[108]++;
      from.setMonth(from.getMonth() - 1);
    }
    /* istanbul ignore next */
    cov_1cw7uf323q().s[109]++;
    from.setHours(0, 0, 0, 0);
    // Get all unique app IDs
    const uniqueAppIds =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[110]++, await AnalyticsDailyKPI_1.AnalyticsDailyKPI.distinct('appId', {
      date: {
        $gte: from,
        $lte: to
      }
    }));
    const apps =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[111]++, []);
    let totalRevenue =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[112]++, 0);
    let totalDAU =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[113]++, 0);
    let totalMAU =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[114]++, 0);
    let totalARPU =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[115]++, 0);
    let totalPayerRate =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[116]++, 0);
    let totalAlerts =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[117]++, 0);
    let activeAlerts =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[118]++, 0);
    let totalSafetyScore =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[119]++, 0);
    let totalGrowthRate =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[120]++, 0);
    // Process each app
    /* istanbul ignore next */
    cov_1cw7uf323q().s[121]++;
    for (const appId of uniqueAppIds) {
      const kpiData =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[122]++, await AnalyticsDailyKPI_1.AnalyticsDailyKPI.find({
        appId,
        date: {
          $gte: from,
          $lte: to
        }
      }).sort({
        date: 1
      }));
      /* istanbul ignore next */
      cov_1cw7uf323q().s[123]++;
      if (kpiData.length === 0) {
        /* istanbul ignore next */
        cov_1cw7uf323q().b[26][0]++;
        cov_1cw7uf323q().s[124]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_1cw7uf323q().b[26][1]++;
      }
      const appRevenue =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[125]++, kpiData.reduce((sum, kpi) => {
        /* istanbul ignore next */
        cov_1cw7uf323q().f[17]++;
        cov_1cw7uf323q().s[126]++;
        return sum + kpi.revenue.total;
      }, 0));
      const appDAU =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[127]++, kpiData.reduce((sum, kpi) => {
        /* istanbul ignore next */
        cov_1cw7uf323q().f[18]++;
        cov_1cw7uf323q().s[128]++;
        return sum + kpi.engagement.dau;
      }, 0));
      const appMAU =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[129]++, Math.max(...kpiData.map(kpi => {
        /* istanbul ignore next */
        cov_1cw7uf323q().f[19]++;
        cov_1cw7uf323q().s[130]++;
        return kpi.engagement.mau;
      })));
      const appARPU =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[131]++, kpiData.reduce((sum, kpi) => {
        /* istanbul ignore next */
        cov_1cw7uf323q().f[20]++;
        cov_1cw7uf323q().s[132]++;
        return sum + kpi.monetization.arpu;
      }, 0) / kpiData.length);
      const appPayerRate =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[133]++, kpiData.reduce((sum, kpi) => {
        /* istanbul ignore next */
        cov_1cw7uf323q().f[21]++;
        cov_1cw7uf323q().s[134]++;
        return sum + kpi.monetization.payerRate;
      }, 0) / kpiData.length);
      const appSafetyScore =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[135]++, kpiData.reduce((sum, kpi) => {
        /* istanbul ignore next */
        cov_1cw7uf323q().f[22]++;
        cov_1cw7uf323q().s[136]++;
        return sum + kpi.safety.safetyScore;
      }, 0) / kpiData.length);
      // Calculate growth rate
      const firstPeriodRevenue =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[137]++,
      /* istanbul ignore next */
      (cov_1cw7uf323q().b[27][0]++, kpiData[0]?.revenue.total) ||
      /* istanbul ignore next */
      (cov_1cw7uf323q().b[27][1]++, 0));
      const lastPeriodRevenue =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[138]++,
      /* istanbul ignore next */
      (cov_1cw7uf323q().b[28][0]++, kpiData[kpiData.length - 1]?.revenue.total) ||
      /* istanbul ignore next */
      (cov_1cw7uf323q().b[28][1]++, 0));
      const appGrowthRate =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[139]++, firstPeriodRevenue > 0 ?
      /* istanbul ignore next */
      (cov_1cw7uf323q().b[29][0]++, (lastPeriodRevenue - firstPeriodRevenue) / firstPeriodRevenue * 100) :
      /* istanbul ignore next */
      (cov_1cw7uf323q().b[29][1]++, 0));
      // Get alert data
      const appAlerts =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[140]++, await AnalyticsAlert_1.AnalyticsAlert.find({
        appId,
        createdAt: {
          $gte: from,
          $lte: to
        }
      }));
      const appActiveAlerts =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[141]++, appAlerts.filter(alert => {
        /* istanbul ignore next */
        cov_1cw7uf323q().f[23]++;
        cov_1cw7uf323q().s[142]++;
        return alert.status === 'active';
      }).length);
      const appSummary =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[143]++, {
        appId,
        totalRevenue: appRevenue,
        totalDAU: appDAU,
        totalMAU: appMAU,
        avgARPU: appARPU,
        avgPayerRate: appPayerRate,
        totalAlerts: appAlerts.length,
        activeAlerts: appActiveAlerts,
        safetyScore: appSafetyScore,
        growthRate: appGrowthRate
      });
      /* istanbul ignore next */
      cov_1cw7uf323q().s[144]++;
      apps.push(appSummary);
      // Accumulate totals
      /* istanbul ignore next */
      cov_1cw7uf323q().s[145]++;
      totalRevenue += appRevenue;
      /* istanbul ignore next */
      cov_1cw7uf323q().s[146]++;
      totalDAU += appDAU;
      /* istanbul ignore next */
      cov_1cw7uf323q().s[147]++;
      totalMAU += appMAU;
      /* istanbul ignore next */
      cov_1cw7uf323q().s[148]++;
      totalARPU += appARPU;
      /* istanbul ignore next */
      cov_1cw7uf323q().s[149]++;
      totalPayerRate += appPayerRate;
      /* istanbul ignore next */
      cov_1cw7uf323q().s[150]++;
      totalAlerts += appAlerts.length;
      /* istanbul ignore next */
      cov_1cw7uf323q().s[151]++;
      activeAlerts += appActiveAlerts;
      /* istanbul ignore next */
      cov_1cw7uf323q().s[152]++;
      totalSafetyScore += appSafetyScore;
      /* istanbul ignore next */
      cov_1cw7uf323q().s[153]++;
      totalGrowthRate += appGrowthRate;
    }
    /* istanbul ignore next */
    cov_1cw7uf323q().s[154]++;
    return {
      summary: {
        totalApps: apps.length,
        totalRevenue,
        totalDAU,
        totalMAU,
        avgARPU: apps.length > 0 ?
        /* istanbul ignore next */
        (cov_1cw7uf323q().b[30][0]++, totalARPU / apps.length) :
        /* istanbul ignore next */
        (cov_1cw7uf323q().b[30][1]++, 0),
        avgPayerRate: apps.length > 0 ?
        /* istanbul ignore next */
        (cov_1cw7uf323q().b[31][0]++, totalPayerRate / apps.length) :
        /* istanbul ignore next */
        (cov_1cw7uf323q().b[31][1]++, 0),
        totalAlerts,
        activeAlerts,
