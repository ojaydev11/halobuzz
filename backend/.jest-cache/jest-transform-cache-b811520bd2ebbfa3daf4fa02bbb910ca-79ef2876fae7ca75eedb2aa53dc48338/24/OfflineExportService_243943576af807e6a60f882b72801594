  async syncToDirectory(sourceDir, targetDir, year, month, weekNumber) {
    /* istanbul ignore next */
    cov_1cw7uf323q().f[30]++;
    cov_1cw7uf323q().s[180]++;
    try {
      const targetPath =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[181]++, path.join(targetDir, String(year), month));
      /* istanbul ignore next */
      cov_1cw7uf323q().s[182]++;
      if (!fs.existsSync(targetPath)) {
        /* istanbul ignore next */
        cov_1cw7uf323q().b[38][0]++;
        cov_1cw7uf323q().s[183]++;
        fs.mkdirSync(targetPath, {
          recursive: true
        });
      } else
      /* istanbul ignore next */
      {
        cov_1cw7uf323q().b[38][1]++;
      }
      const files =
      /* istanbul ignore next */
      (cov_1cw7uf323q().s[184]++, fs.readdirSync(sourceDir));
      /* istanbul ignore next */
      cov_1cw7uf323q().s[185]++;
      for (const file of files) {
        const sourceFile =
        /* istanbul ignore next */
        (cov_1cw7uf323q().s[186]++, path.join(sourceDir, file));
        const targetFile =
        /* istanbul ignore next */
        (cov_1cw7uf323q().s[187]++, path.join(targetPath, file));
        /* istanbul ignore next */
        cov_1cw7uf323q().s[188]++;
        fs.copyFileSync(sourceFile, targetFile);
        /* istanbul ignore next */
        cov_1cw7uf323q().s[189]++;
        logger_1.logger.info(`Synced file to ${targetDir}`, {
          file,
          targetPath
        });
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_1cw7uf323q().s[190]++;
      logger_1.logger.error(`Failed to sync to ${targetDir}:`, error);
      // Don't throw error to prevent sync failures from breaking the export process
    }
  }
  getWeekNumber(date) {
    /* istanbul ignore next */
    cov_1cw7uf323q().f[31]++;
    const firstDayOfYear =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[191]++, new Date(date.getFullYear(), 0, 1));
    const pastDaysOfYear =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[192]++, (date.getTime() - firstDayOfYear.getTime()) / 86400000);
    /* istanbul ignore next */
    cov_1cw7uf323q().s[193]++;
    return Math.ceil((pastDaysOfYear + firstDayOfYear.getDay() + 1) / 7);
  }
  /**
   * Start the cron job for weekly exports (every Sunday at 6:00 AM Sydney time)
   */
  startWeeklyExportCron() {
    /* istanbul ignore next */
    cov_1cw7uf323q().f[32]++;
    // Cron expression for every Sunday at 6:00 AM Sydney time
    // Note: This assumes the server is running in Sydney timezone
    const cronExpression =
    /* istanbul ignore next */
    (cov_1cw7uf323q().s[194]++, '0 6 * * 0'); // Every Sunday at 6:00 AM
    /* istanbul ignore next */
    cov_1cw7uf323q().s[195]++;
    cron.schedule(cronExpression, async () => {
      /* istanbul ignore next */
      cov_1cw7uf323q().f[33]++;
      cov_1cw7uf323q().s[196]++;
