/**
 * Security Monitoring Middleware
 */
/* istanbul ignore next */
cov_fq8lnhetc().s[255]++;
const securityMonitoring = (req, res, next) => {
  /* istanbul ignore next */
  cov_fq8lnhetc().f[42]++;
  const startTime =
  /* istanbul ignore next */
  (cov_fq8lnhetc().s[256]++, Date.now());
  // Monitor response time
  /* istanbul ignore next */
  cov_fq8lnhetc().s[257]++;
  res.on('finish', () => {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[43]++;
    const duration =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[258]++, Date.now() - startTime);
    // Log slow requests
    /* istanbul ignore next */
    cov_fq8lnhetc().s[259]++;
    if (duration > 5000) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[68][0]++;
      cov_fq8lnhetc().s[260]++;
      // 5 seconds
      SecurityLogging.logSecurityEvent('slow_request', req, {
        duration
      });
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[68][1]++;
    }
    // Log high status codes
    cov_fq8lnhetc().s[261]++;
    if (res.statusCode >= 400) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[69][0]++;
      cov_fq8lnhetc().s[262]++;
      SecurityLogging.logSecurityEvent('error_response', req, {
        statusCode: res.statusCode,
        duration
      });
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[69][1]++;
    }
  });
  /* istanbul ignore next */
  cov_fq8lnhetc().s[263]++;
  next();
};
/* istanbul ignore next */
cov_fq8lnhetc().s[264]++;
exports.securityMonitoring = securityMonitoring;
// Export requireAuth as an alias for enhancedAuthMiddleware
/* istanbul ignore next */
cov_fq8lnhetc().s[265]++;
