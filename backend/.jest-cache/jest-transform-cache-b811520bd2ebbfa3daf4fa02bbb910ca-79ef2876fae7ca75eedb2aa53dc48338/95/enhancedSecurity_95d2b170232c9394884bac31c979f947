const requireMFA = (req, res, next) => {
  /* istanbul ignore next */
  cov_fq8lnhetc().f[34]++;
  cov_fq8lnhetc().s[199]++;
  if (!req.user) {
    /* istanbul ignore next */
    cov_fq8lnhetc().b[52][0]++;
    cov_fq8lnhetc().s[200]++;
    res.status(401).json({
      error: 'Authentication required'
    });
    /* istanbul ignore next */
    cov_fq8lnhetc().s[201]++;
    return;
  } else
  /* istanbul ignore next */
  {
    cov_fq8lnhetc().b[52][1]++;
  }
  cov_fq8lnhetc().s[202]++;
  if (
  /* istanbul ignore next */
  (cov_fq8lnhetc().b[54][0]++, req.user.mfaEnabled) &&
  /* istanbul ignore next */
  (cov_fq8lnhetc().b[54][1]++, !req.user.mfaVerified)) {
    /* istanbul ignore next */
    cov_fq8lnhetc().b[53][0]++;
    cov_fq8lnhetc().s[203]++;
    res.status(403).json({
      error: 'MFA required',
      message: 'Multi-factor authentication is required for this action'
