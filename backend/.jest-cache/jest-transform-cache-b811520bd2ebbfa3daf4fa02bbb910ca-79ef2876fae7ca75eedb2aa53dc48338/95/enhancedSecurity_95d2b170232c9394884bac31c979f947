    /* istanbul ignore next */
    (cov_fq8lnhetc().s[32]++, cipher.getAuthTag());
    /* istanbul ignore next */
    cov_fq8lnhetc().s[33]++;
    return {
      encrypted,
      iv: iv.toString('hex'),
      tag: tag.toString('hex')
    };
  }
  static decrypt(encryptedData) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[5]++;
    const decipher =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[34]++, crypto_1.default.createDecipheriv(this.ALGORITHM, this.ENCRYPTION_KEY, Buffer.from(encryptedData.iv, 'hex')));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[35]++;
    decipher.setAuthTag(Buffer.from(encryptedData.tag, 'hex'));
    let decrypted =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[36]++, decipher.update(encryptedData.encrypted, 'hex', 'utf8'));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[37]++;
    decrypted += decipher.final('utf8');
    /* istanbul ignore next */
    cov_fq8lnhetc().s[38]++;
    return decrypted;
  }
  static async hashPassword(password) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[6]++;
    const saltRounds =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[39]++, 12); // Increased from default 10
    /* istanbul ignore next */
    cov_fq8lnhetc().s[40]++;
    return bcryptjs_1.default.hash(password, saltRounds);
  }
  static async verifyPassword(password, hash) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[7]++;
    cov_fq8lnhetc().s[41]++;
    return bcryptjs_1.default.compare(password, hash);
  }
  static generateSecureToken() {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[8]++;
    cov_fq8lnhetc().s[42]++;
    return crypto_1.default.randomBytes(32).toString('hex');
  }
}
