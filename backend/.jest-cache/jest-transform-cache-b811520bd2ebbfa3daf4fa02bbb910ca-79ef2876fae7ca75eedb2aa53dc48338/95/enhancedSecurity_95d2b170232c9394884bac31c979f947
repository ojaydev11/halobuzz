class IntegritySecurity {
  static generateChecksum(data) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[25]++;
    cov_fq8lnhetc().s[113]++;
    return crypto_1.default.createHash('sha256').update(data).digest('hex');
  }
  static verifyChecksum(data, expectedChecksum) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[26]++;
    const actualChecksum =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[114]++, this.generateChecksum(data));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[115]++;
    return actualChecksum === expectedChecksum;
  }
  static async validateFileUpload(file) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[27]++;
    // Check file type, size, and content
    const allowedTypes =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[116]++, ['image/jpeg', 'image/png', 'image/gif', 'image/webp']);
    const maxSize =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[117]++, 10 * 1024 * 1024); // 10MB
    /* istanbul ignore next */
    cov_fq8lnhetc().s[118]++;
    if (!allowedTypes.includes(file.mimetype)) {
      /* istanbul ignore next */
