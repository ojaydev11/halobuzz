/**
 * 4. INSECURE DESIGN - Enhanced security by design
 */
class SecureDesign {
  static async generateDeviceFingerprint(req) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[13]++;
    const components =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[73]++, [
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[21][0]++, req.headers['user-agent']) ||
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[21][1]++, ''),
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[22][0]++, req.headers['accept-language']) ||
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[22][1]++, ''),
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[23][0]++, req.headers['accept-encoding']) ||
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[23][1]++, ''),
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[24][0]++, req.ip) ||
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[24][1]++, ''),
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[25][0]++, req.headers['x-forwarded-for']) ||
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[25][1]++, '')]);
    const fingerprint =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[74]++, crypto_1.default.createHash('sha256').update(components.join('|')).digest('hex'));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[75]++;
    return fingerprint;
  }
  static async validateSession(req) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[14]++;
    cov_fq8lnhetc().s[76]++;
    if (!req.user?.sessionId) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[26][0]++;
      cov_fq8lnhetc().s[77]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[26][1]++;
    }
    const sessionData =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[78]++, await (0, redis_1.getCache)(`session:${req.user.sessionId}`));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[79]++;
    return sessionData !== null;
  }
  static async createSecureSession(userId, deviceFingerprint) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[15]++;
    const sessionId =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[80]++, crypto_1.default.randomUUID());
    const sessionData =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[81]++, {
      userId,
      deviceFingerprint,
      createdAt: new Date(),
      lastActivity: new Date(),
      mfaVerified: false
    });
    /* istanbul ignore next */
    cov_fq8lnhetc().s[82]++;
    await (0, redis_1.setCache)(`session:${sessionId}`, sessionData, 24 * 60 * 60); // 24 hours
    /* istanbul ignore next */
    cov_fq8lnhetc().s[83]++;
    return sessionId;
  }
}
/* istanbul ignore next */
cov_fq8lnhetc().s[84]++;
