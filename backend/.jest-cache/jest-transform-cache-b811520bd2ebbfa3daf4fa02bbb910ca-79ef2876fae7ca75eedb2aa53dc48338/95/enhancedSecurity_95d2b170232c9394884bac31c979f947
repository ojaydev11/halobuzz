/**
 * 3. INJECTION - Enhanced input validation and sanitization
 */
class InjectionPrevention {
  static sanitizeInput(input) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[9]++;
    cov_fq8lnhetc().s[44]++;
    if (typeof input === 'string') {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[10][0]++;
      cov_fq8lnhetc().s[45]++;
      // Remove potentially dangerous characters
      return input.replace(/[<>\"'%;()&+]/g, '').replace(/script/gi, '').replace(/javascript/gi, '').replace(/on\w+=/gi, '').trim();
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[10][1]++;
    }
    cov_fq8lnhetc().s[46]++;
    if (Array.isArray(input)) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[11][0]++;
      cov_fq8lnhetc().s[47]++;
      return input.map(item => {
        /* istanbul ignore next */
        cov_fq8lnhetc().f[10]++;
        cov_fq8lnhetc().s[48]++;
        return this.sanitizeInput(item);
      });
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[11][1]++;
    }
    cov_fq8lnhetc().s[49]++;
    if (
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[13][0]++, typeof input === 'object') &&
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[13][1]++, input !== null)) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[12][0]++;
      const sanitized =
      /* istanbul ignore next */
      (cov_fq8lnhetc().s[50]++, {});
      /* istanbul ignore next */
      cov_fq8lnhetc().s[51]++;
      for (const [key, value] of Object.entries(input)) {
        /* istanbul ignore next */
        cov_fq8lnhetc().s[52]++;
        sanitized[key] = this.sanitizeInput(value);
      }
      /* istanbul ignore next */
      cov_fq8lnhetc().s[53]++;
      return sanitized;
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[12][1]++;
    }
    cov_fq8lnhetc().s[54]++;
    return input;
  }
  static validateEmail(email) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[11]++;
    const emailRegex =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[55]++, /^[^\s@]+@[^\s@]+\.[^\s@]+$/);
    /* istanbul ignore next */
    cov_fq8lnhetc().s[56]++;
    return /* istanbul ignore next */(cov_fq8lnhetc().b[14][0]++, emailRegex.test(email)) &&
    /* istanbul ignore next */
    (cov_fq8lnhetc().b[14][1]++, email.length <= 254);
  }
  static validatePassword(password) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[12]++;
    const errors =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[57]++, []);
    /* istanbul ignore next */
    cov_fq8lnhetc().s[58]++;
    if (password.length < 8) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[15][0]++;
      cov_fq8lnhetc().s[59]++;
      errors.push('Password must be at least 8 characters long');
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[15][1]++;
    }
    cov_fq8lnhetc().s[60]++;
    if (!/[A-Z]/.test(password)) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[16][0]++;
      cov_fq8lnhetc().s[61]++;
      errors.push('Password must contain at least one uppercase letter');
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[16][1]++;
    }
    cov_fq8lnhetc().s[62]++;
    if (!/[a-z]/.test(password)) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[17][0]++;
      cov_fq8lnhetc().s[63]++;
      errors.push('Password must contain at least one lowercase letter');
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[17][1]++;
    }
    cov_fq8lnhetc().s[64]++;
    if (!/\d/.test(password)) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[18][0]++;
      cov_fq8lnhetc().s[65]++;
      errors.push('Password must contain at least one number');
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[18][1]++;
    }
    cov_fq8lnhetc().s[66]++;
    if (!/[!@#$%^&*(),.?":{}|<>]/.test(password)) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[19][0]++;
      cov_fq8lnhetc().s[67]++;
      errors.push('Password must contain at least one special character');
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[19][1]++;
    }
    // Check for common weak passwords
    const commonPasswords =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[68]++, ['password', '123456', 'qwerty', 'admin', 'letmein']);
    /* istanbul ignore next */
    cov_fq8lnhetc().s[69]++;
    if (commonPasswords.includes(password.toLowerCase())) {
      /* istanbul ignore next */
      cov_fq8lnhetc().b[20][0]++;
      cov_fq8lnhetc().s[70]++;
      errors.push('Password is too common');
    } else
    /* istanbul ignore next */
    {
      cov_fq8lnhetc().b[20][1]++;
    }
    cov_fq8lnhetc().s[71]++;
    return {
      isValid: errors.length === 0,
      errors
    };
  }
}
/* istanbul ignore next */
cov_fq8lnhetc().s[72]++;
