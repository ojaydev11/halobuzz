/**
 * 7. IDENTIFICATION AND AUTHENTICATION FAILURES - Enhanced MFA
 */
class MFAService {
  static generateTOTPSecret() {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[17]++;
    const randomBytes =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[88]++, crypto_1.default.randomBytes(20));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[89]++;
    return (0, base32_encode_1.default)(randomBytes, 'RFC4648', {
      padding: false
    });
  }
  static generateTOTPCode(secret) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[18]++;
    const epoch =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[90]++, Math.round(Date.now() / 1000.0));
    const time =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[91]++, Math.floor(epoch / 30));
    const key =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[92]++, Buffer.from((0, base32_decode_1.default)(secret, 'RFC4648')));
    const counter =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[93]++, Buffer.alloc(8));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[94]++;
    counter.writeUIntBE(time, 0, 8);
    const hmac =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[95]++, crypto_1.default.createHmac('sha1', key));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[96]++;
    hmac.update(counter);
    const digest =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[97]++, hmac.digest());
    const offset =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[98]++, digest[digest.length - 1] & 0xf);
    const code =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[99]++, (digest[offset] & 0x7f) << 24 | (digest[offset + 1] & 0xff) << 16 | (digest[offset + 2] & 0xff) << 8 | digest[offset + 3] & 0xff);
    /* istanbul ignore next */
    cov_fq8lnhetc().s[100]++;
    return (code % 1000000).toString().padStart(6, '0');
  }
  static verifyTOTPCode(secret, code) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[19]++;
    const generatedCode =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[101]++, this.generateTOTPCode(secret));
    /* istanbul ignore next */
    cov_fq8lnhetc().s[102]++;
    return generatedCode === code;
  }
  static generateBackupCodes() {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[20]++;
    const codes =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[103]++, []);
    /* istanbul ignore next */
    cov_fq8lnhetc().s[104]++;
    for (let i =
    /* istanbul ignore next */
    (cov_fq8lnhetc().s[105]++, 0); i < 10; i++) {
      /* istanbul ignore next */
      cov_fq8lnhetc().s[106]++;
      codes.push(crypto_1.default.randomBytes(4).toString('hex').toUpperCase());
    }
    /* istanbul ignore next */
    cov_fq8lnhetc().s[107]++;
    return codes;
  }
  static generateSecret() {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[21]++;
    cov_fq8lnhetc().s[108]++;
    return this.generateTOTPSecret();
  }
  static getQRCodeUrl(email, secret) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[22]++;
    cov_fq8lnhetc().s[109]++;
    return `otpauth://totp/HaloBuzz:${email}?secret=${secret}&issuer=HaloBuzz`;
  }
  static verifyToken(secret, token) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[23]++;
    cov_fq8lnhetc().s[110]++;
    return this.verifyTOTPCode(secret, token);
  }
  static verifyBackupCode(backupCodes, code) {
    /* istanbul ignore next */
    cov_fq8lnhetc().f[24]++;
    cov_fq8lnhetc().s[111]++;
    return backupCodes.includes(code.toUpperCase());
  }
}
