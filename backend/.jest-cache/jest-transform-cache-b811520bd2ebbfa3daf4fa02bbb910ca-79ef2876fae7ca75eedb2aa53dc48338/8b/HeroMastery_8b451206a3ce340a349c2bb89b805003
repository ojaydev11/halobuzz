heroMasterySchema.statics.getHeroLeaderboard = function (heroId, limit =
/* istanbul ignore next */
(cov_1dcbphnwga().b[38][0]++, 100)) {
  /* istanbul ignore next */
  cov_1dcbphnwga().f[12]++;
  cov_1dcbphnwga().s[111]++;
  return this.find({
    heroId
  }).sort({
    masteryLevel: -1,
    totalXP: -1
  }).limit(limit).populate('userId', 'username avatar');
};
