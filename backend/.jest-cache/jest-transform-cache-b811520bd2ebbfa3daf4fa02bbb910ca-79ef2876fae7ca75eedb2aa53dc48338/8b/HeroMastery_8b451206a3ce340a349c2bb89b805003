  // Special achievements
  cov_1dcbphnwga().s[70]++;
  if (gameStats.firstBlood) {
    /* istanbul ignore next */
    cov_1dcbphnwga().b[27][0]++;
    cov_1dcbphnwga().s[71]++;
    this.stats.firstBloodCount++;
  } else
  /* istanbul ignore next */
  {
    cov_1dcbphnwga().b[27][1]++;
  }
  cov_1dcbphnwga().s[72]++;
  if (gameStats.clutch) {
    /* istanbul ignore next */
    cov_1dcbphnwga().b[28][0]++;
    cov_1dcbphnwga().s[73]++;
    this.stats.clutchPlays++;
  } else
  /* istanbul ignore next */
  {
    cov_1dcbphnwga().b[28][1]++;
  }
  // Calculate XP earned
  let xpEarned =
  /* istanbul ignore next */
  (cov_1dcbphnwga().s[74]++, 100); // Base XP
  /* istanbul ignore next */
  cov_1dcbphnwga().s[75]++;
  xpEarned += gameStats.kills * 10;
  /* istanbul ignore next */
  cov_1dcbphnwga().s[76]++;
  xpEarned += gameStats.assists * 5;
  /* istanbul ignore next */
  cov_1dcbphnwga().s[77]++;
  xpEarned += gameStats.damage / 100;
  /* istanbul ignore next */
  cov_1dcbphnwga().s[78]++;
  if (gameStats.result === 'win') {
    /* istanbul ignore next */
    cov_1dcbphnwga().b[29][0]++;
    cov_1dcbphnwga().s[79]++;
    xpEarned *= 1.5;
  } else
  /* istanbul ignore next */
  {
    cov_1dcbphnwga().b[29][1]++;
  }
  cov_1dcbphnwga().s[80]++;
  this.totalXP += xpEarned;
  // Check for level up
  const xpPerLevel =
  /* istanbul ignore next */
  (cov_1dcbphnwga().s[81]++, this.masteryLevel * 500); // Progressive XP requirement
  /* istanbul ignore next */
  cov_1dcbphnwga().s[82]++;
  while (
  /* istanbul ignore next */
  (cov_1dcbphnwga().b[30][0]++, this.totalXP >= xpPerLevel) &&
  /* istanbul ignore next */
  (cov_1dcbphnwga().b[30][1]++, this.masteryLevel < 100)) {
    /* istanbul ignore next */
    cov_1dcbphnwga().s[83]++;
    this.masteryLevel++;
    /* istanbul ignore next */
    cov_1dcbphnwga().s[84]++;
    this.checkMilestones();
  }
  // Update XP to next level
  /* istanbul ignore next */
  cov_1dcbphnwga().s[85]++;
  this.xpToNextLevel = this.masteryLevel * 500 - this.totalXP % (this.masteryLevel * 500);
  // Add to recent games
  /* istanbul ignore next */
  cov_1dcbphnwga().s[86]++;
  this.recentGames.push({
    matchId: `match_${Date.now()}`,
    result: gameStats.result,
    kills: gameStats.kills,
    deaths: gameStats.deaths,
    assists: gameStats.assists,
    damage: gameStats.damage,
    healing: gameStats.healing,
    duration: gameStats.duration,
    xpEarned,
    playedAt: new Date()
  });
  // Update best performances
  const kda =
  /* istanbul ignore next */
  (cov_1dcbphnwga().s[87]++, gameStats.deaths > 0 ?
  /* istanbul ignore next */
  (cov_1dcbphnwga().b[31][0]++, (gameStats.kills + gameStats.assists) / gameStats.deaths) :
  /* istanbul ignore next */
  (cov_1dcbphnwga().b[31][1]++, gameStats.kills + gameStats.assists));
  /* istanbul ignore next */
  cov_1dcbphnwga().s[88]++;
  if (gameStats.kills > this.bestPerformances.mostKills.kills) {
    /* istanbul ignore next */
    cov_1dcbphnwga().b[32][0]++;
    cov_1dcbphnwga().s[89]++;
    this.bestPerformances.mostKills = {
      kills: gameStats.kills,
      matchId: `match_${Date.now()}`,
      date: new Date()
    };
  } else
  /* istanbul ignore next */
  {
    cov_1dcbphnwga().b[32][1]++;
  }
  cov_1dcbphnwga().s[90]++;
  if (gameStats.damage > this.bestPerformances.mostDamage.damage) {
    /* istanbul ignore next */
    cov_1dcbphnwga().b[33][0]++;
    cov_1dcbphnwga().s[91]++;
    this.bestPerformances.mostDamage = {
      damage: gameStats.damage,
      matchId: `match_${Date.now()}`,
      date: new Date()
    };
  } else
  /* istanbul ignore next */
  {
    cov_1dcbphnwga().b[33][1]++;
  }
  cov_1dcbphnwga().s[92]++;
  if (kda > this.bestPerformances.highestKDA.kda) {
    /* istanbul ignore next */
    cov_1dcbphnwga().b[34][0]++;
    cov_1dcbphnwga().s[93]++;
    this.bestPerformances.highestKDA = {
      kda,
      matchId: `match_${Date.now()}`,
      date: new Date()
    };
  } else
  /* istanbul ignore next */
  {
    cov_1dcbphnwga().b[34][1]++;
  }
  cov_1dcbphnwga().s[94]++;
  this.lastPlayedAt = new Date();
  /* istanbul ignore next */
  cov_1dcbphnwga().s[95]++;
  return xpEarned;
};
// Method to check and unlock milestones
/* istanbul ignore next */
cov_1dcbphnwga().s[96]++;
heroMasterySchema.methods.checkMilestones = function () {
  /* istanbul ignore next */
  cov_1dcbphnwga().f[11]++;
  const unlockedRewards =
  /* istanbul ignore next */
  (cov_1dcbphnwga().s[97]++, []);
  /* istanbul ignore next */
  cov_1dcbphnwga().s[98]++;
  for (const milestone of this.milestones) {
    /* istanbul ignore next */
    cov_1dcbphnwga().s[99]++;
    if (
    /* istanbul ignore next */
    (cov_1dcbphnwga().b[36][0]++, this.masteryLevel >= milestone.level) &&
    /* istanbul ignore next */
    (cov_1dcbphnwga().b[36][1]++, !milestone.achievedAt)) {
      /* istanbul ignore next */
      cov_1dcbphnwga().b[35][0]++;
      cov_1dcbphnwga().s[100]++;
      milestone.achievedAt = new Date();
      // Add unlocks based on reward type
      /* istanbul ignore next */
      cov_1dcbphnwga().s[101]++;
      switch (milestone.reward.type) {
        case 'skin':
          /* istanbul ignore next */
          cov_1dcbphnwga().b[37][0]++;
          cov_1dcbphnwga().s[102]++;
          this.unlocks.skins.push(milestone.reward.itemId);
          /* istanbul ignore next */
          cov_1dcbphnwga().s[103]++;
          break;
        case 'emote':
          /* istanbul ignore next */
          cov_1dcbphnwga().b[37][1]++;
          cov_1dcbphnwga().s[104]++;
          this.unlocks.emotes.push(milestone.reward.itemId);
          /* istanbul ignore next */
          cov_1dcbphnwga().s[105]++;
          break;
        case 'title':
          /* istanbul ignore next */
          cov_1dcbphnwga().b[37][2]++;
          cov_1dcbphnwga().s[106]++;
          this.unlocks.titles.push(milestone.reward.itemId);
          /* istanbul ignore next */
          cov_1dcbphnwga().s[107]++;
          break;
      }
      /* istanbul ignore next */
      cov_1dcbphnwga().s[108]++;
      unlockedRewards.push(milestone.reward.name);
    } else
    /* istanbul ignore next */
    {
      cov_1dcbphnwga().b[35][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1dcbphnwga().s[109]++;
  return unlockedRewards;
};
