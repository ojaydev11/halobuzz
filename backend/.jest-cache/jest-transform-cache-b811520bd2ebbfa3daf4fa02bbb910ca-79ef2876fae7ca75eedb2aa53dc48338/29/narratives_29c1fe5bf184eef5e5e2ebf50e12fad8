      /* istanbul ignore next */
      cov_fft7bq4vy().b[33][0]++;
      const topInsights =
      /* istanbul ignore next */
      (cov_fft7bq4vy().s[78]++, insights.slice(0, 3));
      /* istanbul ignore next */
      cov_fft7bq4vy().s[79]++;
      narrative.push(`📈 Key Insights: ${topInsights.map(i => {
        /* istanbul ignore next */
        cov_fft7bq4vy().f[11]++;
        cov_fft7bq4vy().s[80]++;
        return i.description;
      }).join('; ')}`);
    } else
    /* istanbul ignore next */
    {
      cov_fft7bq4vy().b[33][1]++;
    }
    // Payment method breakdown
    const paymentMethods =
    /* istanbul ignore next */
    (cov_fft7bq4vy().s[81]++, Object.entries(kpis.revenue.byPaymentMethod).filter(([_, amount]) => {
      /* istanbul ignore next */
      cov_fft7bq4vy().f[12]++;
      cov_fft7bq4vy().s[82]++;
      return amount > 0;
    }).sort(([_, a], [__, b]) => {
      /* istanbul ignore next */
      cov_fft7bq4vy().f[13]++;
      cov_fft7bq4vy().s[83]++;
      return b - a;
    }));
    /* istanbul ignore next */
    cov_fft7bq4vy().s[84]++;
    if (paymentMethods.length > 0) {
      /* istanbul ignore next */
      cov_fft7bq4vy().b[34][0]++;
      const topMethod =
      /* istanbul ignore next */
      (cov_fft7bq4vy().s[85]++, paymentMethods[0]);
      /* istanbul ignore next */
      cov_fft7bq4vy().s[86]++;
      narrative.push(`💳 Payment Leader: ${topMethod[0].toUpperCase()} dominates with ${topMethod[1].toLocaleString()} NPR (${(topMethod[1] / kpis.revenue.total * 100).toFixed(1)}% of total)`);
    } else
    /* istanbul ignore next */
    {
      cov_fft7bq4vy().b[34][1]++;
    }
    cov_fft7bq4vy().s[87]++;
    return narrative.slice(0, 8); // Limit to 8 bullet points
  }
  /**
   * Calculate confidence score based on data quality
   */
  calculateConfidence(current, previous) {
    /* istanbul ignore next */
    cov_fft7bq4vy().f[14]++;
    cov_fft7bq4vy().s[88]++;
    if (previous === 0) {
      /* istanbul ignore next */
      cov_fft7bq4vy().b[35][0]++;
      cov_fft7bq4vy().s[89]++;
      return 50;
    } else
    /* istanbul ignore next */
    {
      cov_fft7bq4vy().b[35][1]++;
    } // Low confidence for zero baseline
    const ratio =
    /* istanbul ignore next */
    (cov_fft7bq4vy().s[90]++, current / previous);
    /* istanbul ignore next */
    cov_fft7bq4vy().s[91]++;
    if (
    /* istanbul ignore next */
    (cov_fft7bq4vy().b[37][0]++, ratio < 0.1) ||
    /* istanbul ignore next */
    (cov_fft7bq4vy().b[37][1]++, ratio > 10)) {
      /* istanbul ignore next */
      cov_fft7bq4vy().b[36][0]++;
      cov_fft7bq4vy().s[92]++;
      return 60;
    } else
    /* istanbul ignore next */
    {
      cov_fft7bq4vy().b[36][1]++;
    } // Low confidence for extreme changes
    cov_fft7bq4vy().s[93]++;
    if (
    /* istanbul ignore next */
    (cov_fft7bq4vy().b[39][0]++, ratio < 0.5) ||
    /* istanbul ignore next */
    (cov_fft7bq4vy().b[39][1]++, ratio > 2)) {
      /* istanbul ignore next */
      cov_fft7bq4vy().b[38][0]++;
      cov_fft7bq4vy().s[94]++;
      return 75;
    } else
    /* istanbul ignore next */
    {
      cov_fft7bq4vy().b[38][1]++;
    } // Medium confidence for large changes
    cov_fft7bq4vy().s[95]++;
    return 90; // High confidence for reasonable changes
  }
}
/* istanbul ignore next */
cov_fft7bq4vy().s[96]++;
exports.NarrativeGenerator = NarrativeGenerator;
/* istanbul ignore next */
cov_fft7bq4vy().s[97]++;
exports.default = NarrativeGenerator;