      }
      cov_2cbfafailx().s[34]++;
      if (filters.ipAddress) {
        /* istanbul ignore next */
        cov_2cbfafailx().b[23][0]++;
        cov_2cbfafailx().s[35]++;
        filteredEvents = filteredEvents.filter(e => {
          /* istanbul ignore next */
          cov_2cbfafailx().f[13]++;
          cov_2cbfafailx().s[36]++;
          return e.ipAddress === filters.ipAddress;
        });
      } else
      /* istanbul ignore next */
      {
        cov_2cbfafailx().b[23][1]++;
      }
      cov_2cbfafailx().s[37]++;
      if (filters.startDate) {
        /* istanbul ignore next */
        cov_2cbfafailx().b[24][0]++;
        cov_2cbfafailx().s[38]++;
        filteredEvents = filteredEvents.filter(e => {
          /* istanbul ignore next */
          cov_2cbfafailx().f[14]++;
          cov_2cbfafailx().s[39]++;
          return e.timestamp >= filters.startDate;
        });
      } else
      /* istanbul ignore next */
      {
        cov_2cbfafailx().b[24][1]++;
      }
      cov_2cbfafailx().s[40]++;
      if (filters.endDate) {
        /* istanbul ignore next */
        cov_2cbfafailx().b[25][0]++;
        cov_2cbfafailx().s[41]++;
        filteredEvents = filteredEvents.filter(e => {
          /* istanbul ignore next */
          cov_2cbfafailx().f[15]++;
          cov_2cbfafailx().s[42]++;
          return e.timestamp <= filters.endDate;
        });
      } else
      /* istanbul ignore next */
      {
        cov_2cbfafailx().b[25][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_2cbfafailx().b[19][1]++;
    }
    // Sort by timestamp (newest first)
    cov_2cbfafailx().s[43]++;
    filteredEvents.sort((a, b) => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[16]++;
      cov_2cbfafailx().s[44]++;
      return b.timestamp.getTime() - a.timestamp.getTime();
    });
    // Apply limit
    /* istanbul ignore next */
    cov_2cbfafailx().s[45]++;
    if (filters?.limit) {
      /* istanbul ignore next */
      cov_2cbfafailx().b[26][0]++;
      cov_2cbfafailx().s[46]++;
      filteredEvents = filteredEvents.slice(0, filters.limit);
    } else
    /* istanbul ignore next */
    {
      cov_2cbfafailx().b[26][1]++;
    }
    cov_2cbfafailx().s[47]++;
    return filteredEvents;
  }
  getRecentEvents(limit =
  /* istanbul ignore next */
  (cov_2cbfafailx().b[27][0]++, 50)) {
    /* istanbul ignore next */
    cov_2cbfafailx().f[17]++;
    const events =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[48]++, [...this.events].sort((a, b) => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[18]++;
      cov_2cbfafailx().s[49]++;
      return b.timestamp.getTime() - a.timestamp.getTime();
    }).slice(0, limit));
    /* istanbul ignore next */
    cov_2cbfafailx().s[50]++;
    return events;
  }
  getUnresolvedAlerts() {
    /* istanbul ignore next */
    cov_2cbfafailx().f[19]++;
    cov_2cbfafailx().s[51]++;
    return this.events.filter(e => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[20]++;
      cov_2cbfafailx().s[52]++;
      return /* istanbul ignore next */(cov_2cbfafailx().b[28][0]++, !e.resolved) && (
      /* istanbul ignore next */
      (cov_2cbfafailx().b[28][1]++, e.severity === 'HIGH') ||
      /* istanbul ignore next */
      (cov_2cbfafailx().b[28][2]++, e.severity === 'CRITICAL'));
    });
  }
  resolveAlert(alertId, _resolvedBy) {
    /* istanbul ignore next */
    cov_2cbfafailx().f[21]++;
    // Synchronously mark as resolved
    const event =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[53]++, this.events.find(e => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[22]++;
      cov_2cbfafailx().s[54]++;
      return e.id === alertId;
    }));
    /* istanbul ignore next */
    cov_2cbfafailx().s[55]++;
    if (event) {
      /* istanbul ignore next */
      cov_2cbfafailx().b[29][0]++;
      cov_2cbfafailx().s[56]++;
      event.resolved = true;
      /* istanbul ignore next */
      cov_2cbfafailx().s[57]++;
      logger_1.logger.info('Security alert resolved', {
        alertId
      });
      /* istanbul ignore next */
      cov_2cbfafailx().s[58]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_2cbfafailx().b[29][1]++;
    }
    cov_2cbfafailx().s[59]++;
    return false;
  }
  getSecurityStats() {
    /* istanbul ignore next */
    cov_2cbfafailx().f[23]++;
    const byType =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[60]++, {});
    const bySeverity =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[61]++, {});
    /* istanbul ignore next */
    cov_2cbfafailx().s[62]++;
    for (const e of this.events) {
      /* istanbul ignore next */
      cov_2cbfafailx().s[63]++;
      byType[e.type] = (
      /* istanbul ignore next */
      (cov_2cbfafailx().b[30][0]++, byType[e.type]) ||
      /* istanbul ignore next */
      (cov_2cbfafailx().b[30][1]++, 0)) + 1;
      /* istanbul ignore next */
      cov_2cbfafailx().s[64]++;
      bySeverity[e.severity] = (
      /* istanbul ignore next */
      (cov_2cbfafailx().b[31][0]++, bySeverity[e.severity]) ||
      /* istanbul ignore next */
      (cov_2cbfafailx().b[31][1]++, 0)) + 1;
    }
    /* istanbul ignore next */
    cov_2cbfafailx().s[65]++;
    return {
      totalEvents: this.events.length,
      unresolved: this.events.filter(e => {
        /* istanbul ignore next */
        cov_2cbfafailx().f[24]++;
        cov_2cbfafailx().s[66]++;
        return !e.resolved;
      }).length,
      byType,
      bySeverity
    };
  }
  cleanup(days =
  /* istanbul ignore next */
  (cov_2cbfafailx().b[32][0]++, 30)) {
    /* istanbul ignore next */
    cov_2cbfafailx().f[25]++;
    const cutoff =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[67]++, Date.now() - days * 24 * 60 * 60 * 1000);
    /* istanbul ignore next */
    cov_2cbfafailx().s[68]++;
    this.events = this.events.filter(e => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[26]++;
      cov_2cbfafailx().s[69]++;
      return e.timestamp.getTime() >= cutoff;
    });
    /* istanbul ignore next */
    cov_2cbfafailx().s[70]++;
    logger_1.logger.info('Security events cleanup performed', {
      remaining: this.events.length,
      days
    });
  }
  /**
   * Resolve a security event
   */
  async resolveSecurityEvent(eventId) {
    /* istanbul ignore next */
    cov_2cbfafailx().f[27]++;
    const event =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[71]++, this.events.find(e => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[28]++;
      cov_2cbfafailx().s[72]++;
      return e.id === eventId;
    }));
    /* istanbul ignore next */
    cov_2cbfafailx().s[73]++;
    if (event) {
      /* istanbul ignore next */
      cov_2cbfafailx().b[33][0]++;
      cov_2cbfafailx().s[74]++;
      event.resolved = true;
      /* istanbul ignore next */
      cov_2cbfafailx().s[75]++;
      logger_1.logger.info('Security event resolved', {
        eventId
      });
      /* istanbul ignore next */
      cov_2cbfafailx().s[76]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_2cbfafailx().b[33][1]++;
    }
    cov_2cbfafailx().s[77]++;
    return false;
  }
  /**
   * Get security dashboard data
   */
  async getSecurityDashboard() {
    /* istanbul ignore next */
    cov_2cbfafailx().f[29]++;
    const unresolvedEvents =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[78]++, this.events.filter(e => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[30]++;
      cov_2cbfafailx().s[79]++;
      return !e.resolved;
    }));
    const eventsByType =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[80]++, this.events.reduce((acc, event) => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[31]++;
      cov_2cbfafailx().s[81]++;
      acc[event.type] = (
      /* istanbul ignore next */
      (cov_2cbfafailx().b[34][0]++, acc[event.type]) ||
      /* istanbul ignore next */
      (cov_2cbfafailx().b[34][1]++, 0)) + 1;
      /* istanbul ignore next */
      cov_2cbfafailx().s[82]++;
      return acc;
    }, {}));
    const eventsBySeverity =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[83]++, this.events.reduce((acc, event) => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[32]++;
      cov_2cbfafailx().s[84]++;
      acc[event.severity] = (
      /* istanbul ignore next */
      (cov_2cbfafailx().b[35][0]++, acc[event.severity]) ||
      /* istanbul ignore next */
      (cov_2cbfafailx().b[35][1]++, 0)) + 1;
      /* istanbul ignore next */
      cov_2cbfafailx().s[85]++;
      return acc;
    }, {}));
    const recentEvents =
    /* istanbul ignore next */
    (cov_2cbfafailx().s[86]++, this.events.sort((a, b) => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[33]++;
      cov_2cbfafailx().s[87]++;
      return b.timestamp.getTime() - a.timestamp.getTime();
    }).slice(0, 10));
    /* istanbul ignore next */
    cov_2cbfafailx().s[88]++;
    return {
      totalEvents: this.events.length,
      unresolvedEvents: unresolvedEvents.length,
      eventsByType,
      eventsBySeverity,
      recentEvents
    };
  }
  /**
   * Middleware to log request security events
   */
  securityMiddleware() {
    /* istanbul ignore next */
    cov_2cbfafailx().f[34]++;
    cov_2cbfafailx().s[89]++;
    return (req, res, next) => {
      /* istanbul ignore next */
      cov_2cbfafailx().f[35]++;
      // Log suspicious patterns
      const suspiciousPatterns =
      /* istanbul ignore next */
      (cov_2cbfafailx().s[90]++, [/\.\.\//,
      // Directory traversal
      /<script/i,
      // XSS attempts
      /union.*select/i,
      // SQL injection
      /javascript:/i // JavaScript injection
      ]);
      const url =
      /* istanbul ignore next */
      (cov_2cbfafailx().s[91]++, req.url);
      const userAgent =
      /* istanbul ignore next */
      (cov_2cbfafailx().s[92]++,
      /* istanbul ignore next */
      (cov_2cbfafailx().b[36][0]++, req.get('User-Agent')) ||
      /* istanbul ignore next */
      (cov_2cbfafailx().b[36][1]++, ''));
      /* istanbul ignore next */
      cov_2cbfafailx().s[93]++;
      for (const pattern of suspiciousPatterns) {
        /* istanbul ignore next */
        cov_2cbfafailx().s[94]++;
        if (
        /* istanbul ignore next */
        (cov_2cbfafailx().b[38][0]++, pattern.test(url)) ||
        /* istanbul ignore next */
        (cov_2cbfafailx().b[38][1]++, pattern.test(userAgent))) {
          /* istanbul ignore next */
          cov_2cbfafailx().b[37][0]++;
          cov_2cbfafailx().s[95]++;
          this.logSecurityEvent({
            type: 'SUSPICIOUS_ACTIVITY',
            severity: 'HIGH',
            ipAddress:
            /* istanbul ignore next */
            (cov_2cbfafailx().b[39][0]++, req.ip) ||
            /* istanbul ignore next */
            (cov_2cbfafailx().b[39][1]++, 'unknown'),
            userAgent,
            details: {
              url,
              pattern: pattern.toString()
            }
          });
        } else
        /* istanbul ignore next */
        {
          cov_2cbfafailx().b[37][1]++;
        }
      }
      /* istanbul ignore next */
      cov_2cbfafailx().s[96]++;
      next();
    };
  }
}
