    cov_gxuoextdz().s[64]++;
    // Log rate limiting events
    if (this.statusCode === 429) {
      /* istanbul ignore next */
      cov_gxuoextdz().b[20][0]++;
      const userId =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[65]++, req.user?.userId);
      const username =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[66]++, req.user?.username);
      /* istanbul ignore next */
      cov_gxuoextdz().s[67]++;
      securityMonitoringService_1.securityMonitoring.logEvent({
        type: 'rate_limit',
        severity: 'medium',
        source: {
          ip:
          /* istanbul ignore next */
          (cov_gxuoextdz().b[21][0]++, req.ip) ||
          /* istanbul ignore next */
          (cov_gxuoextdz().b[21][1]++, 'unknown'),
          userAgent:
          /* istanbul ignore next */
          (cov_gxuoextdz().b[22][0]++, req.get('User-Agent')) ||
          /* istanbul ignore next */
          (cov_gxuoextdz().b[22][1]++, 'unknown'),
          userId,
          username
        },
        details: {
          endpoint: req.path,
          method: req.method,
          payload: req.body,
          error:
          /* istanbul ignore next */
          (cov_gxuoextdz().b[23][0]++, data.error) ||
          /* istanbul ignore next */
          (cov_gxuoextdz().b[23][1]++, data.message),
          riskScore: 30
        },
        metadata: {
          requestId: req.headers['x-request-id'],
          sessionId: req.headers['x-session-id'],
          deviceId: req.headers['x-device-id']
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_gxuoextdz().b[20][1]++;
    }
    cov_gxuoextdz().s[68]++;
    return originalJson.call(this, data);
  };
  /* istanbul ignore next */
  cov_gxuoextdz().s[69]++;
  next();
};
/* istanbul ignore next */
cov_gxuoextdz().s[70]++;
exports.rateLimitMonitoringMiddleware = rateLimitMonitoringMiddleware;
/**
 * Middleware to detect suspicious patterns in requests
 */
/* istanbul ignore next */
cov_gxuoextdz().s[71]++;
const suspiciousPatternDetectionMiddleware = (req, res, next) => {
  /* istanbul ignore next */
  cov_gxuoextdz().f[12]++;
  cov_gxuoextdz().s[72]++;
  try {
    const suspiciousPatterns =
    /* istanbul ignore next */
    (cov_gxuoextdz().s[73]++, [/<script[^>]*>.*?<\/script>/gi, /javascript:/gi, /vbscript:/gi, /onload\s*=/gi, /onerror\s*=/gi, /eval\s*\(/gi, /document\.cookie/gi, /window\.location/gi, /\$ne\s*:\s*null/gi, /\$where\s*:/gi, /union\s+select/gi, /drop\s+table/gi, /delete\s+from/gi, /insert\s+into/gi, /update\s+set/gi]);
    // Check request body
    /* istanbul ignore next */
    cov_gxuoextdz().s[74]++;
    if (req.body) {
      /* istanbul ignore next */
      cov_gxuoextdz().b[24][0]++;
      const bodyStr =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[75]++, JSON.stringify(req.body));
      /* istanbul ignore next */
      cov_gxuoextdz().s[76]++;
      for (const pattern of suspiciousPatterns) {
        /* istanbul ignore next */
        cov_gxuoextdz().s[77]++;
        if (pattern.test(bodyStr)) {
          /* istanbul ignore next */
          cov_gxuoextdz().b[25][0]++;
          const userId =
          /* istanbul ignore next */
          (cov_gxuoextdz().s[78]++, req.user?.userId);
          const username =
          /* istanbul ignore next */
          (cov_gxuoextdz().s[79]++, req.user?.username);
          /* istanbul ignore next */
          cov_gxuoextdz().s[80]++;
          if (
          /* istanbul ignore next */
          (cov_gxuoextdz().b[27][0]++, pattern.source.includes('script')) ||
          /* istanbul ignore next */
          (cov_gxuoextdz().b[27][1]++, pattern.source.includes('javascript'))) {
            /* istanbul ignore next */
            cov_gxuoextdz().b[26][0]++;
            cov_gxuoextdz().s[81]++;
            securityMonitoringService_1.securityMonitoring.logXSSEvent(req, req.body, userId, username);
          } else {
            /* istanbul ignore next */
            cov_gxuoextdz().b[26][1]++;
            cov_gxuoextdz().s[82]++;
            securityMonitoringService_1.securityMonitoring.logInjectionEvent('nosql_injection', req, req.body, userId, username);
          }
          /* istanbul ignore next */
          cov_gxuoextdz().s[83]++;
          logger_1.logger.warn('Suspicious pattern detected', {
            pattern: pattern.source,
            path: req.path,
            ip: req.ip,
            userId,
            username
          });
          /* istanbul ignore next */
          cov_gxuoextdz().s[84]++;
          return res.status(400).json({
            success: false,
            error: 'Invalid request data'
          });
        } else
        /* istanbul ignore next */
        {
          cov_gxuoextdz().b[25][1]++;
        }
      }
    } else
    /* istanbul ignore next */
    {
      cov_gxuoextdz().b[24][1]++;
    }
    // Check query parameters
    cov_gxuoextdz().s[85]++;
    if (req.query) {
      /* istanbul ignore next */
      cov_gxuoextdz().b[28][0]++;
      const queryStr =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[86]++, JSON.stringify(req.query));
      /* istanbul ignore next */
      cov_gxuoextdz().s[87]++;
      for (const pattern of suspiciousPatterns) {
        /* istanbul ignore next */
        cov_gxuoextdz().s[88]++;
        if (pattern.test(queryStr)) {
          /* istanbul ignore next */
          cov_gxuoextdz().b[29][0]++;
          const userId =
          /* istanbul ignore next */
          (cov_gxuoextdz().s[89]++, req.user?.userId);
          const username =
          /* istanbul ignore next */
          (cov_gxuoextdz().s[90]++, req.user?.username);
          /* istanbul ignore next */
          cov_gxuoextdz().s[91]++;
          securityMonitoringService_1.securityMonitoring.logInjectionEvent('nosql_injection', req, req.query, userId, username);
          /* istanbul ignore next */
          cov_gxuoextdz().s[92]++;
          logger_1.logger.warn('Suspicious pattern detected in query', {
            pattern: pattern.source,
            path: req.path,
            ip: req.ip,
            userId,
            username
          });
          /* istanbul ignore next */
          cov_gxuoextdz().s[93]++;
          return res.status(400).json({
            success: false,
            error: 'Invalid query parameters'
          });
        } else
        /* istanbul ignore next */
        {
          cov_gxuoextdz().b[29][1]++;
        }
      }
    } else
    /* istanbul ignore next */
    {
      cov_gxuoextdz().b[28][1]++;
    }
    cov_gxuoextdz().s[94]++;
    next();
  } catch (error) {
    /* istanbul ignore next */
    cov_gxuoextdz().s[95]++;
    logger_1.logger.error('Suspicious pattern detection error:', error);
    /* istanbul ignore next */
    cov_gxuoextdz().s[96]++;
    next();
  }
};
/* istanbul ignore next */
cov_gxuoextdz().s[97]++;
exports.suspiciousPatternDetectionMiddleware = suspiciousPatternDetectionMiddleware;
/**
 * Middleware to monitor file uploads
 */
/* istanbul ignore next */
cov_gxuoextdz().s[98]++;
const fileUploadMonitoringMiddleware = (req, res, next) => {
  /* istanbul ignore next */
  cov_gxuoextdz().f[13]++;
  const originalJson =
  /* istanbul ignore next */
  (cov_gxuoextdz().s[99]++, res.json);
  const originalStatus =
  /* istanbul ignore next */
  (cov_gxuoextdz().s[100]++, res.status);
  /* istanbul ignore next */
  cov_gxuoextdz().s[101]++;
  res.status = function (code) {
    /* istanbul ignore next */
    cov_gxuoextdz().f[14]++;
    cov_gxuoextdz().s[102]++;
    this.statusCode = code;
    /* istanbul ignore next */
    cov_gxuoextdz().s[103]++;
    return this;
  };
  /* istanbul ignore next */
  cov_gxuoextdz().s[104]++;
  res.json = function (data) {
    /* istanbul ignore next */
    cov_gxuoextdz().f[15]++;
    cov_gxuoextdz().s[105]++;
    // Log file upload events
    if (
    /* istanbul ignore next */
    (cov_gxuoextdz().b[31][0]++, req.path.includes('/upload')) ||
    /* istanbul ignore next */
    (cov_gxuoextdz().b[31][1]++, req.path.includes('/file'))) {
      /* istanbul ignore next */
      cov_gxuoextdz().b[30][0]++;
      const userId =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[106]++, req.user?.userId);
      const username =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[107]++, req.user?.username);
      const fileName =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[108]++,
      /* istanbul ignore next */
      (cov_gxuoextdz().b[32][0]++, req.body?.fileName) ||
      /* istanbul ignore next */
      (cov_gxuoextdz().b[32][1]++, 'unknown'));
      const fileSize =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[109]++,
      /* istanbul ignore next */
      (cov_gxuoextdz().b[33][0]++, req.body?.fileSize) ||
      /* istanbul ignore next */
      (cov_gxuoextdz().b[33][1]++, 0));
      const fileType =
      /* istanbul ignore next */
      (cov_gxuoextdz().s[110]++,
      /* istanbul ignore next */
      (cov_gxuoextdz().b[34][0]++, req.body?.fileType) ||
      /* istanbul ignore next */
      (cov_gxuoextdz().b[34][1]++, 'unknown'));
      /* istanbul ignore next */
      cov_gxuoextdz().s[111]++;
      if (this.statusCode === 200) {
        /* istanbul ignore next */
        cov_gxuoextdz().b[35][0]++;
        cov_gxuoextdz().s[112]++;
        securityMonitoringService_1.securityMonitoring.logEvent({
          type: 'admin_action',
          severity: 'low',
          source: {
            ip:
            /* istanbul ignore next */
            (cov_gxuoextdz().b[36][0]++, req.ip) ||
            /* istanbul ignore next */
            (cov_gxuoextdz().b[36][1]++, 'unknown'),
            userAgent:
            /* istanbul ignore next */
            (cov_gxuoextdz().b[37][0]++, req.get('User-Agent')) ||
            /* istanbul ignore next */
            (cov_gxuoextdz().b[37][1]++, ''),
            userId,
            username
          },
          details: {
            uploadType: 'upload_success',
            fileName,
            fileSize,
            fileType
          }
        });
      } else {
        /* istanbul ignore next */
        cov_gxuoextdz().b[35][1]++;
        cov_gxuoextdz().s[113]++;
        if (this.statusCode >= 400) {
          /* istanbul ignore next */
          cov_gxuoextdz().b[38][0]++;
          let uploadType =
          /* istanbul ignore next */
          (cov_gxuoextdz().s[114]++, 'upload_failed');
          /* istanbul ignore next */
          cov_gxuoextdz().s[115]++;
          if (
          /* istanbul ignore next */
          (cov_gxuoextdz().b[40][0]++, data.error?.includes('malicious')) ||
          /* istanbul ignore next */
          (cov_gxuoextdz().b[40][1]++, data.error?.includes('virus'))) {
            /* istanbul ignore next */
            cov_gxuoextdz().b[39][0]++;
            cov_gxuoextdz().s[116]++;
            uploadType = 'malicious_file';
          } else {
            /* istanbul ignore next */
            cov_gxuoextdz().b[39][1]++;
            cov_gxuoextdz().s[117]++;
            if (
            /* istanbul ignore next */
            (cov_gxuoextdz().b[42][0]++, data.error?.includes('size')) ||
            /* istanbul ignore next */
            (cov_gxuoextdz().b[42][1]++, data.error?.includes('large'))) {
              /* istanbul ignore next */
              cov_gxuoextdz().b[41][0]++;
              cov_gxuoextdz().s[118]++;
              uploadType = 'oversized_file';
            } else
            /* istanbul ignore next */
            {
              cov_gxuoextdz().b[41][1]++;
            }
          }
          /* istanbul ignore next */
          cov_gxuoextdz().s[119]++;
          securityMonitoringService_1.securityMonitoring.logEvent({
            type: 'admin_action',
            severity: 'low',
            source: {
              ip:
              /* istanbul ignore next */
              (cov_gxuoextdz().b[43][0]++, req.ip) ||
              /* istanbul ignore next */
              (cov_gxuoextdz().b[43][1]++, 'unknown'),
              userAgent:
              /* istanbul ignore next */
              (cov_gxuoextdz().b[44][0]++, req.get('User-Agent')) ||
              /* istanbul ignore next */
              (cov_gxuoextdz().b[44][1]++, ''),
              userId,
              username
            },
            details: {
              uploadType,
              fileName,
              fileSize,
              fileType,
              error:
              /* istanbul ignore next */
              (cov_gxuoextdz().b[45][0]++, data.error) ||
              /* istanbul ignore next */
              (cov_gxuoextdz().b[45][1]++, data.message)
            }
          });
        } else
        /* istanbul ignore next */
        {
          cov_gxuoextdz().b[38][1]++;
        }
      }
    } else
    /* istanbul ignore next */
    {
      cov_gxuoextdz().b[30][1]++;
    }
    cov_gxuoextdz().s[120]++;
    return originalJson.call(this, data);
  };
  /* istanbul ignore next */
  cov_gxuoextdz().s[121]++;
  next();
};
