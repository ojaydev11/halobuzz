  verifyWebhookSignature(payload, gateway) {
    /* istanbul ignore next */
    cov_1u76ve6fkt().f[30]++;
    cov_1u76ve6fkt().s[157]++;
    if (
    /* istanbul ignore next */
    (cov_1u76ve6fkt().b[41][0]++, !payload.signature) ||
    /* istanbul ignore next */
    (cov_1u76ve6fkt().b[41][1]++, !gateway.apiConfig.secretKey)) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().b[40][0]++;
      cov_1u76ve6fkt().s[158]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_1u76ve6fkt().b[40][1]++;
    }
    // Implementation would depend on gateway's signature verification method
    // This is a simplified example
    const expectedSignature =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[159]++, crypto_1.default.createHmac('sha256', gateway.apiConfig.secretKey).update(JSON.stringify(payload.gatewayResponse)).digest('hex'));
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[160]++;
    return expectedSignature === payload.signature;
  }
  startSessionCleanup() {
    /* istanbul ignore next */
    cov_1u76ve6fkt().f[31]++;
    cov_1u76ve6fkt().s[161]++;
    // Clean up expired sessions every 10 minutes
    setInterval(() => {
      /* istanbul ignore next */
      cov_1u76ve6fkt().f[32]++;
      const now =
      /* istanbul ignore next */
      (cov_1u76ve6fkt().s[162]++, new Date());
      let cleanedCount =
      /* istanbul ignore next */
      (cov_1u76ve6fkt().s[163]++, 0);
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[164]++;
      for (const [sessionId, session] of this.paymentSessions.entries()) {
        /* istanbul ignore next */
        cov_1u76ve6fkt().s[165]++;
        if (
        /* istanbul ignore next */
        (cov_1u76ve6fkt().b[43][0]++, session.expiresAt <= now) &&
        /* istanbul ignore next */
        (cov_1u76ve6fkt().b[43][1]++, session.status === 'pending')) {
          /* istanbul ignore next */
          cov_1u76ve6fkt().b[42][0]++;
          cov_1u76ve6fkt().s[166]++;
          session.status = 'expired';
          /* istanbul ignore next */
          cov_1u76ve6fkt().s[167]++;
          cleanedCount++;
        } else
        /* istanbul ignore next */
        {
          cov_1u76ve6fkt().b[42][1]++;
        }
        // Remove old completed/failed sessions after 24 hours
        cov_1u76ve6fkt().s[168]++;
        if (
        /* istanbul ignore next */
        (cov_1u76ve6fkt().b[45][0]++, session.completedAt) &&
        /* istanbul ignore next */
        (cov_1u76ve6fkt().b[45][1]++, now.getTime() - session.completedAt.getTime() > 24 * 60 * 60 * 1000)) {
          /* istanbul ignore next */
          cov_1u76ve6fkt().b[44][0]++;
          cov_1u76ve6fkt().s[169]++;
          this.paymentSessions.delete(sessionId);
          /* istanbul ignore next */
          cov_1u76ve6fkt().s[170]++;
          cleanedCount++;
        } else
        /* istanbul ignore next */
        {
          cov_1u76ve6fkt().b[44][1]++;
        }
      }
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[171]++;
      if (cleanedCount > 0) {
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[46][0]++;
        cov_1u76ve6fkt().s[172]++;
        logger_1.logger.debug(`Cleaned up ${cleanedCount} payment sessions`);
      } else
      /* istanbul ignore next */
      {
        cov_1u76ve6fkt().b[46][1]++;
      }
    }, 600000); // 10 minutes
  }
