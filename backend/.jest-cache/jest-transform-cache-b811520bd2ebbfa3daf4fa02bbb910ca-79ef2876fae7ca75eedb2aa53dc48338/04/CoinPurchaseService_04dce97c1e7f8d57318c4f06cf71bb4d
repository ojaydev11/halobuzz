  /**
   * Private helper methods
   */
  async createGatewayPayment(session, gateway, request) {
    /* istanbul ignore next */
    cov_1u76ve6fkt().f[20]++;
    const {
      userId,
      clientInfo
    } =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[107]++, request);
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[108]++;
    switch (gateway.id) {
      case 'esewa':
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[36][0]++;
        cov_1u76ve6fkt().s[109]++;
        return await this.createESewaPayment(session, gateway);
      case 'khalti':
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[36][1]++;
        cov_1u76ve6fkt().s[110]++;
        return await this.createKhaltiPayment(session, gateway, clientInfo);
      case 'stripe':
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[36][2]++;
        cov_1u76ve6fkt().s[111]++;
        return await this.createStripePayment(session, gateway, clientInfo);
      case 'paypal':
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[36][3]++;
        cov_1u76ve6fkt().s[112]++;
        return await this.createPayPalPayment(session, gateway);
      default:
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[36][4]++;
        cov_1u76ve6fkt().s[113]++;
        throw new Error(`Unsupported payment gateway: ${gateway.id}`);
    }
  }
  async createESewaPayment(session, gateway) {
    /* istanbul ignore next */
    cov_1u76ve6fkt().f[21]++;
    // eSewa integration
    const params =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[114]++, new URLSearchParams({
      amt: session.amount.toString(),
      pdc: '0',
      psc: '0',
      txAmt: '0',
      tAmt: session.amount.toString(),
      pid: session.sessionId,
      scd: gateway.apiConfig.merchantId,
      su: `${process.env.API_BASE_URL}/payments/esewa/success`,
      fu: `${process.env.API_BASE_URL}/payments/esewa/failure`
    }));
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[115]++;
    return `${gateway.apiConfig.baseUrl}/main?${params.toString()}`;
  }
  async createKhaltiPayment(session, gateway, clientInfo) {
    /* istanbul ignore next */
    cov_1u76ve6fkt().f[22]++;
    cov_1u76ve6fkt().s[116]++;
    // Khalti integration
    try {
      const response =
      /* istanbul ignore next */
      (cov_1u76ve6fkt().s[117]++, await axios_1.default.post(`${gateway.apiConfig.baseUrl}/payment/initiate/`, {
        return_url: `${process.env.API_BASE_URL}/payments/khalti/callback`,
        website_url: process.env.FRONTEND_URL,
        amount: session.amount * 100,
        // Amount in paisa
        purchase_order_id: session.sessionId,
        purchase_order_name: `HaloBuzz Coins - ${session.coins} coins`,
        customer_info: {
          name: 'HaloBuzz User',
          email: `user${session.userId}@halobuzz.com`,
          phone: '9800000000'
        },
        amount_breakdown: [{
          label: 'HaloBuzz Coins',
          amount: session.amount * 100
        }]
      }, {
        headers: {
          'Authorization': `Key ${gateway.apiConfig.secretKey}`,
          'Content-Type': 'application/json'
        }
      }));
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[118]++;
      return response.data.payment_url;
    } catch (error) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[119]++;
      logger_1.logger.error('Error creating Khalti payment:', error);
