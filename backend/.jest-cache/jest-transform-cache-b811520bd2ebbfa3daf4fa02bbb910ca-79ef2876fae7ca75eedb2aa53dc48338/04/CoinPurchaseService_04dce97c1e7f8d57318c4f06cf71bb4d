  generatePaymentStats() {
    /* istanbul ignore next */
    cov_1u76ve6fkt().f[35]++;
    const now =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[176]++, new Date());
    const oneDayAgo =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[177]++, new Date(now.getTime() - 24 * 60 * 60 * 1000));
    const recentSessions =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[178]++, Array.from(this.paymentSessions.values()).filter(s => {
      /* istanbul ignore next */
      cov_1u76ve6fkt().f[36]++;
      cov_1u76ve6fkt().s[179]++;
      return s.createdAt >= oneDayAgo;
    }));
    const stats =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[180]++, {
      total24h: recentSessions.length,
      completed24h: recentSessions.filter(s => {
        /* istanbul ignore next */
        cov_1u76ve6fkt().f[37]++;
        cov_1u76ve6fkt().s[181]++;
        return s.status === 'completed';
      }).length,
      failed24h: recentSessions.filter(s => {
        /* istanbul ignore next */
        cov_1u76ve6fkt().f[38]++;
        cov_1u76ve6fkt().s[182]++;
        return s.status === 'failed';
      }).length,
      pending24h: recentSessions.filter(s => {
        /* istanbul ignore next */
        cov_1u76ve6fkt().f[39]++;
        cov_1u76ve6fkt().s[183]++;
        return /* istanbul ignore next */(cov_1u76ve6fkt().b[47][0]++, s.status === 'pending') ||
        /* istanbul ignore next */
        (cov_1u76ve6fkt().b[47][1]++, s.status === 'processing');
      }).length,
      totalAmount24h: recentSessions.filter(s => {
        /* istanbul ignore next */
        cov_1u76ve6fkt().f[40]++;
        cov_1u76ve6fkt().s[184]++;
        return s.status === 'completed';
      }).reduce((sum, s) => {
        /* istanbul ignore next */
        cov_1u76ve6fkt().f[41]++;
        cov_1u76ve6fkt().s[185]++;
        return sum + s.amount;
      }, 0),
      gatewayBreakdown: {}
    });
    // Gateway breakdown
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[186]++;
    for (const gateway of this.paymentGateways.keys()) {
      const gatewayStats =
      /* istanbul ignore next */
      (cov_1u76ve6fkt().s[187]++, recentSessions.filter(s => {
        /* istanbul ignore next */
        cov_1u76ve6fkt().f[42]++;
        cov_1u76ve6fkt().s[188]++;
        return s.gatewayId === gateway;
      }));
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[189]++;
      stats.gatewayBreakdown[gateway] = {
        total: gatewayStats.length,
        completed: gatewayStats.filter(s => {
          /* istanbul ignore next */
          cov_1u76ve6fkt().f[43]++;
          cov_1u76ve6fkt().s[190]++;
          return s.status === 'completed';
        }).length,
        successRate: gatewayStats.length > 0 ?
        /* istanbul ignore next */
        (cov_1u76ve6fkt().b[48][0]++, gatewayStats.filter(s => {
          /* istanbul ignore next */
          cov_1u76ve6fkt().f[44]++;
          cov_1u76ve6fkt().s[191]++;
          return s.status === 'completed';
        }).length / gatewayStats.length * 100) :
        /* istanbul ignore next */
        (cov_1u76ve6fkt().b[48][1]++, 0)
      };
    }
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[192]++;
    return stats;
  }
