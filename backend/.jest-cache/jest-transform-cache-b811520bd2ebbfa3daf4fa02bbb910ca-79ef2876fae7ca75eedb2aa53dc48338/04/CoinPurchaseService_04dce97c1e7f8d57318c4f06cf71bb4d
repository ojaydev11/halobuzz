  /**
   * Create payment session for coin purchase
   */
  async createPaymentSession(request) {
    /* istanbul ignore next */
    cov_1u76ve6fkt().f[9]++;
    const {
      userId,
      packageId,
      gatewayId,
      currency
    } =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[39]++, request);
    // Validate package
    const coinPackage =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[40]++, this.coinPackages.get(packageId));
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[41]++;
    if (
    /* istanbul ignore next */
    (cov_1u76ve6fkt().b[15][0]++, !coinPackage) ||
    /* istanbul ignore next */
    (cov_1u76ve6fkt().b[15][1]++, !coinPackage.isVisible)) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().b[14][0]++;
      cov_1u76ve6fkt().s[42]++;
      throw new Error('Coin package not available');
    } else
    /* istanbul ignore next */
    {
      cov_1u76ve6fkt().b[14][1]++;
    }
    // Validate gateway
    const gateway =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[43]++, this.paymentGateways.get(gatewayId));
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[44]++;
    if (
    /* istanbul ignore next */
    (cov_1u76ve6fkt().b[17][0]++, !gateway) ||
    /* istanbul ignore next */
    (cov_1u76ve6fkt().b[17][1]++, !gateway.isActive)) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().b[16][0]++;
      cov_1u76ve6fkt().s[45]++;
      throw new Error('Payment gateway not available');
    } else
    /* istanbul ignore next */
    {
      cov_1u76ve6fkt().b[16][1]++;
    }
    cov_1u76ve6fkt().s[46]++;
    if (!gateway.supportedCurrencies.includes(currency)) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().b[18][0]++;
      cov_1u76ve6fkt().s[47]++;
      throw new Error(`Currency ${currency} not supported by ${gateway.name}`);
    } else
    /* istanbul ignore next */
    {
      cov_1u76ve6fkt().b[18][1]++;
    }
    // Get localized pricing
    const pricing =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[48]++, coinPackage.localizedPricing[currency]);
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[49]++;
    if (!pricing) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().b[19][0]++;
      cov_1u76ve6fkt().s[50]++;
      throw new Error(`Pricing not available for currency ${currency}`);
    } else
    /* istanbul ignore next */
    {
      cov_1u76ve6fkt().b[19][1]++;
    }
    // Apply fees
    const baseAmount =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[51]++, pricing.price);
    const fees =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[52]++, this.calculateFees(baseAmount, gateway));
    const totalAmount =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[53]++, baseAmount + fees);
    // Create payment session
    const sessionId =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[54]++, `pay_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`);
    const session =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[55]++, {
      sessionId,
      userId,
      packageId,
      gatewayId,
      amount: totalAmount,
      currency,
      coins: coinPackage.coins,
      bonusCoins: coinPackage.bonusCoins,
      status: 'pending',
      expiresAt: new Date(Date.now() + 30 * 60 * 1000),
      // 30 minutes
      createdAt: new Date()
    });
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[56]++;
    try {
      // Create payment with gateway
      const paymentUrl =
      /* istanbul ignore next */
      (cov_1u76ve6fkt().s[57]++, await this.createGatewayPayment(session, gateway, request));
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[58]++;
      session.paymentUrl = paymentUrl;
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[59]++;
      session.status = 'processing';
      // Store session
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[60]++;
      this.paymentSessions.set(sessionId, session);
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[61]++;
      this.emit('paymentSessionCreated', {
        sessionId,
        userId,
        packageId,
        gatewayId,
        amount: totalAmount,
        currency
      });
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[62]++;
      logger_1.logger.info(`Created payment session ${sessionId} for user ${userId}: ${coinPackage.name} via ${gateway.name}`);
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[63]++;
      return session;
    } catch (error) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[64]++;
      logger_1.logger.error('Error creating payment session:', error);
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[65]++;
      throw error;
    }
  }
  /**
   * Handle webhook from payment gateway
   */
  async handleWebhook(webhookPayload) {
    /* istanbul ignore next */
    cov_1u76ve6fkt().f[10]++;
    const {
      gatewayId,
      transactionId,
      status,
      merchantReference,
      gatewayResponse
    } =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[66]++, webhookPayload);
    // Find payment session
    const session =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[67]++, Array.from(this.paymentSessions.values()).find(s => {
      /* istanbul ignore next */
      cov_1u76ve6fkt().f[11]++;
      cov_1u76ve6fkt().s[68]++;
      return /* istanbul ignore next */(cov_1u76ve6fkt().b[20][0]++, s.sessionId === merchantReference) &&
      /* istanbul ignore next */
      (cov_1u76ve6fkt().b[20][1]++, s.gatewayId === gatewayId);
    }));
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[69]++;
    if (!session) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().b[21][0]++;
      cov_1u76ve6fkt().s[70]++;
      logger_1.logger.warn(`Payment session not found for webhook: ${merchantReference}`);
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[71]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1u76ve6fkt().b[21][1]++;
    }
    const gateway =
    /* istanbul ignore next */
    (cov_1u76ve6fkt().s[72]++, this.paymentGateways.get(gatewayId));
    /* istanbul ignore next */
    cov_1u76ve6fkt().s[73]++;
    if (!gateway) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().b[22][0]++;
      cov_1u76ve6fkt().s[74]++;
      logger_1.logger.error(`Payment gateway not found: ${gatewayId}`);
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[75]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1u76ve6fkt().b[22][1]++;
    }
    // Verify webhook signature if required
    cov_1u76ve6fkt().s[76]++;
    if (webhookPayload.signature) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().b[23][0]++;
      const isValidSignature =
      /* istanbul ignore next */
      (cov_1u76ve6fkt().s[77]++, this.verifyWebhookSignature(webhookPayload, gateway));
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[78]++;
      if (!isValidSignature) {
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[24][0]++;
        cov_1u76ve6fkt().s[79]++;
        logger_1.logger.error(`Invalid webhook signature for session ${session.sessionId}`);
        /* istanbul ignore next */
        cov_1u76ve6fkt().s[80]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_1u76ve6fkt().b[24][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1u76ve6fkt().b[23][1]++;
    }
    cov_1u76ve6fkt().s[81]++;
    try {
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[82]++;
      session.gatewayTransactionId = transactionId;
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[83]++;
      if (status === 'success') {
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[25][0]++;
        cov_1u76ve6fkt().s[84]++;
        await this.completePayment(session);
      } else {
        /* istanbul ignore next */
        cov_1u76ve6fkt().b[25][1]++;
        cov_1u76ve6fkt().s[85]++;
        if (status === 'failed') {
          /* istanbul ignore next */
          cov_1u76ve6fkt().b[26][0]++;
          cov_1u76ve6fkt().s[86]++;
          await this.failPayment(session,
          /* istanbul ignore next */
          (cov_1u76ve6fkt().b[27][0]++, gatewayResponse?.error) ||
          /* istanbul ignore next */
          (cov_1u76ve6fkt().b[27][1]++, 'Payment failed'));
        } else
        /* istanbul ignore next */
        {
          cov_1u76ve6fkt().b[26][1]++;
        }
      }
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[87]++;
      this.emit('webhookProcessed', {
        sessionId: session.sessionId,
        gatewayId,
        status,
        transactionId
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[88]++;
      logger_1.logger.error(`Error processing webhook for session ${session.sessionId}:`, error);
      /* istanbul ignore next */
      cov_1u76ve6fkt().s[89]++;
      await this.failPayment(session, error instanceof Error ?
      /* istanbul ignore next */
      (cov_1u76ve6fkt().b[28][0]++, error.message) :
      /* istanbul ignore next */
      (cov_1u76ve6fkt().b[28][1]++, 'Webhook processing error'));
    }
  }
