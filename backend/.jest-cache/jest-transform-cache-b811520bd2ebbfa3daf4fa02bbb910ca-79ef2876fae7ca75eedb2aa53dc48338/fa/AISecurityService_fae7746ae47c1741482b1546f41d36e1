      // 3. Check for manipulation attempts
      const manipulationCheck =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[20]++, this.detectManipulationAttempts(input.content));
      /* istanbul ignore next */
      cov_f1dj6urga().s[21]++;
      if (manipulationCheck.detected) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[4][0]++;
        cov_f1dj6urga().s[22]++;
        threats.push('manipulation_attempt');
        /* istanbul ignore next */
        cov_f1dj6urga().s[23]++;
        warnings.push(...manipulationCheck.indicators);
        /* istanbul ignore next */
        cov_f1dj6urga().s[24]++;
        confidence -= 0.2;
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[4][1]++;
      }
      // 4. Check for sensitive information requests
      const sensitivityCheck =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[25]++, this.checkSensitiveContent(input.content));
      /* istanbul ignore next */
      cov_f1dj6urga().s[26]++;
      if (sensitivityCheck.containsSensitive) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[5][0]++;
        cov_f1dj6urga().s[27]++;
        threats.push('sensitive_information_request');
        /* istanbul ignore next */
        cov_f1dj6urga().s[28]++;
        warnings.push(...sensitivityCheck.topics);
        /* istanbul ignore next */
        cov_f1dj6urga().s[29]++;
        confidence -= 0.2;
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[5][1]++;
      }
      // 5. Context-specific validation
      const contextCheck =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[30]++, await this.validateContext(input.context, input.content));
      /* istanbul ignore next */
      cov_f1dj6urga().s[31]++;
      if (!contextCheck.isValid) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[6][0]++;
        cov_f1dj6urga().s[32]++;
        threats.push('context_violation');
        /* istanbul ignore next */
        cov_f1dj6urga().s[33]++;
        warnings.push(...contextCheck.violations);
        /* istanbul ignore next */
        cov_f1dj6urga().s[34]++;
        confidence -= 0.1;
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[6][1]++;
      }
      // 6. Semantic analysis for hidden meanings
      const semanticCheck =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[35]++, await this.analyzeSemanticIntention(input.content));
      /* istanbul ignore next */
      cov_f1dj6urga().s[36]++;
      if (semanticCheck.suspiciousIntent) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[7][0]++;
        cov_f1dj6urga().s[37]++;
        threats.push('suspicious_semantic_intent');
        /* istanbul ignore next */
        cov_f1dj6urga().s[38]++;
        confidence -= 0.1;
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[7][1]++;
      }
      const isValid =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[39]++,
      /* istanbul ignore next */
      (cov_f1dj6urga().b[8][0]++, threats.length === 0) &&
      /* istanbul ignore next */
      (cov_f1dj6urga().b[8][1]++, confidence > 0.5));
      // Log threat if detected
      /* istanbul ignore next */
      cov_f1dj6urga().s[40]++;
      if (!isValid) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[9][0]++;
        cov_f1dj6urga().s[41]++;
        await this.logSecurityThreat({
          id: crypto_1.default.randomUUID(),
          type:
          /* istanbul ignore next */
          (cov_f1dj6urga().b[10][0]++, this.categorizeThreats(threats)[0]) ||
          /* istanbul ignore next */
          (cov_f1dj6urga().b[10][1]++, 'manipulation'),
          severity: this.calculateSeverity(threats, confidence),
          detectedAt: new Date(),
          userId: input.userId,
          sessionId: input.sessionId,
          content: input.content,
          mitigationAction: this.determineMitigation(threats, confidence),
          blocked: !isValid
        });
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[9][1]++;
      }
      cov_f1dj6urga().s[42]++;
      return {
        isValid,
        confidence: Math.max(0, Math.min(1, confidence)),
        threats,
        sanitizedContent: isValid ?
        /* istanbul ignore next */
        (cov_f1dj6urga().b[11][0]++, input.content) :
        /* istanbul ignore next */
        (cov_f1dj6urga().b[11][1]++, this.sanitizeContent(input.content)),
        warnings
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[43]++;
      logger_1.logger.error('AI security validation error:', error);
      // Fail secure - block when error occurs
      /* istanbul ignore next */
      cov_f1dj6urga().s[44]++;
      return {
        isValid: false,
        confidence: 0,
        threats: ['validation_error'],
        warnings: ['Security validation failed - request blocked for safety']
      };
    }
  }
  // Detect dangerous patterns in input
  static detectDangerousPatterns(content) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[2]++;
    const threats =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[45]++, []);
    /* istanbul ignore next */
    cov_f1dj6urga().s[46]++;
    for (const pattern of this.DANGEROUS_PATTERNS) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[47]++;
      if (pattern.test(content)) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[12][0]++;
        cov_f1dj6urga().s[48]++;
        threats.push('dangerous_pattern');
        /* istanbul ignore next */
        cov_f1dj6urga().s[49]++;
        break;
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[12][1]++;
      }
    }
    /* istanbul ignore next */
    cov_f1dj6urga().s[50]++;
    return threats;
  }
  // Advanced prompt injection detection
  static async detectPromptInjection(content) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[3]++;
    cov_f1dj6urga().s[51]++;
    try {
      // Check for common injection markers
      const injectionMarkers =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[52]++, ['\\n\\nHuman:', '\\n\\nAssistant:', 'User:', 'System:', '```', '---', '***', '###', 'INSTRUCTION:', 'PROMPT:']);
      let suspiciousMarkers =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[53]++, 0);
      /* istanbul ignore next */
      cov_f1dj6urga().s[54]++;
      for (const marker of injectionMarkers) {
        /* istanbul ignore next */
        cov_f1dj6urga().s[55]++;
        if (content.toLowerCase().includes(marker.toLowerCase())) {
          /* istanbul ignore next */
          cov_f1dj6urga().b[13][0]++;
          cov_f1dj6urga().s[56]++;
          suspiciousMarkers++;
        } else
        /* istanbul ignore next */
        {
          cov_f1dj6urga().b[13][1]++;
        }
      }
      // Check for instruction-like language
      const instructionWords =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[57]++, ['must', 'should', 'will', 'shall', 'do', 'don\'t', 'never', 'always']);
      const instructionCount =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[58]++, instructionWords.reduce((count, word) => {
        /* istanbul ignore next */
        cov_f1dj6urga().f[4]++;
        cov_f1dj6urga().s[59]++;
        return count + (
        /* istanbul ignore next */
        (cov_f1dj6urga().b[14][0]++, content.toLowerCase().match(new RegExp(`\\b${word}\\b`, 'g'))) ||
        /* istanbul ignore next */
        (cov_f1dj6urga().b[14][1]++, [])).length;
      }, 0));
      // Check for role definitions
      const roleDefinitions =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[60]++, /(?:you|i|we)\s+(?:are|am|is)\s+(?:a|an|the)?\s*(?:ai|assistant|bot|human|user|admin|developer)/i);
      const hasRoleDefinition =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[61]++, roleDefinitions.test(content));
      // Calculate injection probability
      const confidence =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[62]++, Math.max(0, 1 - (suspiciousMarkers * 0.3 + Math.min(instructionCount, 10) * 0.05 + (hasRoleDefinition ?
      /* istanbul ignore next */
      (cov_f1dj6urga().b[15][0]++, 0.4) :
      /* istanbul ignore next */
      (cov_f1dj6urga().b[15][1]++, 0)))));
      /* istanbul ignore next */
      cov_f1dj6urga().s[63]++;
      return {
        isValid: confidence > 0.6,
        confidence,
        injectionType: confidence <= 0.6 ?
        /* istanbul ignore next */
        (cov_f1dj6urga().b[16][0]++, 'structured_injection') :
        /* istanbul ignore next */
        (cov_f1dj6urga().b[16][1]++, undefined)
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[64]++;
      logger_1.logger.error('Prompt injection detection error:', error);
      /* istanbul ignore next */
      cov_f1dj6urga().s[65]++;
      return {
        isValid: false,
        confidence: 0,
        injectionType: 'detection_error'
      };
    }
  }
  // Detect manipulation attempts
  static detectManipulationAttempts(content) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[5]++;
    const indicators =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[66]++, []);
    const lowerContent =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[67]++, content.toLowerCase());
    // Emotional pressure tactics
    const emotionalTriggers =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[68]++, ['urgent', 'emergency', 'immediately', 'right now', 'asap', 'please help', 'i\'m desperate', 'i need', 'you must']);
    /* istanbul ignore next */
    cov_f1dj6urga().s[69]++;
    if (emotionalTriggers.some(trigger => {
      /* istanbul ignore next */
      cov_f1dj6urga().f[6]++;
      cov_f1dj6urga().s[70]++;
      return lowerContent.includes(trigger);
    })) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[17][0]++;
      cov_f1dj6urga().s[71]++;
      indicators.push('emotional_pressure');
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[17][1]++;
    }
    // Authority claims
    const authorityClaims =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[72]++, ['i\'m your', 'as your', 'i am the', 'i work for', 'official', 'authorized', 'administrator', 'developer']);
    /* istanbul ignore next */
    cov_f1dj6urga().s[73]++;
    if (authorityClaims.some(claim => {
      /* istanbul ignore next */
      cov_f1dj6urga().f[7]++;
      cov_f1dj6urga().s[74]++;
      return lowerContent.includes(claim);
    })) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[18][0]++;
      cov_f1dj6urga().s[75]++;
      indicators.push('false_authority');
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[18][1]++;
    }
    // Trust exploitation
    const trustExploitation =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[76]++, ['trust me', 'believe me', 'i promise', 'i guarantee', 'you can trust', 'i would never', 'honest']);
    /* istanbul ignore next */
    cov_f1dj6urga().s[77]++;
    if (trustExploitation.some(phrase => {
      /* istanbul ignore next */
      cov_f1dj6urga().f[8]++;
      cov_f1dj6urga().s[78]++;
      return lowerContent.includes(phrase);
    })) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[19][0]++;
      cov_f1dj6urga().s[79]++;
      indicators.push('trust_exploitation');
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[19][1]++;
    }
    // Fear tactics
    const fearTactics =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[80]++, ['dangerous', 'harmful', 'bad things will happen', 'you\'ll regret', 'something terrible', 'warning']);
    /* istanbul ignore next */
    cov_f1dj6urga().s[81]++;
    if (fearTactics.some(tactic => {
      /* istanbul ignore next */
      cov_f1dj6urga().f[9]++;
      cov_f1dj6urga().s[82]++;
      return lowerContent.includes(tactic);
    })) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[20][0]++;
      cov_f1dj6urga().s[83]++;
      indicators.push('fear_mongering');
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[20][1]++;
    }
    const confidence =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[84]++, indicators.length * 0.25);
    /* istanbul ignore next */
    cov_f1dj6urga().s[85]++;
    return {
      detected: indicators.length > 0,
      indicators,
      confidence: Math.min(1, confidence)
    };
  }
  // Check for sensitive content requests
  static checkSensitiveContent(content) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[10]++;
    const detectedTopics =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[86]++, []);
    const lowerContent =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[87]++, content.toLowerCase());
    // Financial information
    /* istanbul ignore next */
    cov_f1dj6urga().s[88]++;
    if (/(?:credit card|bank account|password|ssn|social security|pin|cvv)/i.test(content)) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[21][0]++;
      cov_f1dj6urga().s[89]++;
      detectedTopics.push('financial_data');
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[21][1]++;
    }
    // Personal identification
    cov_f1dj6urga().s[90]++;
    if (/(?:address|phone number|email|full name|date of birth|id number)/i.test(content)) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[22][0]++;
      cov_f1dj6urga().s[91]++;
      detectedTopics.push('personal_information');
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[22][1]++;
    }
    // Medical information
    cov_f1dj6urga().s[92]++;
    if (/(?:medical record|health|diagnosis|prescription|doctor|hospital)/i.test(content)) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[23][0]++;
      cov_f1dj6urga().s[93]++;
      detectedTopics.push('medical_records');
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[23][1]++;
    }
    // System information
    cov_f1dj6urga().s[94]++;
    if (/(?:api key|token|secret|configuration|system|database|server)/i.test(content)) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[24][0]++;
      cov_f1dj6urga().s[95]++;
      detectedTopics.push('system_information');
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[24][1]++;
    }
    const riskLevel =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[96]++, detectedTopics.length >= 3 ?
    /* istanbul ignore next */
    (cov_f1dj6urga().b[25][0]++, 'high') :
    /* istanbul ignore next */
    (cov_f1dj6urga().b[25][1]++, detectedTopics.length >= 1 ?
    /* istanbul ignore next */
    (cov_f1dj6urga().b[26][0]++, 'medium') :
    /* istanbul ignore next */
    (cov_f1dj6urga().b[26][1]++, 'low')));
    /* istanbul ignore next */
    cov_f1dj6urga().s[97]++;
    return {
      containsSensitive: detectedTopics.length > 0,
      topics: detectedTopics,
      riskLevel
    };
  }
  // Context-specific validation
  static async validateContext(context, content) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[11]++;
    const violations =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[98]++, []);
    /* istanbul ignore next */
    cov_f1dj6urga().s[99]++;
    switch (context) {
      case 'content_generation':
        /* istanbul ignore next */
        cov_f1dj6urga().b[27][0]++;
        cov_f1dj6urga().s[100]++;
        if (/(?:generate|create|make|write).*(?:illegal|harmful|dangerous|explicit)/i.test(content)) {
          /* istanbul ignore next */
          cov_f1dj6urga().b[28][0]++;
          cov_f1dj6urga().s[101]++;
          violations.push('harmful_content_generation_request');
        } else
        /* istanbul ignore next */
        {
          cov_f1dj6urga().b[28][1]++;
        }
        cov_f1dj6urga().s[102]++;
        break;
      case 'moderation':
        /* istanbul ignore next */
        cov_f1dj6urga().b[27][1]++;
        cov_f1dj6urga().s[103]++;
        if (/(?:ignore|bypass|disable|turn off).*(?:moderation|filter|safety)/i.test(content)) {
          /* istanbul ignore next */
          cov_f1dj6urga().b[29][0]++;
          cov_f1dj6urga().s[104]++;
          violations.push('moderation_bypass_attempt');
        } else
        /* istanbul ignore next */
        {
          cov_f1dj6urga().b[29][1]++;
        }
        cov_f1dj6urga().s[105]++;
        break;
      case 'recommendation':
        /* istanbul ignore next */
        cov_f1dj6urga().b[27][2]++;
        cov_f1dj6urga().s[106]++;
        if (/(?:recommend|suggest|advise).*(?:illegal|harmful|dangerous)/i.test(content)) {
          /* istanbul ignore next */
          cov_f1dj6urga().b[30][0]++;
          cov_f1dj6urga().s[107]++;
          violations.push('harmful_recommendation_request');
        } else
        /* istanbul ignore next */
        {
          cov_f1dj6urga().b[30][1]++;
        }
        cov_f1dj6urga().s[108]++;
        break;
      case 'chat':
        /* istanbul ignore next */
        cov_f1dj6urga().b[27][3]++;
        cov_f1dj6urga().s[109]++;
        // General chat validation
        if (/(?:role\s*play|pretend|act\s+as).*(?:admin|developer|system)/i.test(content)) {
          /* istanbul ignore next */
          cov_f1dj6urga().b[31][0]++;
          cov_f1dj6urga().s[110]++;
          violations.push('unauthorized_role_play');
        } else
        /* istanbul ignore next */
        {
          cov_f1dj6urga().b[31][1]++;
        }
        cov_f1dj6urga().s[111]++;
        break;
    }
    /* istanbul ignore next */
    cov_f1dj6urga().s[112]++;
    return {
      isValid: violations.length === 0,
      violations
    };
  }
  // Analyze semantic intention
  static async analyzeSemanticIntention(content) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[12]++;
    cov_f1dj6urga().s[113]++;
    try {
      // Simple semantic analysis - in production, use advanced NLP
      const suspiciousPatterns =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[114]++, [/(?:trick|fool|deceive|manipulate)\s+(?:you|ai|system)/i, /(?:bypass|circumvent|avoid|get around)\s+(?:safety|security|rules)/i, /(?:extract|get|obtain|reveal)\s+(?:information|data|secrets)/i]);
      let suspiciousScore =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[115]++, 0);
      let intentionType =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[116]++, '');
      /* istanbul ignore next */
      cov_f1dj6urga().s[117]++;
      for (const pattern of suspiciousPatterns) {
        /* istanbul ignore next */
        cov_f1dj6urga().s[118]++;
        if (pattern.test(content)) {
          /* istanbul ignore next */
          cov_f1dj6urga().b[32][0]++;
          cov_f1dj6urga().s[119]++;
          suspiciousScore += 0.4;
          /* istanbul ignore next */
          cov_f1dj6urga().s[120]++;
          if (!intentionType) {
            /* istanbul ignore next */
            cov_f1dj6urga().b[33][0]++;
            cov_f1dj6urga().s[121]++;
            if (pattern.source.includes('trick|fool')) {
              /* istanbul ignore next */
              cov_f1dj6urga().b[34][0]++;
              cov_f1dj6urga().s[122]++;
              intentionType = 'deception';
            } else {
              /* istanbul ignore next */
              cov_f1dj6urga().b[34][1]++;
              cov_f1dj6urga().s[123]++;
              if (pattern.source.includes('bypass')) {
                /* istanbul ignore next */
                cov_f1dj6urga().b[35][0]++;
                cov_f1dj6urga().s[124]++;
                intentionType = 'circumvention';
              } else {
                /* istanbul ignore next */
                cov_f1dj6urga().b[35][1]++;
                cov_f1dj6urga().s[125]++;
                if (pattern.source.includes('extract')) {
                  /* istanbul ignore next */
                  cov_f1dj6urga().b[36][0]++;
                  cov_f1dj6urga().s[126]++;
                  intentionType = 'information_extraction';
                } else
                /* istanbul ignore next */
                {
                  cov_f1dj6urga().b[36][1]++;
                }
              }
            }
          } else
          /* istanbul ignore next */
          {
            cov_f1dj6urga().b[33][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_f1dj6urga().b[32][1]++;
        }
      }
      // Check for indirect manipulation through storytelling
      /* istanbul ignore next */
      cov_f1dj6urga().s[127]++;
      if (
      /* istanbul ignore next */
      (cov_f1dj6urga().b[38][0]++, /(?:imagine|suppose|what if|hypothetically)/i.test(content)) &&
      /* istanbul ignore next */
      (cov_f1dj6urga().b[38][1]++, /(?:you were|you could|you would)/i.test(content))) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[37][0]++;
        cov_f1dj6urga().s[128]++;
        suspiciousScore += 0.3;
        /* istanbul ignore next */
        cov_f1dj6urga().s[129]++;
        intentionType =
        /* istanbul ignore next */
        (cov_f1dj6urga().b[39][0]++, intentionType) ||
        /* istanbul ignore next */
        (cov_f1dj6urga().b[39][1]++, 'hypothetical_manipulation');
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[37][1]++;
      }
      cov_f1dj6urga().s[130]++;
      return {
        suspiciousIntent: suspiciousScore > 0.5,
        intentionType: suspiciousScore > 0.5 ?
        /* istanbul ignore next */
        (cov_f1dj6urga().b[40][0]++, intentionType) :
        /* istanbul ignore next */
        (cov_f1dj6urga().b[40][1]++, undefined),
        confidence: Math.min(1, suspiciousScore)
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[131]++;
      logger_1.logger.error('Semantic intention analysis error:', error);
      /* istanbul ignore next */
      cov_f1dj6urga().s[132]++;
      return {
        suspiciousIntent: true,
        confidence: 0,
        intentionType: 'analysis_error'
      };
    }
  }
  // Categorize threats by type
  static categorizeThreats(threats) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[13]++;
    const categories =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[133]++, new Set());
    /* istanbul ignore next */
    cov_f1dj6urga().s[134]++;
    for (const threat of threats) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[135]++;
      switch (threat) {
        case 'dangerous_pattern':
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][0]++;
        case 'prompt_injection':
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][1]++;
        case 'structured_injection':
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][2]++;
          cov_f1dj6urga().s[136]++;
          categories.add('prompt_injection');
          /* istanbul ignore next */
          cov_f1dj6urga().s[137]++;
          break;
        case 'manipulation_attempt':
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][3]++;
        case 'emotional_pressure':
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][4]++;
        case 'trust_exploitation':
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][5]++;
          cov_f1dj6urga().s[138]++;
          categories.add('manipulation');
          /* istanbul ignore next */
          cov_f1dj6urga().s[139]++;
          break;
        case 'context_violation':
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][6]++;
        case 'unauthorized_role_play':
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][7]++;
          cov_f1dj6urga().s[140]++;
          categories.add('jailbreak');
          /* istanbul ignore next */
          cov_f1dj6urga().s[141]++;
          break;
        default:
          /* istanbul ignore next */
          cov_f1dj6urga().b[41][8]++;
          cov_f1dj6urga().s[142]++;
          categories.add('manipulation');
      }
    }
    /* istanbul ignore next */
    cov_f1dj6urga().s[143]++;
    return Array.from(categories);
  }
  // Calculate threat severity
  static calculateSeverity(threats, confidence) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[14]++;
    cov_f1dj6urga().s[144]++;
    if (
    /* istanbul ignore next */
    (cov_f1dj6urga().b[43][0]++, confidence < 0.2) ||
    /* istanbul ignore next */
    (cov_f1dj6urga().b[43][1]++, threats.includes('dangerous_pattern'))) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[42][0]++;
      cov_f1dj6urga().s[145]++;
      return 'critical';
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[42][1]++;
    }
    cov_f1dj6urga().s[146]++;
    if (
    /* istanbul ignore next */
    (cov_f1dj6urga().b[45][0]++, confidence < 0.4) ||
    /* istanbul ignore next */
    (cov_f1dj6urga().b[45][1]++, threats.length >= 3)) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[44][0]++;
      cov_f1dj6urga().s[147]++;
      return 'high';
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[44][1]++;
    }
    cov_f1dj6urga().s[148]++;
    if (
    /* istanbul ignore next */
    (cov_f1dj6urga().b[47][0]++, confidence < 0.6) ||
    /* istanbul ignore next */
    (cov_f1dj6urga().b[47][1]++, threats.length >= 2)) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[46][0]++;
      cov_f1dj6urga().s[149]++;
      return 'medium';
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[46][1]++;
    }
    cov_f1dj6urga().s[150]++;
    return 'low';
  }
  // Determine mitigation action
  static determineMitigation(threats, confidence) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[15]++;
    cov_f1dj6urga().s[151]++;
    if (confidence < 0.2) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[48][0]++;
      cov_f1dj6urga().s[152]++;
      return 'immediate_block_and_escalate';
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[48][1]++;
    }
    cov_f1dj6urga().s[153]++;
    if (confidence < 0.4) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[49][0]++;
      cov_f1dj6urga().s[154]++;
      return 'block_and_log';
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[49][1]++;
    }
    cov_f1dj6urga().s[155]++;
    if (confidence < 0.6) {
      /* istanbul ignore next */
      cov_f1dj6urga().b[50][0]++;
      cov_f1dj6urga().s[156]++;
      return 'sanitize_and_warn';
    } else
    /* istanbul ignore next */
    {
      cov_f1dj6urga().b[50][1]++;
    }
    cov_f1dj6urga().s[157]++;
    return 'log_and_monitor';
  }
  // Sanitize content by removing dangerous elements
  static sanitizeContent(content) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[16]++;
    let sanitized =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[158]++, content);
    // Remove potential injection markers
    const injectionMarkers =
    /* istanbul ignore next */
    (cov_f1dj6urga().s[159]++, [/\\n\\n(?:Human|Assistant|User|System):\s*/gi, /```[\s\S]*?```/g, /---+/g, /\*{3,}/g, /##{2,}/g]);
    /* istanbul ignore next */
    cov_f1dj6urga().s[160]++;
    for (const marker of injectionMarkers) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[161]++;
      sanitized = sanitized.replace(marker, '');
    }
    // Remove instruction-like language
    /* istanbul ignore next */
    cov_f1dj6urga().s[162]++;
    sanitized = sanitized.replace(/\b(?:you must|you should|you will|you shall)\b/gi, 'you might consider');
    /* istanbul ignore next */
    cov_f1dj6urga().s[163]++;
    sanitized = sanitized.replace(/\b(?:ignore|forget|disregard)\s+(?:previous|all|everything)\b/gi, 'consider');
    /* istanbul ignore next */
    cov_f1dj6urga().s[164]++;
    return sanitized.trim();
  }
  // Log security threat
  static async logSecurityThreat(threat) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[17]++;
    cov_f1dj6urga().s[165]++;
    try {
      /* istanbul ignore next */
      cov_f1dj6urga().s[166]++;
      // Store in cache for immediate access
      await (0, redis_1.setCache)(`ai_threat:${threat.id}`, threat, 86400); // 24 hours
      // Log to security monitoring system
      /* istanbul ignore next */
      cov_f1dj6urga().s[167]++;
      logger_1.logger.warn('AI Security Threat Detected', {
        threatId: threat.id,
        type: threat.type,
        severity: threat.severity,
        userId: threat.userId,
        sessionId: threat.sessionId,
        blocked: threat.blocked,
        timestamp: threat.detectedAt
      });
      // Update threat statistics
      /* istanbul ignore next */
      cov_f1dj6urga().s[168]++;
      await this.updateThreatStatistics(threat);
      // Alert security team for high severity threats
      /* istanbul ignore next */
      cov_f1dj6urga().s[169]++;
      if (
      /* istanbul ignore next */
      (cov_f1dj6urga().b[52][0]++, threat.severity === 'critical') ||
      /* istanbul ignore next */
      (cov_f1dj6urga().b[52][1]++, threat.severity === 'high')) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[51][0]++;
        cov_f1dj6urga().s[170]++;
        await this.alertSecurityTeam(threat);
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[51][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[171]++;
      logger_1.logger.error('Error logging AI security threat:', error);
    }
  }
  // Update threat statistics
  static async updateThreatStatistics(threat) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[18]++;
    cov_f1dj6urga().s[172]++;
    try {
      const today =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[173]++, new Date().toISOString().split('T')[0]);
      const statsKey =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[174]++, `ai_threat_stats:${today}`);
      const currentStats =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[175]++,
      /* istanbul ignore next */
      (cov_f1dj6urga().b[53][0]++, await (0, redis_1.getCache)(statsKey)) ||
      /* istanbul ignore next */
      (cov_f1dj6urga().b[53][1]++, {
        total: 0,
        byType: {},
        bySeverity: {},
        blocked: 0
      }));
      /* istanbul ignore next */
      cov_f1dj6urga().s[176]++;
      currentStats.total++;
      /* istanbul ignore next */
      cov_f1dj6urga().s[177]++;
      currentStats.byType[threat.type] = (
      /* istanbul ignore next */
      (cov_f1dj6urga().b[54][0]++, currentStats.byType[threat.type]) ||
      /* istanbul ignore next */
      (cov_f1dj6urga().b[54][1]++, 0)) + 1;
      /* istanbul ignore next */
      cov_f1dj6urga().s[178]++;
      currentStats.bySeverity[threat.severity] = (
      /* istanbul ignore next */
      (cov_f1dj6urga().b[55][0]++, currentStats.bySeverity[threat.severity]) ||
      /* istanbul ignore next */
      (cov_f1dj6urga().b[55][1]++, 0)) + 1;
      /* istanbul ignore next */
      cov_f1dj6urga().s[179]++;
      if (threat.blocked) {
        /* istanbul ignore next */
        cov_f1dj6urga().b[56][0]++;
        cov_f1dj6urga().s[180]++;
        currentStats.blocked++;
      } else
      /* istanbul ignore next */
      {
        cov_f1dj6urga().b[56][1]++;
      }
      cov_f1dj6urga().s[181]++;
      await (0, redis_1.setCache)(statsKey, currentStats, 86400 * 7); // Keep for 7 days
    } catch (error) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[182]++;
      logger_1.logger.error('Error updating threat statistics:', error);
    }
  }
