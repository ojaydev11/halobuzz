    }
  }
  // Get threat statistics for monitoring
  static async getThreatStatistics(days =
  /* istanbul ignore next */
  (cov_f1dj6urga().b[57][0]++, 7)) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[20]++;
    cov_f1dj6urga().s[186]++;
    try {
      const stats =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[187]++, {
        totalThreats: 0,
        threatsByType: {},
        threatsBySeverity: {},
        blockedThreats: 0,
        dailyBreakdown: {}
      });
      /* istanbul ignore next */
      cov_f1dj6urga().s[188]++;
      for (let i =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[189]++, 0); i < days; i++) {
        const date =
        /* istanbul ignore next */
        (cov_f1dj6urga().s[190]++, new Date());
        /* istanbul ignore next */
        cov_f1dj6urga().s[191]++;
        date.setDate(date.getDate() - i);
        const dateStr =
        /* istanbul ignore next */
        (cov_f1dj6urga().s[192]++, date.toISOString().split('T')[0]);
        const statsKey =
        /* istanbul ignore next */
        (cov_f1dj6urga().s[193]++, `ai_threat_stats:${dateStr}`);
        const dayStats =
        /* istanbul ignore next */
        (cov_f1dj6urga().s[194]++, await (0, redis_1.getCache)(statsKey));
        /* istanbul ignore next */
        cov_f1dj6urga().s[195]++;
        if (dayStats) {
          /* istanbul ignore next */
          cov_f1dj6urga().b[58][0]++;
          cov_f1dj6urga().s[196]++;
          stats.totalThreats += dayStats.total;
          /* istanbul ignore next */
          cov_f1dj6urga().s[197]++;
          stats.blockedThreats += dayStats.blocked;
          /* istanbul ignore next */
          cov_f1dj6urga().s[198]++;
          stats.dailyBreakdown[dateStr] = dayStats.total;
          // Aggregate by type and severity
          /* istanbul ignore next */
          cov_f1dj6urga().s[199]++;
          for (const [type, count] of Object.entries(dayStats.byType)) {
            /* istanbul ignore next */
            cov_f1dj6urga().s[200]++;
            stats.threatsByType[type] = (
            /* istanbul ignore next */
            (cov_f1dj6urga().b[59][0]++, stats.threatsByType[type]) ||
            /* istanbul ignore next */
            (cov_f1dj6urga().b[59][1]++, 0)) + count;
          }
          /* istanbul ignore next */
          cov_f1dj6urga().s[201]++;
          for (const [severity, count] of Object.entries(dayStats.bySeverity)) {
            /* istanbul ignore next */
            cov_f1dj6urga().s[202]++;
            stats.threatsBySeverity[severity] = (
            /* istanbul ignore next */
            (cov_f1dj6urga().b[60][0]++, stats.threatsBySeverity[severity]) ||
            /* istanbul ignore next */
            (cov_f1dj6urga().b[60][1]++, 0)) + count;
          }
        } else
        /* istanbul ignore next */
        {
          cov_f1dj6urga().b[58][1]++;
        }
      }
      /* istanbul ignore next */
      cov_f1dj6urga().s[203]++;
      return stats;
    } catch (error) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[204]++;
      logger_1.logger.error('Error getting threat statistics:', error);
      /* istanbul ignore next */
      cov_f1dj6urga().s[205]++;
      return {
        totalThreats: 0,
        threatsByType: {},
        threatsBySeverity: {},
        blockedThreats: 0,
        dailyBreakdown: {}
      };
    }
  }
  // Emergency AI shutdown system
  static async emergencyShutdown(reason, adminUserId) {
    /* istanbul ignore next */
    cov_f1dj6urga().f[21]++;
    cov_f1dj6urga().s[206]++;
    try {
      const shutdownId =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[207]++, crypto_1.default.randomUUID());
      const timestamp =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[208]++, new Date());
      // Set emergency shutdown flag
      /* istanbul ignore next */
      cov_f1dj6urga().s[209]++;
      await (0, redis_1.setCache)('ai_emergency_shutdown', {
        id: shutdownId,
        reason,
        adminUserId,
        timestamp,
        active: true
      }, 3600); // 1 hour
      /* istanbul ignore next */
      cov_f1dj6urga().s[210]++;
      logger_1.logger.error('AI EMERGENCY SHUTDOWN ACTIVATED', {
        shutdownId,
        reason,
        adminUserId,
        timestamp
      });
      const affectedSystems =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[211]++, ['ai_recommendations', 'content_generation', 'ai_moderation', 'conversation_ai', 'cultural_intelligence']);
      /* istanbul ignore next */
      cov_f1dj6urga().s[212]++;
      return {
        success: true,
        shutdownId,
        affectedSystems
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[213]++;
      logger_1.logger.error('Error in emergency AI shutdown:', error);
      /* istanbul ignore next */
      cov_f1dj6urga().s[214]++;
      return {
        success: false,
        shutdownId: '',
        affectedSystems: []
      };
    }
  }
  // Check if AI systems are under emergency shutdown
  static async isEmergencyShutdownActive() {
    /* istanbul ignore next */
    cov_f1dj6urga().f[22]++;
    cov_f1dj6urga().s[215]++;
    try {
      const shutdown =
      /* istanbul ignore next */
      (cov_f1dj6urga().s[216]++, await (0, redis_1.getCache)('ai_emergency_shutdown'));
      /* istanbul ignore next */
      cov_f1dj6urga().s[217]++;
      return /* istanbul ignore next */(cov_f1dj6urga().b[61][0]++, shutdown) &&
      /* istanbul ignore next */
      (cov_f1dj6urga().b[61][1]++, shutdown.active);
    } catch (error) {
      /* istanbul ignore next */
      cov_f1dj6urga().s[218]++;
      logger_1.logger.error('Error checking emergency shutdown status:', error);
      /* istanbul ignore next */
      cov_f1dj6urga().s[219]++;
      return false; // Default to allowing operations
    }
  }
