      if (!gifterStats.has(key)) {
        /* istanbul ignore next */
        cov_2mith1c5d2().b[31][0]++;
        cov_2mith1c5d2().s[95]++;
        gifterStats.set(key, {
          userId: key,
          username: tx.senderName,
          totalSpent: 0,
          giftCount: 0,
          topGift: ''
        });
      } else
      /* istanbul ignore next */
      {
        cov_2mith1c5d2().b[31][1]++;
      }
      const stats =
      /* istanbul ignore next */
      (cov_2mith1c5d2().s[96]++, gifterStats.get(key));
      /* istanbul ignore next */
      cov_2mith1c5d2().s[97]++;
      stats.totalSpent += tx.totalPrice;
      /* istanbul ignore next */
      cov_2mith1c5d2().s[98]++;
      stats.giftCount += tx.quantity;
      /* istanbul ignore next */
      cov_2mith1c5d2().s[99]++;
      if (
      /* istanbul ignore next */
      (cov_2mith1c5d2().b[33][0]++, !stats.topGift) ||
      /* istanbul ignore next */
      (cov_2mith1c5d2().b[33][1]++, tx.totalPrice > stats.totalSpent)) {
        /* istanbul ignore next */
        cov_2mith1c5d2().b[32][0]++;
        cov_2mith1c5d2().s[100]++;
        stats.topGift = tx.giftName;
      } else
      /* istanbul ignore next */
      {
        cov_2mith1c5d2().b[32][1]++;
      }
    });
    // Convert to array and sort
    const leaderboard =
    /* istanbul ignore next */
    (cov_2mith1c5d2().s[101]++, Array.from(gifterStats.values()).sort((a, b) => {
      /* istanbul ignore next */
      cov_2mith1c5d2().f[15]++;
      cov_2mith1c5d2().s[102]++;
      return b.totalSpent - a.totalSpent;
    }).slice(0, Number(limit)));
    /* istanbul ignore next */
    cov_2mith1c5d2().s[103]++;
    res.json({
      success: true,
      leaderboard,
      period
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2mith1c5d2().s[104]++;
    logger_1.logger.error('Error fetching gifters leaderboard:', error);
    /* istanbul ignore next */
    cov_2mith1c5d2().s[105]++;
    res.status(500).json({
      success: false,
      message: 'Failed to fetch gifters leaderboard',
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_2mith1c5d2().b[34][0]++, error.message) :
      /* istanbul ignore next */
      (cov_2mith1c5d2().b[34][1]++, 'Unknown error')
    });
  }
});
/* istanbul ignore next */
cov_2mith1c5d2().s[106]++;
exports.default = router;