  matchesHookTrigger(hook, activity) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[34]++;
    cov_2bnxa6qet3().s[161]++;
    switch (hook.trigger.condition) {
      case 'user_login':
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[40][0]++;
        cov_2bnxa6qet3().s[162]++;
        return activity.type === 'login';
      case 'daily_reset':
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[40][1]++;
        cov_2bnxa6qet3().s[163]++;
        return activity.type === 'daily_reset';
      case 'week_start':
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[40][2]++;
        cov_2bnxa6qet3().s[164]++;
        return activity.type === 'week_start';
      case 'month_start':
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[40][3]++;
        cov_2bnxa6qet3().s[165]++;
        return activity.type === 'month_start';
      case 'user_return_after_absence':
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[40][4]++;
        cov_2bnxa6qet3().s[166]++;
        return /* istanbul ignore next */(cov_2bnxa6qet3().b[41][0]++, activity.type === 'login') &&
        /* istanbul ignore next */
        (cov_2bnxa6qet3().b[41][1]++, activity.data?.daysSinceLastLogin >= 3);
      case 'peak_activity_detected':
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[40][5]++;
        cov_2bnxa6qet3().s[167]++;
        return activity.type === 'peak_activity';
      default:
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[40][6]++;
        cov_2bnxa6qet3().s[168]++;
        return false;
    }
  }
  async updateMilestoneProgress(userId, activity) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[35]++;
    const profile =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[169]++, await this.getUserRetentionProfile(userId));
