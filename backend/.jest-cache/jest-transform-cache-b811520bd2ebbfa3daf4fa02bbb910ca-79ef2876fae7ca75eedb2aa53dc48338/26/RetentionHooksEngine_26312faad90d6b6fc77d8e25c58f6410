    (cov_2bnxa6qet3().s[28]++, await this.getUserRetentionProfile(userId));
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[29]++;
    if (
    /* istanbul ignore next */
    (cov_2bnxa6qet3().b[3][0]++, !hook) ||
    /* istanbul ignore next */
    (cov_2bnxa6qet3().b[3][1]++, !this.shouldTriggerHook(profile, hook, context))) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[2][0]++;
      cov_2bnxa6qet3().s[30]++;
      return {
        success: false
      };
    } else
    /* istanbul ignore next */
    {
      cov_2bnxa6qet3().b[2][1]++;
    }
    // Check personalization fit
    cov_2bnxa6qet3().s[31]++;
    if (!this.isPersonalizedMatch(profile, hook)) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[4][0]++;
      cov_2bnxa6qet3().s[32]++;
      return {
        success: false
      };
