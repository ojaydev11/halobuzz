    /* istanbul ignore next */
    (cov_2bnxa6qet3().b[53][0]++, nextPreferredTime) ||
    /* istanbul ignore next */
    (cov_2bnxa6qet3().b[53][1]++, 18), 0, 0, 0);
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[207]++;
