  calculateOptimalTiming(profile) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[42]++;
    const preferredHours =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[202]++, profile.activity_pattern.preferred_times);
