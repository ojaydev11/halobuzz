  }
  analyzeHookEffectiveness(profile) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[44]++;
    const hookStats =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[210]++, new Map());
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[211]++;
    profile.engagement_history.hook_responses.forEach(response => {
      /* istanbul ignore next */
      cov_2bnxa6qet3().f[45]++;
      cov_2bnxa6qet3().s[212]++;
      if (!hookStats.has(response.hookId)) {
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[55][0]++;
        cov_2bnxa6qet3().s[213]++;
        hookStats.set(response.hookId, {
          triggered: 0,
          engaged: 0
        });
      } else
      /* istanbul ignore next */
      {
        cov_2bnxa6qet3().b[55][1]++;
      }
      const stats =
      /* istanbul ignore next */
      (cov_2bnxa6qet3().s[214]++, hookStats.get(response.hookId));
      /* istanbul ignore next */
      cov_2bnxa6qet3().s[215]++;
      stats.triggered++;
      /* istanbul ignore next */
      cov_2bnxa6qet3().s[216]++;
      if (response.response === 'engaged') {
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[56][0]++;
        cov_2bnxa6qet3().s[217]++;
        stats.engaged++;
      } else
      /* istanbul ignore next */
      {
        cov_2bnxa6qet3().b[56][1]++;
      }
    });
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[218]++;
    return Array.from(hookStats.entries()).map(([hookId, stats]) => {
      /* istanbul ignore next */
      cov_2bnxa6qet3().f[46]++;
      cov_2bnxa6qet3().s[219]++;
      return {
        hookId,
        effectiveness: stats.triggered > 0 ?
        /* istanbul ignore next */
        (cov_2bnxa6qet3().b[57][0]++, stats.engaged / stats.triggered) :
        /* istanbul ignore next */
        (cov_2bnxa6qet3().b[57][1]++, 0)
      };
    }).sort((a, b) => {
      /* istanbul ignore next */
      cov_2bnxa6qet3().f[47]++;
      cov_2bnxa6qet3().s[220]++;
      return b.effectiveness - a.effectiveness;
    });
  }
