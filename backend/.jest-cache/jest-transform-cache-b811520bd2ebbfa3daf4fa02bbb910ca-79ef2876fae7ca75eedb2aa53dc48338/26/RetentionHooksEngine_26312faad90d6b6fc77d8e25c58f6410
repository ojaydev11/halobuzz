    /* istanbul ignore next */
    cov_2bnxa6qet3().s[112]++;
    if (
    /* istanbul ignore next */
    (cov_2bnxa6qet3().b[21][0]++, profile.personalization_data.preferred_rewards.includes('coins')) &&
    /* istanbul ignore next */
    (cov_2bnxa6qet3().b[21][1]++, rewards.coins)) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[20][0]++;
