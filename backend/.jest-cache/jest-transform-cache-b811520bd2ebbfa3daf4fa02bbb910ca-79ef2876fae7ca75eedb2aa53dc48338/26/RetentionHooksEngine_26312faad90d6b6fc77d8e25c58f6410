        /* istanbul ignore next */
        {
          cov_2bnxa6qet3().b[43][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_2bnxa6qet3().b[42][1]++;
      }
    });
    // Similar logic for weekly and monthly goals...
  }
