      }
    }
  }
  shouldTriggerHook(profile, hook, context) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[19]++;
    // Check cooldown
    const lastTrigger =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[78]++, profile.engagement_history.hook_responses.filter(r => {
      /* istanbul ignore next */
      cov_2bnxa6qet3().f[20]++;
      cov_2bnxa6qet3().s[79]++;
      return r.hookId === hook.id;
    }).sort((a, b) => {
      /* istanbul ignore next */
      cov_2bnxa6qet3().f[21]++;
      cov_2bnxa6qet3().s[80]++;
      return b.timestamp.getTime() - a.timestamp.getTime();
    })[0]);
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[81]++;
    if (lastTrigger) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[11][0]++;
      const timeSinceLastTrigger =
      /* istanbul ignore next */
      (cov_2bnxa6qet3().s[82]++, Date.now() - lastTrigger.timestamp.getTime());
      /* istanbul ignore next */
      cov_2bnxa6qet3().s[83]++;
      if (timeSinceLastTrigger < hook.trigger.cooldown * 60 * 1000) {
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[12][0]++;
        cov_2bnxa6qet3().s[84]++;
        return false;
      } else
      /* istanbul ignore next */
      {
        cov_2bnxa6qet3().b[12][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_2bnxa6qet3().b[11][1]++;
    }
    // Check daily limits
    const today =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[85]++, new Date().toDateString());
    const todayTriggers =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[86]++, profile.engagement_history.hook_responses.filter(r => {
      /* istanbul ignore next */
      cov_2bnxa6qet3().f[22]++;
      cov_2bnxa6qet3().s[87]++;
      return /* istanbul ignore next */(cov_2bnxa6qet3().b[13][0]++, r.hookId === hook.id) &&
      /* istanbul ignore next */
      (cov_2bnxa6qet3().b[13][1]++, r.timestamp.toDateString() === today);
    }).length);
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[88]++;
    if (todayTriggers >= hook.trigger.maxTriggers) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[14][0]++;
      cov_2bnxa6qet3().s[89]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2bnxa6qet3().b[14][1]++;
    }
    // Check fatigue
    cov_2bnxa6qet3().s[90]++;
    if (profile.engagement_history.fatigue_indicators.includes(hook.id)) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[15][0]++;
      cov_2bnxa6qet3().s[91]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2bnxa6qet3().b[15][1]++;
    }
    cov_2bnxa6qet3().s[92]++;
    return true;
  }
