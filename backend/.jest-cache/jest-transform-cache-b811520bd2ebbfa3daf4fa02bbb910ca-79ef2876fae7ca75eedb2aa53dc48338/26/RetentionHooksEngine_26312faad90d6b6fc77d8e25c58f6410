  }
  async executeRetentionHook(userId, hook, context) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[24]++;
    const profile =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[100]++, await this.getUserRetentionProfile(userId));
    // Calculate personalized rewards
    let rewards =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[101]++, {
      ...hook.hooks.rewards.immediate
    });
    // Apply streak bonuses
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[102]++;
    if (hook.hooks.rewards.progressive.streak_bonus > 0) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[18][0]++;
      const streakMultiplier =
      /* istanbul ignore next */
      (cov_2bnxa6qet3().s[103]++, Math.max(1, profile.streak_data.current_daily_streak / 7));
      /* istanbul ignore next */
      cov_2bnxa6qet3().s[104]++;
      rewards.coins = Math.floor(rewards.coins * streakMultiplier);
    } else
    /* istanbul ignore next */
    {
      cov_2bnxa6qet3().b[18][1]++;
    }
    // Apply progressive rewards
    const progressiveReward =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[105]++, hook.hooks.rewards.progressive.escalating_rewards.find(r => {
      /* istanbul ignore next */
      cov_2bnxa6qet3().f[25]++;
      cov_2bnxa6qet3().s[106]++;
      return profile.streak_data.current_daily_streak >= r.threshold;
    }));
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[107]++;
    if (progressiveReward) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[19][0]++;
      cov_2bnxa6qet3().s[108]++;
      rewards = {
        ...rewards,
        ...progressiveReward.reward
      };
    } else
    /* istanbul ignore next */
    {
      cov_2bnxa6qet3().b[19][1]++;
    }
    // Apply personalization
    cov_2bnxa6qet3().s[109]++;
    rewards = this.personalizeRewards(rewards, profile);
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[110]++;
    return rewards;
  }
  personalizeRewards(rewards, profile) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[26]++;
