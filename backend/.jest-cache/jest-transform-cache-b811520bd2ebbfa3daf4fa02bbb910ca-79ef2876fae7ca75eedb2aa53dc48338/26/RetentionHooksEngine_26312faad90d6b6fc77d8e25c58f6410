    /* istanbul ignore next */
    (cov_2bnxa6qet3().b[25][0]++, response === 'engaged') &&
    /* istanbul ignore next */
    (cov_2bnxa6qet3().b[25][1]++, !profile.engagement_history.effective_hooks.includes(hookId))) {
