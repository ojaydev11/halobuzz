    } else
    /* istanbul ignore next */
    {
      cov_2bnxa6qet3().b[29][1]++;
    }
  }
  async updateChurnRisk(profile) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[31]++;
    const daysSinceLastActivity =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[129]++, Math.floor((Date.now() - profile.last_activity.getTime()) / (1000 * 60 * 60 * 24)));
    let churnProbability =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[130]++, 0);
    // Activity recency factor
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[131]++;
    if (daysSinceLastActivity > 7) {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[30][0]++;
      cov_2bnxa6qet3().s[132]++;
      churnProbability += 0.4;
    } else {
      /* istanbul ignore next */
      cov_2bnxa6qet3().b[30][1]++;
      cov_2bnxa6qet3().s[133]++;
      if (daysSinceLastActivity > 3) {
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[31][0]++;
        cov_2bnxa6qet3().s[134]++;
        churnProbability += 0.2;
      } else {
        /* istanbul ignore next */
        cov_2bnxa6qet3().b[31][1]++;
        cov_2bnxa6qet3().s[135]++;
        if (daysSinceLastActivity > 1) {
          /* istanbul ignore next */
          cov_2bnxa6qet3().b[32][0]++;
          cov_2bnxa6qet3().s[136]++;
          churnProbability += 0.1;
        } else
        /* istanbul ignore next */
        {
          cov_2bnxa6qet3().b[32][1]++;
        }
      }
    }
    // Engagement quality factor
    const engagementRate =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[137]++, profile.engagement_history.effective_hooks.length / Math.max(1, profile.engagement_history.hook_responses.length));
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[138]++;
    churnProbability += (1 - engagementRate) * 0.3;
    // Streak factor
    const streakFactor =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[139]++, Math.max(0, 1 - profile.streak_data.current_daily_streak / 30));
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[140]++;
    churnProbability += streakFactor * 0.2;
    // Fatigue factor
    const fatigueFactor =
    /* istanbul ignore next */
    (cov_2bnxa6qet3().s[141]++, profile.engagement_history.fatigue_indicators.length / 10);
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[142]++;
    churnProbability += Math.min(0.3, fatigueFactor);
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[143]++;
    profile.churn_probability = Math.min(1, Math.max(0, churnProbability));
    /* istanbul ignore next */
    cov_2bnxa6qet3().s[144]++;
    profile.risk_level = this.determineRiskLevel(profile.churn_probability);
  }
  determineRiskLevel(churnProbability) {
    /* istanbul ignore next */
    cov_2bnxa6qet3().f[32]++;
