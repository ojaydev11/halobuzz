  /**
   * Get cross-game global rankings
   */
  getGlobalPlayerRankings(limit =
  /* istanbul ignore next */
  (cov_2iaek2thu1().b[21][0]++, 100)) {
    /* istanbul ignore next */
    cov_2iaek2thu1().f[23]++;
    const allPlayers =
    /* istanbul ignore next */
    (cov_2iaek2thu1().s[93]++, new Map());
    // Aggregate player data across all games
    /* istanbul ignore next */
    cov_2iaek2thu1().s[94]++;
    for (const [gameCode, leaderboard] of this.globalLeaderboards.entries()) {
      /* istanbul ignore next */
      cov_2iaek2thu1().s[95]++;
      leaderboard.forEach(entry => {
        /* istanbul ignore next */
        cov_2iaek2thu1().f[24]++;
        cov_2iaek2thu1().s[96]++;
        if (!allPlayers.has(entry.playerId)) {
          /* istanbul ignore next */
          cov_2iaek2thu1().b[22][0]++;
          cov_2iaek2thu1().s[97]++;
          allPlayers.set(entry.playerId, {
            playerId: entry.playerId,
            username: entry.username,
            avatar: entry.avatar,
            country: entry.country,
            totalScore: 0,
            totalWinnings: 0,
            totalGames: 0,
            gamesWon: 0,
            achievements: new Set(),
            level: entry.level,
            gameStats: {}
          });
        } else
        /* istanbul ignore next */
        {
          cov_2iaek2thu1().b[22][1]++;
        }
        const player =
        /* istanbul ignore next */
        (cov_2iaek2thu1().s[98]++, allPlayers.get(entry.playerId));
        /* istanbul ignore next */
        cov_2iaek2thu1().s[99]++;
        player.totalScore += entry.score;
        /* istanbul ignore next */
        cov_2iaek2thu1().s[100]++;
        player.totalWinnings += entry.totalWinnings;
        /* istanbul ignore next */
        cov_2iaek2thu1().s[101]++;
        player.totalGames += entry.gamesPlayed;
        /* istanbul ignore next */
        cov_2iaek2thu1().s[102]++;
        player.gamesWon += Math.floor(entry.gamesPlayed * entry.winRate);
        /* istanbul ignore next */
        cov_2iaek2thu1().s[103]++;
        entry.achievements.forEach(achievement => {
          /* istanbul ignore next */
          cov_2iaek2thu1().f[25]++;
          cov_2iaek2thu1().s[104]++;
          return player.achievements.add(achievement);
        });
        /* istanbul ignore next */
        cov_2iaek2thu1().s[105]++;
        player.level = Math.max(player.level, entry.level);
        /* istanbul ignore next */
        cov_2iaek2thu1().s[106]++;
        player.gameStats[gameCode] = {
          rank: entry.rank,
          score: entry.score,
          winRate: entry.winRate
        };
      });
    }
    // Convert to array and sort by total score
    /* istanbul ignore next */
    cov_2iaek2thu1().s[107]++;
    return Array.from(allPlayers.values()).map(player => {
      /* istanbul ignore next */
      cov_2iaek2thu1().f[26]++;
      cov_2iaek2thu1().s[108]++;
      return {
        ...player,
        achievements: Array.from(player.achievements),
        overallWinRate: player.totalGames > 0 ?
        /* istanbul ignore next */
        (cov_2iaek2thu1().b[23][0]++, player.gamesWon / player.totalGames) :
        /* istanbul ignore next */
        (cov_2iaek2thu1().b[23][1]++, 0)
      };
    }).sort((a, b) => {
      /* istanbul ignore next */
      cov_2iaek2thu1().f[27]++;
      cov_2iaek2thu1().s[109]++;
      return b.totalScore - a.totalScore;
    }).slice(0, limit).map((player, index) => {
      /* istanbul ignore next */
      cov_2iaek2thu1().f[28]++;
      cov_2iaek2thu1().s[110]++;
      return {
        ...player,
        globalRank: index + 1
      };
    });
  }
