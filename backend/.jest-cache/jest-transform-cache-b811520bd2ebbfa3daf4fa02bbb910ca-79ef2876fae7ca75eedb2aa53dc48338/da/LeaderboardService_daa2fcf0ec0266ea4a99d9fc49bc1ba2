  /**
   * Update player stats after game completion
   */
  async updatePlayerStats(gameCode, playerId, gameResult) {
    /* istanbul ignore next */
    cov_2iaek2thu1().f[14]++;
    cov_2iaek2thu1().s[49]++;
    try {
      // Update global leaderboard
      const leaderboard =
      /* istanbul ignore next */
      (cov_2iaek2thu1().s[50]++,
      /* istanbul ignore next */
      (cov_2iaek2thu1().b[12][0]++, this.globalLeaderboards.get(gameCode)) ||
      /* istanbul ignore next */
      (cov_2iaek2thu1().b[12][1]++, []));
      let playerEntry =
      /* istanbul ignore next */
      (cov_2iaek2thu1().s[51]++, leaderboard.find(entry => {
        /* istanbul ignore next */
        cov_2iaek2thu1().f[15]++;
        cov_2iaek2thu1().s[52]++;
        return entry.playerId === playerId;
      }));
      /* istanbul ignore next */
      cov_2iaek2thu1().s[53]++;
      if (!playerEntry) {
        /* istanbul ignore next */
        cov_2iaek2thu1().b[13][0]++;
        cov_2iaek2thu1().s[54]++;
        // Create new player entry
        playerEntry = {
          playerId,
          username: `Player_${playerId}`,
          score: 0,
          rank: leaderboard.length + 1,
          gamesPlayed: 0,
          winRate: 0,
          totalWinnings: 0,
          level: 1,
          lastPlayed: new Date(),
          achievements: [],
          winStreak: 0,
          bestRank: 9999
        };
        /* istanbul ignore next */
        cov_2iaek2thu1().s[55]++;
        leaderboard.push(playerEntry);
      } else
      /* istanbul ignore next */
      {
        cov_2iaek2thu1().b[13][1]++;
      }
      // Update stats
      cov_2iaek2thu1().s[56]++;
      playerEntry.gamesPlayed += 1;
      /* istanbul ignore next */
      cov_2iaek2thu1().s[57]++;
      playerEntry.score += gameResult.score;
      /* istanbul ignore next */
      cov_2iaek2thu1().s[58]++;
      playerEntry.totalWinnings += gameResult.winnings;
      /* istanbul ignore next */
      cov_2iaek2thu1().s[59]++;
      playerEntry.lastPlayed = new Date();
      /* istanbul ignore next */
      cov_2iaek2thu1().s[60]++;
      if (gameResult.won) {
        /* istanbul ignore next */
        cov_2iaek2thu1().b[14][0]++;
        cov_2iaek2thu1().s[61]++;
        playerEntry.winStreak += 1;
      } else {
        /* istanbul ignore next */
        cov_2iaek2thu1().b[14][1]++;
        cov_2iaek2thu1().s[62]++;
        playerEntry.winStreak = 0;
      }
      /* istanbul ignore next */
      cov_2iaek2thu1().s[63]++;
      playerEntry.winRate = playerEntry.gamesPlayed > 0 ?
      /* istanbul ignore next */
      (cov_2iaek2thu1().b[15][0]++, (playerEntry.winStreak + (playerEntry.gamesPlayed - playerEntry.winStreak) * playerEntry.winRate) / playerEntry.gamesPlayed) :
      /* istanbul ignore next */
      (cov_2iaek2thu1().b[15][1]++, 0);
      // Recalculate rankings
      /* istanbul ignore next */
      cov_2iaek2thu1().s[64]++;
      leaderboard.sort((a, b) => {
        /* istanbul ignore next */
        cov_2iaek2thu1().f[16]++;
        cov_2iaek2thu1().s[65]++;
        return b.score - a.score;
      });
      /* istanbul ignore next */
      cov_2iaek2thu1().s[66]++;
      leaderboard.forEach((entry, index) => {
        /* istanbul ignore next */
        cov_2iaek2thu1().f[17]++;
        cov_2iaek2thu1().s[67]++;
        entry.rank = index + 1;
        /* istanbul ignore next */
        cov_2iaek2thu1().s[68]++;
        if (entry.playerId === playerId) {
          /* istanbul ignore next */
          cov_2iaek2thu1().b[16][0]++;
          cov_2iaek2thu1().s[69]++;
          entry.bestRank = Math.min(entry.bestRank, entry.rank);
        } else
        /* istanbul ignore next */
        {
          cov_2iaek2thu1().b[16][1]++;
        }
      });
      /* istanbul ignore next */
      cov_2iaek2thu1().s[70]++;
      this.globalLeaderboards.set(gameCode, leaderboard);
      // Broadcast leaderboard update
      /* istanbul ignore next */
      cov_2iaek2thu1().s[71]++;
      MultiplayerSocketService_1.multiplayerSocketService.broadcastLeaderboardUpdate(gameCode, leaderboard.slice(0, 10));
      /* istanbul ignore next */
      cov_2iaek2thu1().s[72]++;
      logger_1.logger.info(`Updated player stats for ${playerId} in ${gameCode}`);
    } catch (error) {
      /* istanbul ignore next */
      cov_2iaek2thu1().s[73]++;
      logger_1.logger.error('Error updating player stats:', error);
    }
  }
