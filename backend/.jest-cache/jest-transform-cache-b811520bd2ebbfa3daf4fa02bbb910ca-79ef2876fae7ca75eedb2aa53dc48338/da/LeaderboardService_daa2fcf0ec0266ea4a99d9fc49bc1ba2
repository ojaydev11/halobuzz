  /**
   * Start periodic updates for leaderboards
   */
  startPeriodicUpdates() {
    /* istanbul ignore next */
    cov_2iaek2thu1().f[30]++;
    cov_2iaek2thu1().s[112]++;
    // Update leaderboards every 5 minutes
    setInterval(() => {
      /* istanbul ignore next */
      cov_2iaek2thu1().f[31]++;
      cov_2iaek2thu1().s[113]++;
      this.updateLeaderboardRankings();
    }, 5 * 60 * 1000);
    // Reset weekly leaderboards every week
    /* istanbul ignore next */
    cov_2iaek2thu1().s[114]++;
    setInterval(() => {
      /* istanbul ignore next */
      cov_2iaek2thu1().f[32]++;
      cov_2iaek2thu1().s[115]++;
      this.resetWeeklyLeaderboards();
    }, 7 * 24 * 60 * 60 * 1000);
    // Reset monthly leaderboards every month
    /* istanbul ignore next */
    cov_2iaek2thu1().s[116]++;
    setInterval(() => {
      /* istanbul ignore next */
      cov_2iaek2thu1().f[33]++;
      cov_2iaek2thu1().s[117]++;
      this.resetMonthlyLeaderboards();
    }, 30 * 24 * 60 * 60 * 1000);
    // Check tournament statuses
    /* istanbul ignore next */
    cov_2iaek2thu1().s[118]++;
    setInterval(() => {
      /* istanbul ignore next */
      cov_2iaek2thu1().f[34]++;
      cov_2iaek2thu1().s[119]++;
      this.updateTournamentStatuses();
    }, 60 * 1000); // Every minute
  }
  /**
   * Update leaderboard rankings
   */
  updateLeaderboardRankings() {
    /* istanbul ignore next */
    cov_2iaek2thu1().f[35]++;
    cov_2iaek2thu1().s[120]++;
    for (const [gameCode, leaderboard] of this.globalLeaderboards.entries()) {
      /* istanbul ignore next */
      cov_2iaek2thu1().s[121]++;
      leaderboard.sort((a, b) => {
        /* istanbul ignore next */
        cov_2iaek2thu1().f[36]++;
        cov_2iaek2thu1().s[122]++;
        return b.score - a.score;
      });
      /* istanbul ignore next */
      cov_2iaek2thu1().s[123]++;
      leaderboard.forEach((entry, index) => {
        /* istanbul ignore next */
        cov_2iaek2thu1().f[37]++;
        cov_2iaek2thu1().s[124]++;
        entry.rank = index + 1;
      });
    }
  }
