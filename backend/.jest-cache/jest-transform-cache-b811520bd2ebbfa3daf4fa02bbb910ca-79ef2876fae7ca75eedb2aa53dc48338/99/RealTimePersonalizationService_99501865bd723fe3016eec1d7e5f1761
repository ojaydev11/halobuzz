        /* istanbul ignore next */
        (cov_22qg0r8bj2().s[50]++, {
          high: 3,
          medium: 2,
          low: 1
        });
        /* istanbul ignore next */
        cov_22qg0r8bj2().s[51]++;
        return /* istanbul ignore next */(cov_22qg0r8bj2().b[0][0]++, priorityOrder[b.priority] - priorityOrder[a.priority]) ||
        /* istanbul ignore next */
        (cov_22qg0r8bj2().b[0][1]++, b.personalizationScore - a.personalizationScore);
      }));
      // Log notification personalization
