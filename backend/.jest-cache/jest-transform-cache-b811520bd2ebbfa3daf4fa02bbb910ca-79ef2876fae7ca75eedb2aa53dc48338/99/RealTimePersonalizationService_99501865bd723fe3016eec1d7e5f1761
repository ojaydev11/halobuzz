      /* istanbul ignore next */
      (cov_22qg0r8bj2().s[48]++, await this.applyNotificationPersonalization(userContext, notificationCandidates));
      // Sort by priority and personalization score
      const sortedNotifications =
      /* istanbul ignore next */
      (cov_22qg0r8bj2().s[49]++, personalizedNotifications.sort((a, b) => {
        /* istanbul ignore next */
        cov_22qg0r8bj2().f[11]++;
