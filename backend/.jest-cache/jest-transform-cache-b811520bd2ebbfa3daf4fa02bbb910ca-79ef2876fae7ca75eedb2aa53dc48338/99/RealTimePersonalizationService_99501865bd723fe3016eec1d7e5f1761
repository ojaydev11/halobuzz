  /**
   * Get time range in milliseconds
   */
  getTimeRangeMs(timeRange) {
    /* istanbul ignore next */
    cov_22qg0r8bj2().f[42]++;
    cov_22qg0r8bj2().s[219]++;
    switch (timeRange) {
      case 'hour':
        /* istanbul ignore next */
        cov_22qg0r8bj2().b[40][0]++;
        cov_22qg0r8bj2().s[220]++;
        return 60 * 60 * 1000;
      case 'day':
        /* istanbul ignore next */
        cov_22qg0r8bj2().b[40][1]++;
        cov_22qg0r8bj2().s[221]++;
        return 24 * 60 * 60 * 1000;
      case 'week':
        /* istanbul ignore next */
        cov_22qg0r8bj2().b[40][2]++;
        cov_22qg0r8bj2().s[222]++;
        return 7 * 24 * 60 * 60 * 1000;
      case 'month':
        /* istanbul ignore next */
        cov_22qg0r8bj2().b[40][3]++;
        cov_22qg0r8bj2().s[223]++;
        return 30 * 24 * 60 * 60 * 1000;
      default:
        /* istanbul ignore next */
        cov_22qg0r8bj2().b[40][4]++;
        cov_22qg0r8bj2().s[224]++;
        return 24 * 60 * 60 * 1000;
    }
  }
