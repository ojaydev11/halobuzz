    /* istanbul ignore next */
    (cov_22qg0r8bj2().s[181]++, []);
    /* istanbul ignore next */
    cov_22qg0r8bj2().s[182]++;
    for (const candidate of candidates) {
      let personalizationScore =
      /* istanbul ignore next */
      (cov_22qg0r8bj2().s[183]++, 0.5);
      const reasons =
      /* istanbul ignore next */
      (cov_22qg0r8bj2().s[184]++, []);
      // Check notification preferences
      /* istanbul ignore next */
      cov_22qg0r8bj2().s[185]++;
