  /**
   * Translate text
   */
  async translateText(text, sourceLanguage, targetLanguage) {
    /* istanbul ignore next */
    cov_2hsec9c4i5().f[6]++;
    cov_2hsec9c4i5().s[58]++;
    try {
      // Check cache first
      const cacheKey =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[59]++, `translation:${sourceLanguage}:${targetLanguage}:${text}`);
      const cachedTranslation =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[60]++, this.translationCache.get(cacheKey));
      /* istanbul ignore next */
      cov_2hsec9c4i5().s[61]++;
      if (cachedTranslation) {
        /* istanbul ignore next */
        cov_2hsec9c4i5().b[17][0]++;
        cov_2hsec9c4i5().s[62]++;
        return cachedTranslation;
      } else
      /* istanbul ignore next */
      {
        cov_2hsec9c4i5().b[17][1]++;
      }
      cov_2hsec9c4i5().s[63]++;
      if (process.env.AI_ENGINE_URL) {
        /* istanbul ignore next */
        cov_2hsec9c4i5().b[18][0]++;
        const response =
        /* istanbul ignore next */
        (cov_2hsec9c4i5().s[64]++, await axios_1.default.post(`${process.env.AI_ENGINE_URL}/internal/translation/translate`, {
          text,
          sourceLanguage,
          targetLanguage
        }, {
          headers: {
            'Authorization': `Bearer ${process.env.AI_ENGINE_SECRET}`,
            'Content-Type': 'application/json'
          },
          timeout: 10000
        }));
        const result =
        /* istanbul ignore next */
        (cov_2hsec9c4i5().s[65]++,
        /* istanbul ignore next */
        (cov_2hsec9c4i5().b[19][0]++, response.data) ||
        /* istanbul ignore next */
        (cov_2hsec9c4i5().b[19][1]++, {}));
        const translation =
        /* istanbul ignore next */
        (cov_2hsec9c4i5().s[66]++, {
          translatedText:
          /* istanbul ignore next */
          (cov_2hsec9c4i5().b[20][0]++, result.translatedText) ||
          /* istanbul ignore next */
          (cov_2hsec9c4i5().b[20][1]++, text),
          confidence:
          /* istanbul ignore next */
          (cov_2hsec9c4i5().b[21][0]++, result.confidence) ||
          /* istanbul ignore next */
          (cov_2hsec9c4i5().b[21][1]++, 0.8),
          detectedLanguage: result.detectedLanguage,
          alternatives: result.alternatives
        });
        // Cache translation
        /* istanbul ignore next */
        cov_2hsec9c4i5().s[67]++;
        this.translationCache.set(cacheKey, translation);
        /* istanbul ignore next */
        cov_2hsec9c4i5().s[68]++;
        return translation;
      } else
      /* istanbul ignore next */
      {
        cov_2hsec9c4i5().b[18][1]++;
      }
      // Fallback: Return original text
      cov_2hsec9c4i5().s[69]++;
      return {
        translatedText: text,
        confidence: 0.5,
        detectedLanguage: sourceLanguage
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_2hsec9c4i5().s[70]++;
      this.logger.error('Error translating text:', error);
      /* istanbul ignore next */
      cov_2hsec9c4i5().s[71]++;
      return {
        translatedText: text,
        confidence: 0.0,
        detectedLanguage: sourceLanguage
      };
    }
  }
