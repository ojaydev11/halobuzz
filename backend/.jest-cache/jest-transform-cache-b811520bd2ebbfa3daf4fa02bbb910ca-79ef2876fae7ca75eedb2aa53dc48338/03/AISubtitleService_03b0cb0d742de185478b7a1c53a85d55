      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[43]++,
      /* istanbul ignore next */
      (cov_2hsec9c4i5().b[16][0]++, language) ||
      /* istanbul ignore next */
      (cov_2hsec9c4i5().b[16][1]++, await this.detectLanguage(text)));
      // Split text into segments
      const segments =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[44]++, this.splitTextIntoSegments(text));
      const subtitles =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[45]++, segments.map((segment, index) => {
        /* istanbul ignore next */
        cov_2hsec9c4i5().f[4]++;
        cov_2hsec9c4i5().s[46]++;
        return {
          startTime: index * 3,
          // 3 seconds per segment
          endTime: (index + 1) * 3,
          text: segment,
          language: detectedLanguage,
          confidence: 0.9
        };
      }));
      /* istanbul ignore next */
      cov_2hsec9c4i5().s[47]++;
      return {
        success: true,
        subtitles,
