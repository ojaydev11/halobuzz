  /**
   * Split text into segments
   */
  splitTextIntoSegments(text) {
    /* istanbul ignore next */
    cov_2hsec9c4i5().f[9]++;
    const sentences =
    /* istanbul ignore next */
    (cov_2hsec9c4i5().s[100]++, text.split(/[.!?]+/).filter(s => {
      /* istanbul ignore next */
      cov_2hsec9c4i5().f[10]++;
      cov_2hsec9c4i5().s[101]++;
      return s.trim().length > 0;
    }));
    const segments =
    /* istanbul ignore next */
    (cov_2hsec9c4i5().s[102]++, []);
    /* istanbul ignore next */
    cov_2hsec9c4i5().s[103]++;
    for (const sentence of sentences) {
      const words =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[104]++, sentence.trim().split(' '));
      const maxWordsPerSegment =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[105]++, 8);
      /* istanbul ignore next */
      cov_2hsec9c4i5().s[106]++;
      for (let i =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[107]++, 0); i < words.length; i += maxWordsPerSegment) {
        const segment =
        /* istanbul ignore next */
        (cov_2hsec9c4i5().s[108]++, words.slice(i, i + maxWordsPerSegment).join(' '));
        /* istanbul ignore next */
        cov_2hsec9c4i5().s[109]++;
        if (segment.trim()) {
          /* istanbul ignore next */
          cov_2hsec9c4i5().b[43][0]++;
          cov_2hsec9c4i5().s[110]++;
          segments.push(segment.trim());
        } else
        /* istanbul ignore next */
        {
          cov_2hsec9c4i5().b[43][1]++;
        }
      }
    }
    /* istanbul ignore next */
    cov_2hsec9c4i5().s[111]++;
    return segments;
  }
