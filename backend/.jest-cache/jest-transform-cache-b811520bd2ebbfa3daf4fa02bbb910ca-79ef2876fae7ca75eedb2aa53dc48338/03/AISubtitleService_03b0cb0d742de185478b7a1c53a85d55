  /**
   * Get subtitles for stream
   */
  async getStreamSubtitles(streamId, language =
  /* istanbul ignore next */
  (cov_2hsec9c4i5().b[45][0]++, 'en')) {
    /* istanbul ignore next */
    cov_2hsec9c4i5().f[13]++;
    cov_2hsec9c4i5().s[117]++;
    try {
      const cacheKey =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[118]++, `subtitles:${streamId}:${language}`);
      const cachedResult =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[119]++, await (0, redis_1.getCache)(cacheKey));
      /* istanbul ignore next */
      cov_2hsec9c4i5().s[120]++;
