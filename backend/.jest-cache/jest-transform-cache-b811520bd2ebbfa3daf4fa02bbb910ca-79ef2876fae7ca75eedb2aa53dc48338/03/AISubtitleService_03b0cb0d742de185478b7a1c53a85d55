      /* istanbul ignore next */
      {
        cov_2hsec9c4i5().b[47][1]++;
      }
      // Check if subtitles are available (using a different approach since metadata doesn't exist)
      const subtitleInfo =
      /* istanbul ignore next */
      (cov_2hsec9c4i5().s[125]++, stream.metadata?.subtitles);
      /* istanbul ignore next */
      cov_2hsec9c4i5().s[126]++;
