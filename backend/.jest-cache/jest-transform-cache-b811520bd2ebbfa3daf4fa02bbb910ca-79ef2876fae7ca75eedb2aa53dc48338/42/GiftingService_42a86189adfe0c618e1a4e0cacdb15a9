  async updateLiveStreamGifting(streamId, hostId, senderId, gift, quantity, isAnonymous) {
    /* istanbul ignore next */
    cov_103e59fyd1().f[22]++;
    let streamData =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[144]++, this.activeStreams.get(streamId));
    /* istanbul ignore next */
    cov_103e59fyd1().s[145]++;
    if (!streamData) {
      /* istanbul ignore next */
      cov_103e59fyd1().b[38][0]++;
      cov_103e59fyd1().s[146]++;
      streamData = await this.startLiveStreamGifting(streamId, hostId);
    } else
    /* istanbul ignore next */
    {
      cov_103e59fyd1().b[38][1]++;
    }
    const totalCost =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[147]++, gift.price * quantity);
    const hostEarnings =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[148]++, gift.hostEarning * quantity);
    // Update stream totals
    /* istanbul ignore next */
    cov_103e59fyd1().s[149]++;
    streamData.totalGiftsReceived += quantity;
    /* istanbul ignore next */
    cov_103e59fyd1().s[150]++;
    streamData.totalEarnings += hostEarnings;
    // Update top gifters
    let gifterIndex =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[151]++, streamData.topGifters.findIndex(g => {
      /* istanbul ignore next */
      cov_103e59fyd1().f[23]++;
      cov_103e59fyd1().s[152]++;
      return g.userId === senderId;
    }));
    /* istanbul ignore next */
    cov_103e59fyd1().s[153]++;
    if (gifterIndex === -1) {
      /* istanbul ignore next */
      cov_103e59fyd1().b[39][0]++;
      cov_103e59fyd1().s[154]++;
      streamData.topGifters.push({
        userId: senderId,
        totalSpent: totalCost,
        giftCount: quantity
      });
      /* istanbul ignore next */
      cov_103e59fyd1().s[155]++;
      gifterIndex = streamData.topGifters.length - 1;
    } else {
      /* istanbul ignore next */
      cov_103e59fyd1().b[39][1]++;
      cov_103e59fyd1().s[156]++;
      streamData.topGifters[gifterIndex].totalSpent += totalCost;
      /* istanbul ignore next */
      cov_103e59fyd1().s[157]++;
      streamData.topGifters[gifterIndex].giftCount += quantity;
    }
    // Sort top gifters
    /* istanbul ignore next */
    cov_103e59fyd1().s[158]++;
    streamData.topGifters.sort((a, b) => {
      /* istanbul ignore next */
      cov_103e59fyd1().f[24]++;
      cov_103e59fyd1().s[159]++;
      return b.totalSpent - a.totalSpent;
    });
    /* istanbul ignore next */
    cov_103e59fyd1().s[160]++;
    streamData.topGifters = streamData.topGifters.slice(0, 20);
    // Add to recent gifts
    /* istanbul ignore next */
    cov_103e59fyd1().s[161]++;
    streamData.recentGifts.unshift({
      senderId: isAnonymous ?
      /* istanbul ignore next */
      (cov_103e59fyd1().b[40][0]++, 'anonymous') :
      /* istanbul ignore next */
      (cov_103e59fyd1().b[40][1]++, senderId),
      gift,
      quantity,
      timestamp: new Date(),
      isAnonymous
    });
    // Keep only last 50 recent gifts
    /* istanbul ignore next */
    cov_103e59fyd1().s[162]++;
    streamData.recentGifts = streamData.recentGifts.slice(0, 50);
    // Update gifting goals
    /* istanbul ignore next */
    cov_103e59fyd1().s[163]++;
    streamData.giftingGoals.forEach(goal => {
      /* istanbul ignore next */
      cov_103e59fyd1().f[25]++;
      cov_103e59fyd1().s[164]++;
      goal.current = streamData.totalEarnings;
    });
    /* istanbul ignore next */
    cov_103e59fyd1().s[165]++;
    this.activeStreams.set(streamId, streamData);
    // Emit real-time update
    /* istanbul ignore next */
    cov_103e59fyd1().s[166]++;
    this.emit('liveStreamGiftUpdate', {
      streamId,
      hostId,
      senderId: isAnonymous ?
      /* istanbul ignore next */
      (cov_103e59fyd1().b[41][0]++, null) :
      /* istanbul ignore next */
      (cov_103e59fyd1().b[41][1]++, senderId),
      gift,
      quantity,
      newTotals: {
        totalEarnings: streamData.totalEarnings,
        totalGifts: streamData.totalGiftsReceived
      }
