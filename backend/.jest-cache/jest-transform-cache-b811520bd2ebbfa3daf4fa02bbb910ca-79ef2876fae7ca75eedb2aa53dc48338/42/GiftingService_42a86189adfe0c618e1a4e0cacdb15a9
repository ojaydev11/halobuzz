  }
  async buildGiftingStats(userId) {
    /* istanbul ignore next */
    cov_103e59fyd1().f[26]++;
    const [sentStats, receivedStats] =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[167]++, await Promise.all([CoinTransaction_1.CoinTransaction.aggregate([{
      $match: {
        userId,
        type: 'gift_sent',
        status: 'completed'
      }
    }, {
      $group: {
        _id: null,
        totalGifts: {
          $sum: 1
        },
        totalSpent: {
          $sum: '$amount'
        },
        recipients: {
          $addToSet: '$targetUserId'
        },
        lastGiftAt: {
          $max: '$createdAt'
        }
      }
    }]), CoinTransaction_1.CoinTransaction.aggregate([{
      $match: {
        userId,
        type: 'gift_received',
        status: 'completed'
      }
    }, {
      $group: {
        _id: null,
        totalGifts: {
          $sum: 1
        },
        totalEarned: {
          $sum: '$amount'
        },
        gifters: {
          $addToSet: '$targetUserId'
        },
        giftIds: {
          $push: '$context.giftId'
        }
      }
    }])]));
    const sent =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[168]++,
    /* istanbul ignore next */
    (cov_103e59fyd1().b[42][0]++, sentStats[0]) ||
    /* istanbul ignore next */
    (cov_103e59fyd1().b[42][1]++, {}));
    const received =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[169]++,
    /* istanbul ignore next */
    (cov_103e59fyd1().b[43][0]++, receivedStats[0]) ||
    /* istanbul ignore next */
    (cov_103e59fyd1().b[43][1]++, {}));
    /* istanbul ignore next */
    cov_103e59fyd1().s[170]++;
    return {
      totalGiftsSent:
      /* istanbul ignore next */
      (cov_103e59fyd1().b[44][0]++, sent.totalGifts) ||
      /* istanbul ignore next */
      (cov_103e59fyd1().b[44][1]++, 0),
      totalGiftsReceived:
      /* istanbul ignore next */
      (cov_103e59fyd1().b[45][0]++, received.totalGifts) ||
      /* istanbul ignore next */
      (cov_103e59fyd1().b[45][1]++, 0),
      totalCoinsSpent:
      /* istanbul ignore next */
      (cov_103e59fyd1().b[46][0]++, sent.totalSpent) ||
      /* istanbul ignore next */
      (cov_103e59fyd1().b[46][1]++, 0),
      totalCoinsEarned:
      /* istanbul ignore next */
      (cov_103e59fyd1().b[47][0]++, received.totalEarned) ||
      /* istanbul ignore next */
      (cov_103e59fyd1().b[47][1]++, 0),
      favoriteGifts:
      /* istanbul ignore next */
      (cov_103e59fyd1().b[48][0]++, received.giftIds?.slice(0, 5)) ||
      /* istanbul ignore next */
      (cov_103e59fyd1().b[48][1]++, []),
      topRecipients:
      /* istanbul ignore next */
      (cov_103e59fyd1().b[49][0]++, sent.recipients?.slice(0, 5)) ||
      /* istanbul ignore next */
      (cov_103e59fyd1().b[49][1]++, []),
      giftingStreak: 0,
      // Would calculate based on consecutive days
      lastGiftAt: sent.lastGiftAt
    };
  }
