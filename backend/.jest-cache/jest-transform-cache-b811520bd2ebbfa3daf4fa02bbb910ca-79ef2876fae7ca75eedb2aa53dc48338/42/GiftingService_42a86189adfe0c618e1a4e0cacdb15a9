  async getTopRecipients(timeframe =
  /* istanbul ignore next */
  (cov_103e59fyd1().b[26][0]++, 'weekly'), limit =
  /* istanbul ignore next */
  (cov_103e59fyd1().b[27][0]++, 50)) {
    /* istanbul ignore next */
