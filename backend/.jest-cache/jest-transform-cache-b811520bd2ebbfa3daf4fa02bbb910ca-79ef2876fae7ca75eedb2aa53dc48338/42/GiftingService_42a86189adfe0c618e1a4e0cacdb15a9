    /* istanbul ignore next */
    cov_103e59fyd1().s[175]++;
    return /* istanbul ignore next */(cov_103e59fyd1().b[51][0]++, now >= event.startDate) &&
    /* istanbul ignore next */
    (cov_103e59fyd1().b[51][1]++, now <= event.endDate);
  }
