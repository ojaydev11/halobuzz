          cov_103e59fyd1().s[189]++;
          logger_1.logger.info(`Special event ended: ${event.name}`);
        } else
        /* istanbul ignore next */
        {
          cov_103e59fyd1().b[52][1]++;
        }
      }
    }, 3600000); // 1 hour
  }
  async updateGiftingTrends() {
    /* istanbul ignore next */
    cov_103e59fyd1().f[33]++;
    const dailyStats =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[190]++, await CoinTransaction_1.CoinTransaction.aggregate([{
      $match: {
        type: {
          $in: ['gift_sent', 'gift_received']
        },
        createdAt: {
          $gte: new Date(Date.now() - 24 * 60 * 60 * 1000)
        }
      }
    }, {
      $group: {
        _id: {
          type: '$type',
          hour: {
            $hour: '$createdAt'
          }
        },
        count: {
          $sum: 1
        },
        totalAmount: {
          $sum: '$amount'
        }
      }
    }]));
    /* istanbul ignore next */
    cov_103e59fyd1().s[191]++;
    this.emit('giftingTrends', {
      dailyStats,
      timestamp: new Date()
    });
  }
  async checkGiftingAnomalies() {
    /* istanbul ignore next */
    cov_103e59fyd1().f[34]++;
    // Check for unusual gifting patterns
    const unusualActivity =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[192]++, await CoinTransaction_1.CoinTransaction.find({
      type: 'gift_sent',
      amount: {
        $gte: 5000
      },
      // Large gifts
      createdAt: {
        $gte: new Date(Date.now() - 60 * 60 * 1000)
      } // Last hour
    }).limit(10));
    /* istanbul ignore next */
    cov_103e59fyd1().s[193]++;
    if (unusualActivity.length > 5) {
      /* istanbul ignore next */
      cov_103e59fyd1().b[53][0]++;
      cov_103e59fyd1().s[194]++;
      this.emit('giftingAnomaly', {
        type: 'high_value_gifts',
        count: unusualActivity.length,
        details: unusualActivity
      });
    } else
    /* istanbul ignore next */
    {
      cov_103e59fyd1().b[53][1]++;
    }
  }
