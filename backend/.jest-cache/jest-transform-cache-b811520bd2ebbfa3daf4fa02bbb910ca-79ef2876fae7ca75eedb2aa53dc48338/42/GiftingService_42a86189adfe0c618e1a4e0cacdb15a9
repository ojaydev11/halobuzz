  /**
   * Get top gifters leaderboard
   */
  async getTopGifters(timeframe =
  /* istanbul ignore next */
  (cov_103e59fyd1().b[22][0]++, 'weekly'), limit =
  /* istanbul ignore next */
  (cov_103e59fyd1().b[23][0]++, 50)) {
    /* istanbul ignore next */
    cov_103e59fyd1().f[15]++;
    const timeframeDays =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[97]++, timeframe === 'daily' ?
    /* istanbul ignore next */
    (cov_103e59fyd1().b[24][0]++, 1) :
    /* istanbul ignore next */
    (cov_103e59fyd1().b[24][1]++, timeframe === 'weekly' ?
    /* istanbul ignore next */
    (cov_103e59fyd1().b[25][0]++, 7) :
    /* istanbul ignore next */
    (cov_103e59fyd1().b[25][1]++, 30)));
    const since =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[98]++, new Date(Date.now() - timeframeDays * 24 * 60 * 60 * 1000));
    const topGifters =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[99]++, await CoinTransaction_1.CoinTransaction.aggregate([{
      $match: {
        type: 'gift_sent',
        status: 'completed',
        createdAt: {
          $gte: since
        }
      }
    }, {
      $group: {
        _id: '$userId',
        totalGifted: {
          $sum: '$amount'
        },
        giftCount: {
          $sum: 1
        },
        uniqueRecipients: {
          $addToSet: '$targetUserId'
        },
        lastGiftAt: {
          $max: '$createdAt'
        }
      }
    }, {
      $addFields: {
        uniqueRecipientCount: {
          $size: '$uniqueRecipients'
        }
      }
    }, {
      $sort: {
        totalGifted: -1
      }
    }, {
      $limit: limit
    }]));
    /* istanbul ignore next */
    cov_103e59fyd1().s[100]++;
    return topGifters;
  }
  /**
   * Get top gift recipients
