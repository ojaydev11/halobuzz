      } catch (error) {
        /* istanbul ignore next */
        cov_103e59fyd1().s[180]++;
        logger_1.logger.error('Error in gifting analytics:', error);
      }
    }, 600000); // 10 minutes
  }
  startSpecialEventMonitoring() {
    /* istanbul ignore next */
    cov_103e59fyd1().f[30]++;
    cov_103e59fyd1().s[181]++;
    // Check special events every hour
    setInterval(async () => {
      /* istanbul ignore next */
      cov_103e59fyd1().f[31]++;
      const now =
      /* istanbul ignore next */
      (cov_103e59fyd1().s[182]++, new Date());
      /* istanbul ignore next */
      cov_103e59fyd1().s[183]++;
      for (const [eventId, event] of this.specialEvents) {
        /* istanbul ignore next */
        cov_103e59fyd1().s[184]++;
        if (now > event.endDate) {
          /* istanbul ignore next */
          cov_103e59fyd1().b[52][0]++;
          cov_103e59fyd1().s[185]++;
          // Remove expired special gifts
          event.specialGifts.forEach(gift => {
            /* istanbul ignore next */
            cov_103e59fyd1().f[32]++;
            cov_103e59fyd1().s[186]++;
            this.availableGifts.delete(gift.id);
          });
          /* istanbul ignore next */
          cov_103e59fyd1().s[187]++;
          this.specialEvents.delete(eventId);
          /* istanbul ignore next */
          cov_103e59fyd1().s[188]++;
          this.emit('specialEventEnded', {
            eventId,
            event
          });
