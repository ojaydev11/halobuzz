  /**
   * Private helper methods
   */
  determineGiftCategory(price) {
    /* istanbul ignore next */
    cov_103e59fyd1().f[19]++;
    cov_103e59fyd1().s[112]++;
    if (price >= 1000) {
      /* istanbul ignore next */
      cov_103e59fyd1().b[30][0]++;
      cov_103e59fyd1().s[113]++;
      return 'vip';
    } else
    /* istanbul ignore next */
    {
      cov_103e59fyd1().b[30][1]++;
    }
    cov_103e59fyd1().s[114]++;
    if (price >= 100) {
      /* istanbul ignore next */
      cov_103e59fyd1().b[31][0]++;
      cov_103e59fyd1().s[115]++;
      return 'premium';
    } else
    /* istanbul ignore next */
    {
      cov_103e59fyd1().b[31][1]++;
    }
    cov_103e59fyd1().s[116]++;
    if (price >= 500) {
      /* istanbul ignore next */
      cov_103e59fyd1().b[32][0]++;
      cov_103e59fyd1().s[117]++;
      return 'seasonal';
    } else
    /* istanbul ignore next */
    {
      cov_103e59fyd1().b[32][1]++;
    } // Special pricing for seasonal
    cov_103e59fyd1().s[118]++;
    return 'basic';
  }
  getGiftEffects(rarity) {
    /* istanbul ignore next */
    cov_103e59fyd1().f[20]++;
    const baseEffects =
    /* istanbul ignore next */
    (cov_103e59fyd1().s[119]++, {
      duration: 2000
    });
    /* istanbul ignore next */
    cov_103e59fyd1().s[120]++;
    switch (rarity) {
      case 'legendary':
        /* istanbul ignore next */
        cov_103e59fyd1().b[33][0]++;
        cov_103e59fyd1().s[121]++;
        return {
          ...baseEffects,
          duration: 8000,
          vibration: true,
          screenEffect: 'full_screen'
        };
      case 'epic':
        /* istanbul ignore next */
        cov_103e59fyd1().b[33][1]++;
        cov_103e59fyd1().s[122]++;
        return {
          ...baseEffects,
          duration: 6000,
          vibration: true,
          screenEffect: 'particle_burst'
        };
      case 'rare':
        /* istanbul ignore next */
        cov_103e59fyd1().b[33][2]++;
        cov_103e59fyd1().s[123]++;
        return {
          ...baseEffects,
          duration: 4000,
          screenEffect: 'sparkles'
        };
      case 'common':
        /* istanbul ignore next */
        cov_103e59fyd1().b[33][3]++;
      default:
        /* istanbul ignore next */
        cov_103e59fyd1().b[33][4]++;
        cov_103e59fyd1().s[124]++;
        return baseEffects;
    }
  }
  async updateGiftingStats(senderId, recipientId, gift, quantity, totalCost, hostEarnings) {
    /* istanbul ignore next */
    cov_103e59fyd1().f[21]++;
    // Update sender stats
