  async getUserRank(userId) {
    /* istanbul ignore next */
    cov_drx99t1iw().f[15]++;
    const user =
    /* istanbul ignore next */
    (cov_drx99t1iw().s[108]++, await User_1.User.findById(userId));
    /* istanbul ignore next */
    cov_drx99t1iw().s[109]++;
    if (!user) {
      /* istanbul ignore next */
      cov_drx99t1iw().b[42][0]++;
      cov_drx99t1iw().s[110]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_drx99t1iw().b[42][1]++;
    }
    const combinedScore =
    /* istanbul ignore next */
    (cov_drx99t1iw().s[111]++, this.calculateCombinedScore(
    /* istanbul ignore next */
    (cov_drx99t1iw().b[43][0]++, user.trust?.score) ||
    /* istanbul ignore next */
    (cov_drx99t1iw().b[43][1]++, 0),
    /* istanbul ignore next */
    (cov_drx99t1iw().b[44][0]++, user.karma?.total) ||
    /* istanbul ignore next */
    (cov_drx99t1iw().b[44][1]++, 0)));
    const usersAbove =
    /* istanbul ignore next */
    (cov_drx99t1iw().s[112]++, await User_1.User.countDocuments({
      $expr: {
        $gt: [{
          $add: [{
            $multiply: [{
              $ifNull: ['$trust.score', 0]
            }, 0.3]
          }, {
            $multiply: [{
              $ifNull: ['$karma.total', 0]
            }, 0.7]
          }]
        }, combinedScore]
      }
    }));
    /* istanbul ignore next */
    cov_drx99t1iw().s[113]++;
    return usersAbove + 1;
  }
  async updateUserCombinedScore(userId) {
    /* istanbul ignore next */
    cov_drx99t1iw().f[16]++;
    const unifiedScore =
    /* istanbul ignore next */
    (cov_drx99t1iw().s[114]++, await this.getUserUnifiedScore(userId));
    /* istanbul ignore next */
    cov_drx99t1iw().s[115]++;
    if (
    /* istanbul ignore next */
    (cov_drx99t1iw().b[46][0]++, unifiedScore.success) &&
    /* istanbul ignore next */
    (cov_drx99t1iw().b[46][1]++, unifiedScore.data)) {
      /* istanbul ignore next */
      cov_drx99t1iw().b[45][0]++;
      cov_drx99t1iw().s[116]++;
      // Update user with latest karma level name
      await User_1.User.findByIdAndUpdate(userId, {
        'karma.levelName': unifiedScore.data.karma.levelName
      });
    } else
    /* istanbul ignore next */
    {
      cov_drx99t1iw().b[45][1]++;
    }
  }
