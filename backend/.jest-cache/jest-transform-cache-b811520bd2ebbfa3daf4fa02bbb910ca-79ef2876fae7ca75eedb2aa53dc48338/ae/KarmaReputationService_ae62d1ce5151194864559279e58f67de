  /**
   * Award festival bonus karma and reputation
   */
  async awardFestivalBonus(userId, festivalName, festivalNameNepali, bonusType) {
    /* istanbul ignore next */
    cov_drx99t1iw().f[6]++;
    cov_drx99t1iw().s[61]++;
    try {
      const bonusMultipliers =
      /* istanbul ignore next */
      (cov_drx99t1iw().s[62]++, {
        participation: {
          karma: 10,
          reputation: 5
        },
        celebration: {
          karma: 20,
          reputation: 10
        },
        cultural_contribution: {
          karma: 30,
          reputation: 15
        }
      });
      const bonus =
      /* istanbul ignore next */
      (cov_drx99t1iw().s[63]++, bonusMultipliers[bonusType]);
      /* istanbul ignore next */
      cov_drx99t1iw().s[64]++;
      if (!bonus) {
        /* istanbul ignore next */
        cov_drx99t1iw().b[26][0]++;
        cov_drx99t1iw().s[65]++;
        return {
          success: false,
          error: 'Invalid bonus type'
        };
      } else
      /* istanbul ignore next */
      {
        cov_drx99t1iw().b[26][1]++;
      }
      // Award karma
      const karmaResult =
      /* istanbul ignore next */
      (cov_drx99t1iw().s[66]++, await this.communityLoveService.recordCommunityAction({
        userId,
        type: 'create_positive_content',
        description: `Festival ${bonusType}: ${festivalName}`,
        impact: bonus.karma / 5,
        // Convert to impact scale
        verificationRequired: false
      }));
      // Award reputation
      const reputationResult =
      /* istanbul ignore next */
      (cov_drx99t1iw().s[67]++, await this.reputationService.applyReputationDelta(userId, 'cultural_celebration', {
        festivalName,
        festivalNameNepali,
        bonusType,
        culturalSignificance: 'Festival celebration and cultural participation'
      }));
      // Create unified event
      /* istanbul ignore next */
      cov_drx99t1iw().s[68]++;
      await this.createUnifiedEvent(userId, 'cultural_celebration',
      /* istanbul ignore next */
      (cov_drx99t1iw().b[27][0]++, reputationResult.delta) ||
      /* istanbul ignore next */
      (cov_drx99t1iw().b[27][1]++, 0),
      /* istanbul ignore next */
      (cov_drx99t1iw().b[28][0]++, karmaResult.karmaAwarded) ||
      /* istanbul ignore next */
      (cov_drx99t1iw().b[28][1]++, 0), {
        festivalName,
        festivalNameNepali,
        bonusType,
        culturalSignificance: 'Festival celebration and cultural participation'
      }, {
        festivalName,
        festivalNameNepali,
        culturalSignificance: 'Festival celebration and cultural participation'
      });
      // Update combined score
      /* istanbul ignore next */
      cov_drx99t1iw().s[69]++;
      await this.updateUserCombinedScore(userId);
      const message =
      /* istanbul ignore next */
      (cov_drx99t1iw().s[70]++, `Festival bonus awarded: ${festivalNameNepali} (${festivalName}) - Karma: +${
      /* istanbul ignore next */
      (cov_drx99t1iw().b[29][0]++, karmaResult.karmaAwarded) ||
      /* istanbul ignore next */
      (cov_drx99t1iw().b[29][1]++, 0)}, Reputation: +${
      /* istanbul ignore next */
      (cov_drx99t1iw().b[30][0]++, reputationResult.delta) ||
      /* istanbul ignore next */
      (cov_drx99t1iw().b[30][1]++, 0)}`);
      /* istanbul ignore next */
      cov_drx99t1iw().s[71]++;
      return {
        success: true,
        karmaBonus:
        /* istanbul ignore next */
        (cov_drx99t1iw().b[31][0]++, karmaResult.karmaAwarded) ||
        /* istanbul ignore next */
        (cov_drx99t1iw().b[31][1]++, 0),
        reputationBonus:
        /* istanbul ignore next */
        (cov_drx99t1iw().b[32][0]++, reputationResult.delta) ||
        /* istanbul ignore next */
        (cov_drx99t1iw().b[32][1]++, 0),
        message
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_drx99t1iw().s[72]++;
      logger.error('Failed to award festival bonus:', error);
      /* istanbul ignore next */
      cov_drx99t1iw().s[73]++;
      return {
        success: false,
        error: error.message
      };
    }
  }
