      descriptionNepali: culturalContext?.festivalNameNepali ?
      /* istanbul ignore next */
      (cov_drx99t1iw().b[35][0]++, `${type.replace('_', ' ')} - ${culturalContext.festivalNameNepali}`) :
      /* istanbul ignore next */
      (cov_drx99t1iw().b[35][1]++, null),
      metadata: {
        ...metadata,
        ...culturalContext,
        unifiedEvent: true
      }
    }));
    /* istanbul ignore next */
    cov_drx99t1iw().s[93]++;
    await event.save();
  }
