      /* istanbul ignore next */
      (cov_1oy6657d7a().s[108]++, await this.getUserTier(userId));
      /* istanbul ignore next */
      cov_1oy6657d7a().s[109]++;
      if (!multiplier.conditions.userSegment.includes(userTier)) {
        /* istanbul ignore next */
        cov_1oy6657d7a().b[50][0]++;
        cov_1oy6657d7a().s[110]++;
        return false;
