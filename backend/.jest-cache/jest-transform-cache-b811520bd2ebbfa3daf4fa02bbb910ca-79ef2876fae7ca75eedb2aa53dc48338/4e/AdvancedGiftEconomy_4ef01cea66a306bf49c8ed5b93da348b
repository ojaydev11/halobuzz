  /**
   * Apply gift multipliers
   */
  async applyGiftMultipliers(userId, giftId, baseAmount) {
    /* istanbul ignore next */
    cov_1oy6657d7a().f[1]++;
    cov_1oy6657d7a().s[19]++;
    try {
      let totalMultiplier =
      /* istanbul ignore next */
      (cov_1oy6657d7a().s[20]++, 1);
      // Apply active multipliers
      /* istanbul ignore next */
      cov_1oy6657d7a().s[21]++;
      for (const multiplier of this.activeMultipliers) {
        /* istanbul ignore next */
        cov_1oy6657d7a().s[22]++;
        if (await this.isMultiplierApplicable(multiplier, userId, giftId)) {
          /* istanbul ignore next */
          cov_1oy6657d7a().b[0][0]++;
          cov_1oy6657d7a().s[23]++;
          totalMultiplier *= multiplier.multiplier;
        } else
        /* istanbul ignore next */
        {
          cov_1oy6657d7a().b[0][1]++;
        }
      }
      // Apply user tier multiplier
      const userTier =
      /* istanbul ignore next */
      (cov_1oy6657d7a().s[24]++, await this.getUserTier(userId));
      const tierMultiplier =
      /* istanbul ignore next */
      (cov_1oy6657d7a().s[25]++,
      /* istanbul ignore next */
      (cov_1oy6657d7a().b[1][0]++, this.giftTiers.find(t => {
        /* istanbul ignore next */
        cov_1oy6657d7a().f[2]++;
        cov_1oy6657d7a().s[26]++;
        return t.id === userTier;
      })?.multiplier) ||
      /* istanbul ignore next */
      (cov_1oy6657d7a().b[1][1]++, 1));
      /* istanbul ignore next */
      cov_1oy6657d7a().s[27]++;
      totalMultiplier *= tierMultiplier;
      // Apply streak multiplier
      const streakMultiplier =
      /* istanbul ignore next */
      (cov_1oy6657d7a().s[28]++, await this.getStreakMultiplier(userId));
      /* istanbul ignore next */
      cov_1oy6657d7a().s[29]++;
      totalMultiplier *= streakMultiplier;
      /* istanbul ignore next */
      cov_1oy6657d7a().s[30]++;
      return Math.round(baseAmount * totalMultiplier);
    } catch (error) {
      /* istanbul ignore next */
      cov_1oy6657d7a().s[31]++;
      this.logger.error('Error applying gift multipliers:', error);
      /* istanbul ignore next */
      cov_1oy6657d7a().s[32]++;
      return baseAmount;
    }
  }
