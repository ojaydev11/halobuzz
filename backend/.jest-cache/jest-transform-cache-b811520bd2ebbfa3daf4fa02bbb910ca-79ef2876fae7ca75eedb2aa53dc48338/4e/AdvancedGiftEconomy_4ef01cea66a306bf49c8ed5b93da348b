    /* istanbul ignore next */
    {
      cov_1oy6657d7a().b[46][1]++;
    }
    // Check user segment
    cov_1oy6657d7a().s[107]++;
    if (multiplier.conditions.userSegment) {
      /* istanbul ignore next */
      cov_1oy6657d7a().b[49][0]++;
