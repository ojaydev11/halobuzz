      /* istanbul ignore next */
      (cov_1oy6657d7a().b[29][0]++, dynamicGift.maxQuantity) ||
      /* istanbul ignore next */
      (cov_1oy6657d7a().b[29][1]++, 0)))) {
        /* istanbul ignore next */
        cov_1oy6657d7a().b[27][0]++;
        cov_1oy6657d7a().s[58]++;
