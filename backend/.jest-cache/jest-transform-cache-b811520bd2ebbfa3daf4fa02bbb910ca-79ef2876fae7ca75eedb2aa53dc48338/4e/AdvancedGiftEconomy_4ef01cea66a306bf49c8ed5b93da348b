    /* istanbul ignore next */
    {
      cov_1oy6657d7a().b[44][1]++;
    }
    // Check time range
    cov_1oy6657d7a().s[100]++;
    if (multiplier.conditions.timeRange) {
      /* istanbul ignore next */
      cov_1oy6657d7a().b[46][0]++;
