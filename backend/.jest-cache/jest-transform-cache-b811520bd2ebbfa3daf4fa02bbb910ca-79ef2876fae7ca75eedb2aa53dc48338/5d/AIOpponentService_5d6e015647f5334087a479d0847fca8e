    /* istanbul ignore next */
    (cov_2oa31zz9su().s[106]++, Math.min(0.95, accuracy * (1 - targetDifficulty * 0.3)));
    const shouldHit =
    /* istanbul ignore next */
    (cov_2oa31zz9su().s[107]++, Math.random() < hitProbability);
    /* istanbul ignore next */
    cov_2oa31zz9su().s[108]++;
