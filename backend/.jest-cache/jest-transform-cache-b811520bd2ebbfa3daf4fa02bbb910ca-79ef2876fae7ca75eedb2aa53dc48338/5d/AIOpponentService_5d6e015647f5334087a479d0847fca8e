    /* istanbul ignore next */
    (cov_2oa31zz9su().s[103]++, this.calculateReactionTime(personality, difficulty));
    const accuracy =
    /* istanbul ignore next */
    (cov_2oa31zz9su().s[104]++, difficulty / 100 * personality.traits.precision);
