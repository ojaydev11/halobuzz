    /* istanbul ignore next */
    (cov_1r36gp2gwu().s[70]++, Math.round(baseGrowth * 1.5)); // Base expectation
    const growthScenarios =
    /* istanbul ignore next */
    (cov_1r36gp2gwu().s[71]++, scenarios.map(s => {
      /* istanbul ignore next */
      cov_1r36gp2gwu().f[14]++;
