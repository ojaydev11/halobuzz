  async predictViralGrowth(timeframe) {
    /* istanbul ignore next */
    cov_1r36gp2gwu().f[13]++;
    const currentMetrics =
    /* istanbul ignore next */
    (cov_1r36gp2gwu().s[67]++, this.growthMetrics);
