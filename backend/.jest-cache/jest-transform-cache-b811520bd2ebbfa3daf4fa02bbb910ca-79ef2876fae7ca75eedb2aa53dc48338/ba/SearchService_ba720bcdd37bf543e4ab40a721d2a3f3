      (cov_1t2ob4bgsi().s[75]++, await Reel_1.Reel.find(conditions).populate('userId', 'username avatar').select('title description category viewCount createdAt').sort(sortCriteria).limit(limit).skip(offset));
      /* istanbul ignore next */
