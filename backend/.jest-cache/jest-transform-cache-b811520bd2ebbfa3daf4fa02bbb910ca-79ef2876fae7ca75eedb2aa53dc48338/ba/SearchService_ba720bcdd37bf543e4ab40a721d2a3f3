            (cov_1t2ob4bgsi().b[30][0]++, stream.userId?._id?.toString?.()) ||
            /* istanbul ignore next */
            (cov_1t2ob4bgsi().b[30][1]++, ''),
            username:
            /* istanbul ignore next */
            (cov_1t2ob4bgsi().b[31][0]++, stream.userId?.username) ||
