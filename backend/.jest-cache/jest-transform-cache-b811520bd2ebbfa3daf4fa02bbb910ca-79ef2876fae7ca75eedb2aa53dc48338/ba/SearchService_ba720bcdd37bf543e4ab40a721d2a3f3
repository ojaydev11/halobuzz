          $regex: query,
          $options: 'i'
        }
      }, {
        description: {
          $regex: query,
          $options: 'i'
        }
      }, {
        tags: {
          $in: [new RegExp(query, 'i')]
        }
      }];
    } else
    /* istanbul ignore next */
    {
      cov_1t2ob4bgsi().b[15][1]++;
    }
    // Category filter
    cov_1t2ob4bgsi().s[33]++;
    if (filters.category) {
      /* istanbul ignore next */
      cov_1t2ob4bgsi().b[16][0]++;
      cov_1t2ob4bgsi().s[34]++;
      conditions.category = filters.category;
    } else
    /* istanbul ignore next */
    {
      cov_1t2ob4bgsi().b[16][1]++;
    }
    // Live stream filter
    cov_1t2ob4bgsi().s[35]++;
    if (filters.isLive !== undefined) {
      /* istanbul ignore next */
      cov_1t2ob4bgsi().b[17][0]++;
      cov_1t2ob4bgsi().s[36]++;
      conditions.isLive = filters.isLive;
    } else
    /* istanbul ignore next */
    {
      cov_1t2ob4bgsi().b[17][1]++;
    }
    // Follower count filters
    cov_1t2ob4bgsi().s[37]++;
    if (
    /* istanbul ignore next */
    (cov_1t2ob4bgsi().b[19][0]++, filters.minFollowers) ||
    /* istanbul ignore next */
    (cov_1t2ob4bgsi().b[19][1]++, filters.maxFollowers)) {
      /* istanbul ignore next */
      cov_1t2ob4bgsi().b[18][0]++;
      cov_1t2ob4bgsi().s[38]++;
      conditions.followers = {};
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[39]++;
      if (filters.minFollowers) {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[20][0]++;
        cov_1t2ob4bgsi().s[40]++;
        conditions.followers.$gte = filters.minFollowers;
      } else
      /* istanbul ignore next */
      {
        cov_1t2ob4bgsi().b[20][1]++;
      }
      cov_1t2ob4bgsi().s[41]++;
      if (filters.maxFollowers) {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[21][0]++;
        cov_1t2ob4bgsi().s[42]++;
        conditions.followers.$lte = filters.maxFollowers;
      } else
      /* istanbul ignore next */
      {
        cov_1t2ob4bgsi().b[21][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1t2ob4bgsi().b[18][1]++;
    }
    // Date range filter
    cov_1t2ob4bgsi().s[43]++;
    if (
    /* istanbul ignore next */
    (cov_1t2ob4bgsi().b[23][0]++, filters.dateRange) &&
    /* istanbul ignore next */
    (cov_1t2ob4bgsi().b[23][1]++, filters.dateRange !== 'all')) {
      /* istanbul ignore next */
      cov_1t2ob4bgsi().b[22][0]++;
      const dateFilter =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[44]++, this.getDateFilter(filters.dateRange));
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[45]++;
      conditions.createdAt = dateFilter;
    } else
    /* istanbul ignore next */
    {
      cov_1t2ob4bgsi().b[22][1]++;
    }
    cov_1t2ob4bgsi().s[46]++;
    return conditions;
  }
  getDateFilter(dateRange) {
    /* istanbul ignore next */
    cov_1t2ob4bgsi().f[3]++;
    const now =
    /* istanbul ignore next */
    (cov_1t2ob4bgsi().s[47]++, new Date());
    let startDate;
    /* istanbul ignore next */
    cov_1t2ob4bgsi().s[48]++;
    switch (dateRange) {
      case 'today':
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[24][0]++;
        cov_1t2ob4bgsi().s[49]++;
        startDate = new Date(now.getFullYear(), now.getMonth(), now.getDate());
        /* istanbul ignore next */
        cov_1t2ob4bgsi().s[50]++;
        break;
      case 'week':
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[24][1]++;
        cov_1t2ob4bgsi().s[51]++;
        startDate = new Date(now.getTime() - 7 * 24 * 60 * 60 * 1000);
        /* istanbul ignore next */
        cov_1t2ob4bgsi().s[52]++;
        break;
      case 'month':
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[24][2]++;
        cov_1t2ob4bgsi().s[53]++;
        startDate = new Date(now.getFullYear(), now.getMonth(), 1);
        /* istanbul ignore next */
        cov_1t2ob4bgsi().s[54]++;
        break;
      case 'year':
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[24][3]++;
        cov_1t2ob4bgsi().s[55]++;
        startDate = new Date(now.getFullYear(), 0, 1);
        /* istanbul ignore next */
        cov_1t2ob4bgsi().s[56]++;
        break;
      default:
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[24][4]++;
        cov_1t2ob4bgsi().s[57]++;
        return {};
    }
    /* istanbul ignore next */
    cov_1t2ob4bgsi().s[58]++;
    return {
      $gte: startDate
    };
  }
  async searchUsers(query, conditions, sortBy, limit, offset) {
    /* istanbul ignore next */
    cov_1t2ob4bgsi().f[4]++;
    cov_1t2ob4bgsi().s[59]++;
    try {
      const sortCriteria =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[60]++, this.getSortCriteria(sortBy, 'users'));
      const users =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[61]++, await User_1.User.find(conditions).select('username displayName avatar followers isVerified ogLevel createdAt').sort(sortCriteria).limit(limit).skip(offset));
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[62]++;
      return users.map(user => {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().f[5]++;
        cov_1t2ob4bgsi().s[63]++;
        return {
          id: user._id.toString(),
          username: user.username,
          displayName:
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[25][0]++, user.displayName) ||
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[25][1]++, user.username),
          avatar:
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[26][0]++, user.avatar) ||
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[26][1]++, ''),
          followers:
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[27][0]++, user.followers) ||
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[27][1]++, 0),
          isVerified:
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[28][0]++, user.isVerified) ||
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[28][1]++, false),
          ogLevel:
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[29][0]++, user.ogLevel) ||
          /* istanbul ignore next */
          (cov_1t2ob4bgsi().b[29][1]++, 0)
        };
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[64]++;
      logger_1.logger.error('User search failed:', error);
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[65]++;
      return [];
    }
  }
  async searchStreams(query, conditions, sortBy, limit, offset) {
    /* istanbul ignore next */
    cov_1t2ob4bgsi().f[6]++;
    cov_1t2ob4bgsi().s[66]++;
    try {
      const sortCriteria =
      /* istanbul ignore next */
