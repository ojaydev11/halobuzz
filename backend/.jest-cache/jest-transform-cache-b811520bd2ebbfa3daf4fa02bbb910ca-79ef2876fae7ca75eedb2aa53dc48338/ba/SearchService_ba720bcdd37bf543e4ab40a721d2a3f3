  }
  async searchHashtags(query, limit, offset) {
    /* istanbul ignore next */
    cov_1t2ob4bgsi().f[10]++;
    cov_1t2ob4bgsi().s[80]++;
    try {
      // Extract hashtags from query
      const hashtags =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[81]++,
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().b[37][0]++, query.match(/#\w+/g)) ||
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().b[37][1]++, []));
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[82]++;
      if (hashtags.length === 0) {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[38][0]++;
        cov_1t2ob4bgsi().s[83]++;
        return [];
      } else
      /* istanbul ignore next */
      {
        cov_1t2ob4bgsi().b[38][1]++;
      }
      // Search for hashtags in streams and reels
      const streamHashtags =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[84]++, await LiveStream_1.LiveStream.aggregate([{
        $unwind: '$tags'
      }, {
        $match: {
          tags: {
            $regex: query.replace('#', ''),
            $options: 'i'
          }
        }
      }, {
        $group: {
          _id: '$tags',
          count: {
            $sum: 1
          }
        }
      }, {
        $sort: {
          count: -1
        }
      }, {
        $limit: limit
      }]));
      const reelHashtags =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[85]++, await Reel_1.Reel.aggregate([{
        $unwind: '$tags'
      }, {
        $match: {
          tags: {
            $regex: query.replace('#', ''),
            $options: 'i'
          }
        }
      }, {
        $group: {
          _id: '$tags',
          count: {
            $sum: 1
          }
        }
      }, {
        $sort: {
          count: -1
        }
      }, {
        $limit: limit
      }]));
      // Combine and deduplicate hashtags
      const allHashtags =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[86]++, new Map());
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[87]++;
      [...streamHashtags, ...reelHashtags].forEach(hashtag => {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().f[11]++;
        const tag =
        /* istanbul ignore next */
        (cov_1t2ob4bgsi().s[88]++, hashtag._id);
        /* istanbul ignore next */
        cov_1t2ob4bgsi().s[89]++;
        if (allHashtags.has(tag)) {
          /* istanbul ignore next */
          cov_1t2ob4bgsi().b[39][0]++;
          cov_1t2ob4bgsi().s[90]++;
          allHashtags.set(tag, allHashtags.get(tag) + hashtag.count);
        } else {
          /* istanbul ignore next */
          cov_1t2ob4bgsi().b[39][1]++;
          cov_1t2ob4bgsi().s[91]++;
          allHashtags.set(tag, hashtag.count);
        }
      });
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[92]++;
      return Array.from(allHashtags.entries()).map(([tag, count]) => {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().f[12]++;
        cov_1t2ob4bgsi().s[93]++;
        return {
          tag: `#${tag}`,
          count: count,
          trending: count > 100 // Simple trending logic
        };
      }).sort((a, b) => {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().f[13]++;
        cov_1t2ob4bgsi().s[94]++;
        return b.count - a.count;
      }).slice(offset, offset + limit);
    } catch (error) {
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[95]++;
      logger_1.logger.error('Hashtag search failed:', error);
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[96]++;
      return [];
    }
  }
  getSortCriteria(sortBy, type) {
    /* istanbul ignore next */
    cov_1t2ob4bgsi().f[14]++;
    cov_1t2ob4bgsi().s[97]++;
    switch (sortBy) {
      case 'popularity':
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[40][0]++;
        cov_1t2ob4bgsi().s[98]++;
        if (type === 'users') {
          /* istanbul ignore next */
          cov_1t2ob4bgsi().b[41][0]++;
          cov_1t2ob4bgsi().s[99]++;
          return {
            followers: -1
          };
        } else
        /* istanbul ignore next */
        {
          cov_1t2ob4bgsi().b[41][1]++;
        }
        cov_1t2ob4bgsi().s[100]++;
        if (type === 'streams') {
          /* istanbul ignore next */
          cov_1t2ob4bgsi().b[42][0]++;
          cov_1t2ob4bgsi().s[101]++;
          return {
            currentViewers: -1
          };
        } else
        /* istanbul ignore next */
        {
          cov_1t2ob4bgsi().b[42][1]++;
        }
        cov_1t2ob4bgsi().s[102]++;
        if (type === 'reels') {
          /* istanbul ignore next */
          cov_1t2ob4bgsi().b[43][0]++;
          cov_1t2ob4bgsi().s[103]++;
          return {
            viewCount: -1
          };
        } else
        /* istanbul ignore next */
        {
          cov_1t2ob4bgsi().b[43][1]++;
        }
        cov_1t2ob4bgsi().s[104]++;
        return {
          createdAt: -1
        };
      case 'date':
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[40][1]++;
        cov_1t2ob4bgsi().s[105]++;
        return {
          createdAt: -1
        };
      case 'followers':
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[40][2]++;
        cov_1t2ob4bgsi().s[106]++;
        return {
          followers: -1
        };
      case 'relevance':
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[40][3]++;
      default:
        /* istanbul ignore next */
        cov_1t2ob4bgsi().b[40][4]++;
        cov_1t2ob4bgsi().s[107]++;
        // For relevance, we'll use a combination of factors
        if (type === 'users') {
          /* istanbul ignore next */
          cov_1t2ob4bgsi().b[44][0]++;
          cov_1t2ob4bgsi().s[108]++;
          return {
            followers: -1,
            createdAt: -1
          };
        } else
        /* istanbul ignore next */
        {
          cov_1t2ob4bgsi().b[44][1]++;
        }
        cov_1t2ob4bgsi().s[109]++;
        if (type === 'streams') {
          /* istanbul ignore next */
          cov_1t2ob4bgsi().b[45][0]++;
          cov_1t2ob4bgsi().s[110]++;
          return {
            currentViewers: -1,
            createdAt: -1
          };
        } else
        /* istanbul ignore next */
        {
          cov_1t2ob4bgsi().b[45][1]++;
        }
        cov_1t2ob4bgsi().s[111]++;
        if (type === 'reels') {
          /* istanbul ignore next */
          cov_1t2ob4bgsi().b[46][0]++;
          cov_1t2ob4bgsi().s[112]++;
          return {
            viewCount: -1,
            createdAt: -1
          };
        } else
        /* istanbul ignore next */
        {
          cov_1t2ob4bgsi().b[46][1]++;
        }
        cov_1t2ob4bgsi().s[113]++;
        return {
          createdAt: -1
        };
    }
  }
  async getTrendingHashtags(limit =
  /* istanbul ignore next */
  (cov_1t2ob4bgsi().b[47][0]++, 10)) {
    /* istanbul ignore next */
    cov_1t2ob4bgsi().f[15]++;
    cov_1t2ob4bgsi().s[114]++;
    try {
      const trendingHashtags =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[115]++, await LiveStream_1.LiveStream.aggregate([{
        $unwind: '$tags'
      }, {
        $match: {
          createdAt: {
            $gte: new Date(Date.now() - 24 * 60 * 60 * 1000)
          }
        }
      },
      // Last 24 hours
      {
        $group: {
          _id: '$tags',
          count: {
            $sum: 1
          }
        }
      }, {
        $sort: {
          count: -1
        }
      }, {
        $limit: limit
      }]));
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[116]++;
      return trendingHashtags.map(hashtag => {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().f[16]++;
        cov_1t2ob4bgsi().s[117]++;
        return {
          tag: `#${hashtag._id}`,
          count: hashtag.count,
          trending: hashtag.count > 50
        };
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[118]++;
      logger_1.logger.error('Failed to get trending hashtags:', error);
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[119]++;
      return [];
    }
  }
  async getSearchSuggestions(query, limit =
  /* istanbul ignore next */
  (cov_1t2ob4bgsi().b[48][0]++, 5)) {
    /* istanbul ignore next */
    cov_1t2ob4bgsi().f[17]++;
    cov_1t2ob4bgsi().s[120]++;
    try {
      const suggestions =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[121]++, []);
      // Get username suggestions
      const usernameSuggestions =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[122]++, await User_1.User.find({
        username: {
          $regex: query,
          $options: 'i'
        }
      }).select('username').limit(limit).sort({
        followers: -1
      }));
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[123]++;
      suggestions.push(...usernameSuggestions.map(user => {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().f[18]++;
        cov_1t2ob4bgsi().s[124]++;
        return `@${user.username}`;
      }));
      // Get hashtag suggestions
      const hashtagSuggestions =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[125]++, await LiveStream_1.LiveStream.aggregate([{
        $unwind: '$tags'
      }, {
        $match: {
          tags: {
            $regex: query.replace('#', ''),
            $options: 'i'
          }
        }
      }, {
        $group: {
          _id: '$tags',
          count: {
            $sum: 1
          }
        }
      }, {
        $sort: {
          count: -1
        }
      }, {
        $limit: limit
      }]));
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[126]++;
      suggestions.push(...hashtagSuggestions.map(hashtag => {
        /* istanbul ignore next */
        cov_1t2ob4bgsi().f[19]++;
        cov_1t2ob4bgsi().s[127]++;
        return `#${hashtag._id}`;
      }));
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[128]++;
      return suggestions.slice(0, limit);
    } catch (error) {
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[129]++;
      logger_1.logger.error('Failed to get search suggestions:', error);
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[130]++;
      return [];
    }
  }
}
/* istanbul ignore next */
cov_1t2ob4bgsi().s[131]++;
exports.SearchService = SearchService;
/* istanbul ignore next */
cov_1t2ob4bgsi().s[132]++;
exports.searchService = SearchService.getInstance();