      const streams =
      /* istanbul ignore next */
      (cov_1t2ob4bgsi().s[68]++, await LiveStream_1.LiveStream.find(conditions).populate('userId', 'username avatar').select('title category currentViewers thumbnail isLive createdAt userId').sort(sortCriteria).limit(limit).skip(offset));
      /* istanbul ignore next */
      cov_1t2ob4bgsi().s[69]++;
