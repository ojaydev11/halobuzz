    /* istanbul ignore next */
    cov_i7cwypwt8().f[12]++;
    cov_i7cwypwt8().s[90]++;
    try {
      const severity =
      /* istanbul ignore next */
      (cov_i7cwypwt8().s[91]++, await this.assessIssueSeverity(issue));
      /* istanbul ignore next */
      cov_i7cwypwt8().s[92]++;
      if (severity.level === 1) {
        /* istanbul ignore next */
        cov_i7cwypwt8().b[15][0]++;
        cov_i7cwypwt8().s[93]++;
        // Minor issues - community self-resolution
        return {
          moderationType: 'community',
          escalationLevel: 1,
          suggestedAction: 'Community dialogue and mutual understanding',
          communityMediators: await this.findCommunityMediators(issue.reporterId, issue.reportedUserId)
        };
      } else {
        /* istanbul ignore next */
        cov_i7cwypwt8().b[15][1]++;
        cov_i7cwypwt8().s[94]++;
        if (severity.level === 2) {
          /* istanbul ignore next */
          cov_i7cwypwt8().b[16][0]++;
          cov_i7cwypwt8().s[95]++;
          // Moderate issues - elder council involvement
          return {
            moderationType: 'elder_council',
            escalationLevel: 2,
            suggestedAction: 'Elder council mediation with restorative approach',
            communityMediators: await this.findElderMediators()
          };
        } else {
          /* istanbul ignore next */
          cov_i7cwypwt8().b[16][1]++;
          cov_i7cwypwt8().s[96]++;
          // Severe issues - immediate automated action + elder review
          return {
            moderationType: 'automated',
            escalationLevel: 3,
            suggestedAction: 'Immediate protection with elder council review',
            communityMediators: await this.findElderMediators()
          };
        }
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_i7cwypwt8().s[97]++;
      logger_1.logger.error('Error in Panchayat moderation:', error);
      /* istanbul ignore next */
      cov_i7cwypwt8().s[98]++;
      return {
        moderationType: 'automated',
        escalationLevel: 3,
        suggestedAction: 'Standard automated moderation',
        communityMediators: []
      };
    }
  }
  // Helper methods
  static calculateAge(birthDate) {
    /* istanbul ignore next */
    cov_i7cwypwt8().f[13]++;
    cov_i7cwypwt8().s[99]++;
    if (!birthDate) {
      /* istanbul ignore next */
      cov_i7cwypwt8().b[17][0]++;
      cov_i7cwypwt8().s[100]++;
      return 25;
    } else
    /* istanbul ignore next */
    {
      cov_i7cwypwt8().b[17][1]++;
    } // Default age assumption
    const today =
    /* istanbul ignore next */
    (cov_i7cwypwt8().s[101]++, new Date());
    let age =
    /* istanbul ignore next */
    (cov_i7cwypwt8().s[102]++, today.getFullYear() - birthDate.getFullYear());
    const monthDiff =
    /* istanbul ignore next */
    (cov_i7cwypwt8().s[103]++, today.getMonth() - birthDate.getMonth());
    /* istanbul ignore next */
    cov_i7cwypwt8().s[104]++;
    if (
    /* istanbul ignore next */
    (cov_i7cwypwt8().b[19][0]++, monthDiff < 0) ||
    /* istanbul ignore next */
    (cov_i7cwypwt8().b[19][1]++, monthDiff === 0) &&
    /* istanbul ignore next */
    (cov_i7cwypwt8().b[19][2]++, today.getDate() < birthDate.getDate())) {
      /* istanbul ignore next */
      cov_i7cwypwt8().b[18][0]++;
      cov_i7cwypwt8().s[105]++;
      age--;
    } else
    /* istanbul ignore next */
    {
      cov_i7cwypwt8().b[18][1]++;
    }
    cov_i7cwypwt8().s[106]++;
    return age;
  }
  static async assessIssueSeverity(issue) {
    /* istanbul ignore next */
    cov_i7cwypwt8().f[14]++;
    // AI-powered severity assessment would go here
    // For now, simple rule-based assessment
    const severKeywords =
    /* istanbul ignore next */
    (cov_i7cwypwt8().s[107]++, ['threat', 'violence', 'harassment', 'abuse', 'hate']);
    const moderateKeywords =
    /* istanbul ignore next */
    (cov_i7cwypwt8().s[108]++, ['inappropriate', 'offensive', 'spam', 'misleading']);
    const description =
    /* istanbul ignore next */
    (cov_i7cwypwt8().s[109]++, issue.description.toLowerCase());
    /* istanbul ignore next */
    cov_i7cwypwt8().s[110]++;
    if (severKeywords.some(keyword => {
      /* istanbul ignore next */
      cov_i7cwypwt8().f[15]++;
      cov_i7cwypwt8().s[111]++;
      return description.includes(keyword);
    })) {
      /* istanbul ignore next */
      cov_i7cwypwt8().b[20][0]++;
      cov_i7cwypwt8().s[112]++;
      return {
        level: 3,
        reasoning: 'Contains severe violation keywords'
      };
    } else {
      /* istanbul ignore next */
      cov_i7cwypwt8().b[20][1]++;
      cov_i7cwypwt8().s[113]++;
      if (moderateKeywords.some(keyword => {
        /* istanbul ignore next */
        cov_i7cwypwt8().f[16]++;
        cov_i7cwypwt8().s[114]++;
        return description.includes(keyword);
      })) {
        /* istanbul ignore next */
        cov_i7cwypwt8().b[21][0]++;
        cov_i7cwypwt8().s[115]++;
        return {
          level: 2,
          reasoning: 'Contains moderate concern keywords'
        };
      } else {
        /* istanbul ignore next */
        cov_i7cwypwt8().b[21][1]++;
        cov_i7cwypwt8().s[116]++;
        return {
          level: 1,
          reasoning: 'Minor community disagreement'
        };
      }
    }
  }
  static async findCommunityMediators(userId1, userId2) {
    /* istanbul ignore next */
    cov_i7cwypwt8().f[17]++;
    cov_i7cwypwt8().s[117]++;
    try {
      // Find users with high trust scores and community involvement
      const mediators =
      /* istanbul ignore next */
      (cov_i7cwypwt8().s[118]++, await User_1.User.find({
        'trust.level': {
          $in: ['high', 'verified']
        },
        'trust.factors.totalStreams': {
          $gte: 50
        },
        isBanned: false,
        country: 'NP'
      }).sort({
        'trust.score': -1
      }).limit(3).select('_id username'));
      /* istanbul ignore next */
      cov_i7cwypwt8().s[119]++;
      return mediators.map(m => {
        /* istanbul ignore next */
        cov_i7cwypwt8().f[18]++;
        cov_i7cwypwt8().s[120]++;
        return m._id.toString();
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_i7cwypwt8().s[121]++;
      logger_1.logger.error('Error finding community mediators:', error);
      /* istanbul ignore next */
      cov_i7cwypwt8().s[122]++;
      return [];
    }
  }
  static async findElderMediators() {
    /* istanbul ignore next */
    cov_i7cwypwt8().f[19]++;
    cov_i7cwypwt8().s[123]++;
    try {
      // Find elder users (age 40+) with high trust and community respect
      const elders =
      /* istanbul ignore next */
      (cov_i7cwypwt8().s[124]++, await User_1.User.find({
        'trust.level': {
          $in: ['high', 'verified']
        },
        'trust.score': {
          $gte: 80
        },
        dateOfBirth: {
          $lte: new Date(new Date().getFullYear() - 40, 0, 1)
        },
        isBanned: false,
        country: 'NP'
      }).sort({
        'trust.score': -1,
        followers: -1
      }).limit(5).select('_id username'));
      /* istanbul ignore next */
      cov_i7cwypwt8().s[125]++;
      return elders.map(e => {
        /* istanbul ignore next */
        cov_i7cwypwt8().f[20]++;
        cov_i7cwypwt8().s[126]++;
        return e._id.toString();
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_i7cwypwt8().s[127]++;
      logger_1.logger.error('Error finding elder mediators:', error);
      /* istanbul ignore next */
      cov_i7cwypwt8().s[128]++;
      return [];
    }
  }
