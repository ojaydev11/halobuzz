      (cov_b9431avi7().b[24][1]++, invite.expiresAt < new Date())) {
        /* istanbul ignore next */
        cov_b9431avi7().b[23][0]++;
        cov_b9431avi7().s[125]++;
        return {
          success: false
        };
      } else
      /* istanbul ignore next */
      {
        cov_b9431avi7().b[23][1]++;
      }
      cov_b9431avi7().s[126]++;
      invite.status = 'accepted';
      /* istanbul ignore next */
      cov_b9431avi7().s[127]++;
      this.emit('gameInviteAccepted', {
        invite
      });
      /* istanbul ignore next */
