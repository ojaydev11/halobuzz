    if (
    /* istanbul ignore next */
    (cov_s4rf1hb1b().b[37][0]++, featureId === 'halo_throne') ||
    /* istanbul ignore next */
    (cov_s4rf1hb1b().b[37][1]++, featureId === 'stealth_mode')) {
      /* istanbul ignore next */
