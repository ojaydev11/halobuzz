  /**
   * Upgrade user's OG membership
   */
  async upgradeMembership(userId, targetTier, paymentType =
  /* istanbul ignore next */
  (cov_s4rf1hb1b().b[3][0]++, 'monthly')) {
    /* istanbul ignore next */
    cov_s4rf1hb1b().f[8]++;
    const tier =
    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[37]++, this.ogTiers.get(targetTier));
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[38]++;
    if (!tier) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().b[4][0]++;
      cov_s4rf1hb1b().s[39]++;
      throw new Error('Invalid OG tier');
    } else
    /* istanbul ignore next */
    {
      cov_s4rf1hb1b().b[4][1]++;
    }
    // Get user's current membership
    const currentMembership =
    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[40]++, await this.getUserMembership(userId));
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[41]++;
    if (currentMembership.currentTier >= targetTier) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().b[5][0]++;
      cov_s4rf1hb1b().s[42]++;
      throw new Error('Cannot downgrade or maintain same tier');
    } else
    /* istanbul ignore next */
    {
      cov_s4rf1hb1b().b[5][1]++;
    }
    // Check requirements
    cov_s4rf1hb1b().s[43]++;
    if (tier.requirements) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().b[6][0]++;
      const wallet =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[44]++, await CoinWallet_1.CoinWallet.findOne({
        userId
      }));
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[45]++;
      if (!wallet) {
        /* istanbul ignore next */
        cov_s4rf1hb1b().b[7][0]++;
        cov_s4rf1hb1b().s[46]++;
        throw new Error('User wallet not found');
      } else
      /* istanbul ignore next */
      {
        cov_s4rf1hb1b().b[7][1]++;
      }
      cov_s4rf1hb1b().s[47]++;
      if (tier.requirements.minSpending > 0) {
        /* istanbul ignore next */
        cov_s4rf1hb1b().b[8][0]++;
        // Check monthly spending requirement
        const monthlySpending =
        /* istanbul ignore next */
        (cov_s4rf1hb1b().s[48]++, await this.calculateMonthlySpending(userId));
        /* istanbul ignore next */
        cov_s4rf1hb1b().s[49]++;
        if (monthlySpending < tier.requirements.minSpending) {
          /* istanbul ignore next */
          cov_s4rf1hb1b().b[9][0]++;
          cov_s4rf1hb1b().s[50]++;
          throw new Error(`Minimum monthly spending of ${tier.requirements.minSpending} coins required`);
        } else
        /* istanbul ignore next */
        {
          cov_s4rf1hb1b().b[9][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_s4rf1hb1b().b[8][1]++;
      }
      cov_s4rf1hb1b().s[51]++;
      if (tier.requirements.minActivity > 0) {
        /* istanbul ignore next */
        cov_s4rf1hb1b().b[10][0]++;
        // Check activity requirement
        const monthlyActivity =
        /* istanbul ignore next */
        (cov_s4rf1hb1b().s[52]++, await this.calculateMonthlyActivity(userId));
        /* istanbul ignore next */
        cov_s4rf1hb1b().s[53]++;
        if (monthlyActivity < tier.requirements.minActivity) {
          /* istanbul ignore next */
          cov_s4rf1hb1b().b[11][0]++;
          cov_s4rf1hb1b().s[54]++;
          throw new Error(`Minimum ${tier.requirements.minActivity} active days per month required`);
        } else
        /* istanbul ignore next */
        {
          cov_s4rf1hb1b().b[11][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_s4rf1hb1b().b[10][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_s4rf1hb1b().b[6][1]++;
    }
    // Calculate cost
    const cost =
    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[55]++, paymentType === 'yearly' ?
    /* istanbul ignore next */
    (cov_s4rf1hb1b().b[12][0]++, tier.yearlyPrice) :
    /* istanbul ignore next */
    (cov_s4rf1hb1b().b[12][1]++, tier.monthlyPrice));
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[56]++;
    try {
      // Process payment
      const transaction =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[57]++, await CoinLedgerService_1.coinLedger.processTransaction({
        userId,
        type: 'og_purchase',
        amount: cost,
        source: 'og',
        destination: 'system_fee',
        context: {
          tier: targetTier,
          tierName: tier.name,
          paymentType,
          previousTier: currentMembership.currentTier
        }
      }));
      // Update user membership
      const nextBillingDate =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[58]++, new Date());
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[59]++;
      if (paymentType === 'yearly') {
        /* istanbul ignore next */
        cov_s4rf1hb1b().b[13][0]++;
        cov_s4rf1hb1b().s[60]++;
        nextBillingDate.setFullYear(nextBillingDate.getFullYear() + 1);
      } else {
        /* istanbul ignore next */
        cov_s4rf1hb1b().b[13][1]++;
        cov_s4rf1hb1b().s[61]++;
        nextBillingDate.setMonth(nextBillingDate.getMonth() + 1);
      }
      const updatedMembership =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[62]++, await this.updateUserMembership(userId, {
        currentTier: targetTier,
        nextBillingDate,
        isActive: true,
        totalSpentOnOG: currentMembership.totalSpentOnOG + cost,
        membershipHistory: [...currentMembership.membershipHistory, {
          tier: targetTier,
          startDate: new Date(),
          paymentAmount: cost,
          paymentType
        }]
      }));
      // Apply tier benefits immediately
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[63]++;
      await this.applyTierBenefits(userId, targetTier);
      const result =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[64]++, {
        success: true,
        newTier: targetTier,
        cost,
        benefits: this.getTierBenefitsList(tier),
        nextBillingDate,
        transactionId: transaction.txId
      });
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[65]++;
      this.emit('membershipUpgraded', {
        userId,
        previousTier: currentMembership.currentTier,
        newTier: targetTier,
        paymentType,
        cost
      });
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[66]++;
      logger_1.logger.info(`User ${userId} upgraded to OG ${tier.name} (${paymentType})`);
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[67]++;
      return result;
    } catch (error) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[68]++;
      logger_1.logger.error('Error upgrading membership:', error);
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[69]++;
      throw error;
    }
  }
  /**
   * Purchase premium feature
   */
  async purchasePremiumFeature(userId, featureId, duration) {
    /* istanbul ignore next */
    cov_s4rf1hb1b().f[9]++;
    const feature =
    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[70]++, this.premiumFeatures.get(featureId));
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[71]++;
    if (
    /* istanbul ignore next */
    (cov_s4rf1hb1b().b[15][0]++, !feature) ||
    /* istanbul ignore next */
    (cov_s4rf1hb1b().b[15][1]++, !feature.isActive)) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().b[14][0]++;
      cov_s4rf1hb1b().s[72]++;
      throw new Error('Premium feature not available');
    } else
    /* istanbul ignore next */
    {
      cov_s4rf1hb1b().b[14][1]++;
    }
    // Check OG level requirement
    const membership =
    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[73]++, await this.getUserMembership(userId));
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[74]++;
    if (membership.currentTier < feature.requiredOGLevel) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().b[16][0]++;
      cov_s4rf1hb1b().s[75]++;
      throw new Error(`OG Level ${feature.requiredOGLevel} required for this feature`);
    } else
    /* istanbul ignore next */
    {
      cov_s4rf1hb1b().b[16][1]++;
    }
    // Calculate cost and duration
    let cost =
    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[76]++, feature.pricing.coins);
    let expiresAt;
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[77]++;
    if (feature.pricing.duration !== 'permanent') {
      /* istanbul ignore next */
      cov_s4rf1hb1b().b[17][0]++;
      const durationMs =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[78]++, duration ?
      /* istanbul ignore next */
      (cov_s4rf1hb1b().b[18][0]++, duration * 1000) :
      /* istanbul ignore next */
      (cov_s4rf1hb1b().b[18][1]++, feature.pricing.durationValue));
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[79]++;
      expiresAt = new Date(Date.now() + durationMs);
      // Adjust cost for custom duration
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[80]++;
      if (
      /* istanbul ignore next */
      (cov_s4rf1hb1b().b[20][0]++, duration) &&
      /* istanbul ignore next */
      (cov_s4rf1hb1b().b[20][1]++, feature.pricing.duration === 'hourly')) {
        /* istanbul ignore next */
        cov_s4rf1hb1b().b[19][0]++;
        cov_s4rf1hb1b().s[81]++;
        cost = Math.ceil(feature.pricing.coins * (duration / 3600)); // duration in seconds
      } else
      /* istanbul ignore next */
      {
        cov_s4rf1hb1b().b[19][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_s4rf1hb1b().b[17][1]++;
    }
    // Apply OG discount
    const ogTier =
    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[82]++, this.ogTiers.get(membership.currentTier));
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[83]++;
    if (
    /* istanbul ignore next */
    (cov_s4rf1hb1b().b[22][0]++, ogTier) &&
    /* istanbul ignore next */
    (cov_s4rf1hb1b().b[22][1]++, featureId === 'halo_throne')) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().b[21][0]++;
      const discount =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[84]++, ogTier.perks.throneDiscountPercent / 100);
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[85]++;
      cost = Math.floor(cost * (1 - discount));
    } else
    /* istanbul ignore next */
    {
      cov_s4rf1hb1b().b[21][1]++;
    }
    cov_s4rf1hb1b().s[86]++;
    try {
      // Process payment
      const transaction =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[87]++, await CoinLedgerService_1.coinLedger.processTransaction({
        userId,
        type: 'premium_feature',
        amount: cost,
        source: 'premium',
        destination: 'system_fee',
        context: {
          feature: featureId,
          featureName: feature.name,
          duration,
          expiresAt
        }
      }));
      // Activate feature for user
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[88]++;
      await this.activatePremiumFeature(userId, featureId, expiresAt, cost);
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[89]++;
      this.emit('premiumFeaturePurchased', {
        userId,
        featureId,
        cost,
        expiresAt
      });
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[90]++;
      logger_1.logger.info(`User ${userId} purchased ${feature.name} for ${cost} coins`);
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[91]++;
      return {
        success: true,
        feature,
        cost,
        expiresAt,
        transactionId: transaction.txId
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[92]++;
      logger_1.logger.error('Error purchasing premium feature:', error);
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[93]++;
      throw error;
    }
  }
