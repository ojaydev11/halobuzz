    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[138]++, await this.getUserMembership(userId));
    // Remove existing activation of same feature
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[139]++;
    membership.activePremiumFeatures = membership.activePremiumFeatures.filter(f => {
      /* istanbul ignore next */
      cov_s4rf1hb1b().f[23]++;
      cov_s4rf1hb1b().s[140]++;
      return f.featureId !== featureId;
    });
