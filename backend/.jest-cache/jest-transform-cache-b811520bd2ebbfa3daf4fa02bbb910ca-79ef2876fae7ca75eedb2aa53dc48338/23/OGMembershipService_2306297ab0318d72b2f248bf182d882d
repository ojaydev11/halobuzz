      activityMetrics: {
        monthlySpending: 0,
        dailyLoginStreak: 0,
        lastActiveDate: new Date(),
        gamesPlayedThisMonth: 0,
        giftsGivenThisMonth: 0
      }
    });
    // Load membership history from transactions
    const ogTransactions =
    /* istanbul ignore next */
