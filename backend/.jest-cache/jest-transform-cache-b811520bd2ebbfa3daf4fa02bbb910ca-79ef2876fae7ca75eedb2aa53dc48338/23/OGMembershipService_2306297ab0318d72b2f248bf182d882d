          /* istanbul ignore next */
          cov_s4rf1hb1b().f[42]++;
          cov_s4rf1hb1b().s[191]++;
          return /* istanbul ignore next */(cov_s4rf1hb1b().b[47][0]++, !f.expiresAt) ||
          /* istanbul ignore next */
          (cov_s4rf1hb1b().b[47][1]++, f.expiresAt > now);
        });
        /* istanbul ignore next */
        cov_s4rf1hb1b().s[192]++;
        this.emit('premiumFeaturesExpired', {
          userId: membership.userId,
          expiredFeatures: expiredFeatures.map(f => {
            /* istanbul ignore next */
            cov_s4rf1hb1b().f[43]++;
            cov_s4rf1hb1b().s[193]++;
            return f.featureId;
          })
        });
        /* istanbul ignore next */
        cov_s4rf1hb1b().s[194]++;
        logger_1.logger.info(`Expired ${expiredFeatures.length} premium features for user ${membership.userId}`);
      } else
      /* istanbul ignore next */
      {
        cov_s4rf1hb1b().b[46][1]++;
      }
    }
  }
  async distributeDailyBonuses() {
    /* istanbul ignore next */
    cov_s4rf1hb1b().f[44]++;
    const activeMembers =
    /* istanbul ignore next */
    (cov_s4rf1hb1b().s[195]++, Array.from(this.userMemberships.values()).filter(m => {
      /* istanbul ignore next */
      cov_s4rf1hb1b().f[45]++;
      cov_s4rf1hb1b().s[196]++;
      return /* istanbul ignore next */(cov_s4rf1hb1b().b[48][0]++, m.isActive) &&
      /* istanbul ignore next */
      (cov_s4rf1hb1b().b[48][1]++, m.currentTier > 0);
    }));
    /* istanbul ignore next */
    cov_s4rf1hb1b().s[197]++;
    for (const member of activeMembers) {
      const bonusAmount =
      /* istanbul ignore next */
      (cov_s4rf1hb1b().s[198]++, await this.calculateDailyBonus(member.userId));
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[199]++;
      if (bonusAmount > 0) {
        /* istanbul ignore next */
        cov_s4rf1hb1b().b[49][0]++;
        cov_s4rf1hb1b().s[200]++;
        try {
          /* istanbul ignore next */
          cov_s4rf1hb1b().s[201]++;
          await CoinLedgerService_1.coinLedger.processTransaction({
            userId: member.userId,
            type: 'reward',
            amount: bonusAmount,
            source: 'system',
            destination: 'wallet',
            context: {
              type: 'og_daily_bonus',
              ogLevel: member.currentTier
            }
          });
          /* istanbul ignore next */
          cov_s4rf1hb1b().s[202]++;
          member.benefits.totalBonusCoinsEarned += bonusAmount;
          /* istanbul ignore next */
          cov_s4rf1hb1b().s[203]++;
          logger_1.logger.debug(`Distributed ${bonusAmount} daily bonus coins to OG${member.currentTier} user ${member.userId}`);
        } catch (error) {
          /* istanbul ignore next */
          cov_s4rf1hb1b().s[204]++;
          logger_1.logger.error(`Error distributing daily bonus to user ${member.userId}:`, error);
        }
      } else
      /* istanbul ignore next */
      {
        cov_s4rf1hb1b().b[49][1]++;
      }
    }
  }
  async updateMemberBenefits() {
    /* istanbul ignore next */
    cov_s4rf1hb1b().f[46]++;
    cov_s4rf1hb1b().s[205]++;
    // Update activity metrics and benefits for all members
    for (const membership of this.userMemberships.values()) {
      /* istanbul ignore next */
      cov_s4rf1hb1b().s[206]++;
      try {
        /* istanbul ignore next */
        cov_s4rf1hb1b().s[207]++;
        // Calculate monthly activity
        membership.activityMetrics.monthlySpending = await this.calculateMonthlySpending(membership.userId);
        // Update other metrics as needed
        /* istanbul ignore next */
        cov_s4rf1hb1b().s[208]++;
        this.userMemberships.set(membership.userId, membership);
      } catch (error) {
        /* istanbul ignore next */
        cov_s4rf1hb1b().s[209]++;
        logger_1.logger.error(`Error updating benefits for user ${membership.userId}:`, error);
      }
    }
  }
}
