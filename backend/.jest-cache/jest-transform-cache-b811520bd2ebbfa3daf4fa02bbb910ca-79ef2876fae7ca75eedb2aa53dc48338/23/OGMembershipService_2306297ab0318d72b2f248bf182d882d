      /* istanbul ignore next */
      (cov_s4rf1hb1b().b[29][0]++, wallet?.ogLevel) ||
      /* istanbul ignore next */
      (cov_s4rf1hb1b().b[29][1]++, 0),
      memberSince: new Date(),
      // Default to current date since createdAt is not available
      nextBillingDate: new Date(Date.now() + 30 * 24 * 60 * 60 * 1000),
      // 30 days from now
      isActive: (
      /* istanbul ignore next */
      (cov_s4rf1hb1b().b[30][0]++, wallet?.ogLevel) ||
