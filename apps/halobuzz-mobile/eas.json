{
  "cli": {
    "version": ">= 14.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "default"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_NO_TELEMETRY": "1"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform all"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "default"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_NO_TELEMETRY": "1",
        "EXPO_PUBLIC_API_BASE_URL": "https://halobuzz-api-proxy.ojayshah123.workers.dev/"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform all"
    },
    "production": {
      "ios": {
        "resourceClass": "default"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "prebuildCommand": "npx expo prebuild --clean --platform all"
    }
  },
  "submit": {
    "production": {}
  },
  "update": {
    "preview": {
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://halobuzz-api-proxy.ojayshah123.workers.dev/"
      }
    }
  }
}
